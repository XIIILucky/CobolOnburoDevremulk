        move rez-no to hesayr-rez-no
        start hesayr key > hesayr-anah
            invalid continue
            not invalid
              perform until fs-hesayr = "10"
                   read hesayr next no lock  
                      end move "10" to fs-hesayr
                      not end
                        if hesayr-rez-no not = rez-no 
                           move "10" to fs-hesayr
                           exit perform cycle
                        end-if
