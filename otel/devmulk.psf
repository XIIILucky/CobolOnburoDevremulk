              when "K"
                   modify form1-gd-1(renk-sayi + 1),
                          row-color = 176
              end-evaluate
              
        
        end-read 
        end-perform
        end-start
        close donhrk rez konuk
     end-if
     modify form1-gd-1,mass-update = 0.

