*
 hata-yaz-formyaz.
     display message box
       formyaz-kodu " daha once kullanilmis", new-line
       "Lutfen DZN dosyasinda farkli bir kod kullanin"
       title "!!! HATA !!!"
       icon  mb-error-icon
     .
*
 konumlan-ilk.
     move 1 to islem-sayfa
     perform aktar-sayfa-grid
     display acc-sayfa
     .
*
 konumlan-onceki.
     subtract 1 from islem-sayfa
     if islem-sayfa = zero
        move 1 to islem-sayfa
     end-if
     perform aktar-sayfa-grid
     display acc-sayfa
     .
*
 konumlan-sonraki.
     open i-o sayf
     initialize sayf-rec
     move islem-sayfa    to sayf-sayfa
     move all high-value to sayf-satir
     start sayf key > sayf-anah
       invalid
         continue
       not invalid
         read sayf next no lock
           end
             continue
           not end
             move sayf-sayfa to islem-sayfa
         end-read
     end-start
     close sayf
     perform aktar-sayfa-grid
     display acc-sayfa
     .
*
 konumlan-son.
     open i-o sayf
     initialize sayf-rec
     move all high-value to sayf-sayfa
                            sayf-satir
     start sayf key < sayf-anah
       invalid
         continue
       not invalid
         read sayf previous no lock
           end
             continue
           not end
             move sayf-sayfa to islem-sayfa
         end-read
     end-start
     close sayf
     perform aktar-sayfa-grid
     display acc-sayfa
     .
*
 konumlan-sayfa.
     perform aktar-sayfa-grid
     display acc-sayfa
     .
