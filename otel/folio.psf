     open i-o istek 
     initialize istek-rec 
     move konuk-folio  to istek-folio-no 
     move konuk-rez-no to istek-rez-no 
     if konuk-extra-rez-no > 0
        move konuk-extra-rez-no to istek-rez-no 
     end-if
     move sifre        to istek-sifre
     move "F"          to istek-turu
     read istek no lock invalid
          move 1           to istek-durumu
          move tarih-tasi  to istek-tar
          accept istek-zaman from time
          move k-kodu-tasi to istek-staff
          
          move konuk-folio to istek-aciklama
          move " Nolu folio uzerinde gecmise yonelik degisiklik icin onayinizi rica ederim." 
                           to istek-aciklama(9:)

          write istek-rec invalid
                move 1 to islem-hata
          end-write 

     not invalid
          move 1 to islem-hata
     end-read 
     close istek 
     .
*
 sifre-uret.
        accept zaman from time
        move function random(saniye) to random-num
        move random-num(7:)  to sifre
*     move function random() to random-num
*     move random-num(5:2)   to sifre(1:2)
*     move function random() to random-num
*     move random-num(5:2)   to sifre(3:2)
*     move function random() to random-num
*     move random-num(5:2)   to sifre(5:2)
///     display message box sifre 
     .
*
 Form6-Ex-Other.
     evaluate key-status 
         when 2 
*            if isyeri-adres-tasi(1:4) not = "kame"   
              if tmp-sifre <> sifre 
                 display message box "Hatali Sifre Girdiniz.Tekrar deneyiniz."
                                 title "Uyari"
                                 icon 2
                 exit paragraph
              end-if               
*            end-if 
              initialize  sifreli-cagirma
              move 1 to sifreli-cagirma
                      perform cek-gir
                      set exit-pushed to true
     end-evaluate.
*
 Form6-Aft-Initdata.
     initialize tmp-sifre
     display Form6-Ef-1
     .
*
 sifreleme-yap.
                  unstring sifreli-text delimited by " " into
                           kelimem(1)
                           kelimem(2)
                           kelimem(3)
                           kelimem(4)
                           kelimem(5)
                           kelimem(6)
                           kelimem(7)
                           kelimem(8)
                           kelimem(9)
                           kelimem(10)
                  
                  inspect kelime-occurs replacing all spaces by low-values
                  
                  perform varying r
                  from 1
                  by 1
                  until r > 10
                  
                     initialize word-length
                     perform varying word-length
                     from 1 
                     by 1
                     until word-length > 50
                        if kelimem(r)(word-length:1) = low-values 
                           exit perform  
                        end-if
                     end-perform 
                     compute word-length = word-length - 3
                  
                     if kelimem(r) <> low-values                        
                        move all "*" to kelimem(r)(3:word-length)
                     end-if
                  end-perform                                                                                                                   
                  
                  initialize sifreli-text
                  move low-values                 to sifreli-text
                  
                  perform varying r
                  from 1
                  by 1
                  until r > 10
                      string sifreli-text 
                             kelimem(r) delimited by low-values
                             " "       delimited by low-values
                             into sifreli-text
                  end-perform
                  inspect sifreli-text replacing all low-values by spaces.

@}}

