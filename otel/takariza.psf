 listbox-eventlar.
     evaluate event-type
          when cmd-dblclick
               inquire form1-lb-1,value in form1-lb-1-value
               move lb-1-col-1 to grup-alt
               perform accept-0101-Aft-Procedure
               move 4 to accept-control
               move 8 to control-id
               exit paragraph
          when ntf-selchange
          when cmd-goto
               inquire form1-lb-1,value in form1-lb-1-value
               move lb-1-col-1 to grup-alt
               display accept-0101,
               perform accept-0101-Aft-Procedure
     end-evaluate.
     
     .
 listbox-yer-bul.
     initialize durum tekrar
     perform varying i from 1 by 1 until (durum = "E" or "H")
             initialize egrup-alt
             modify form1-lb-1,
                    query-index = i
             inquire form1-lb-1,
                    item-value in egrup-alt
             if egrup-alt <> spaces
                if egrup-alt = tekrar
                   move "E" to durum
                   exit perform
                else
                   move egrup-alt to tekrar
                end-if
             end-if
             if egrup-alt = spaces
                move "H" to durum
                initialize i 
                exit perform
             else
                if egrup-alt = grup-alt
                   move "E" to durum
                   exit perform
                end-if
             end-if
    end-perform
    .

*
 label-tazele.
    evaluate combo-durum-tipi-value(1:1)
    when "A"
         modify label-0101,
                title "T/S ariza Kodu :"
         modify label-0103,
                title "Aciklama :"
         modify label-0104,
                title " "
         modify combo-oda-durumu,
                visible = false

    when "H"
         modify label-0101,
                title "Oda Durum Kodu :"
         modify label-0103,
                title "Oda Durum Adi :"
         modify label-0104,
                title "H/K Oda kodu :"
         modify combo-oda-durumu,
                visible = true
    when "M"
         modify label-0101,
                title "Maid Kodu :",
         modify label-0103,
                title "Maid Adi/Soyadi"
         modify label-0104,
                title " "
         modify combo-oda-durumu,
                visible = false
    end-evaluate.
