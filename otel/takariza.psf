 exception-procedure.
     evaluate key-status
         when 1
          initialize pazar-cagir
          move "G"   to pazar-tipi-cagir
          call "/asya/ytl/obj/otel/odatara.asy" using pazar-cagir
               on exception perform callerr-proc
               not on exception
          cancel "/asya/ytl/obj/otel/odatara.asy" 
          end-call
          move pazar-kodu-cagir     to grup-alt
               display accept-0101
               move 4 to accept-control
               move 5 to control-id
         when 2 
              open i-o grup
              move "R" to grup-tipi
              move combo-durum-tipi-value(1:1) to grup-turu
              move combo-oda-durumu-value(1:1) to grup-oda-durumu
              write grup-rec invalid 
                    rewrite grup-rec 
                    end-rewrite 
              end-write
                  perform log-operation-grup
              close grup
              perform listbox-yer-bul
              if i > zeroes
                   initialize form1-lb-1-value
                   modify form1-lb-1,
                          item-to-delete(i)
                   move grup-alt  to lb-1-col-1
                   move grup-turu to lb-1-col-2
                   move grup-adi  to lb-1-col-3
                   modify form1-lb-1,
                          item-to-add(form1-lb-1-value)
              else
                 initialize form1-lb-1-value
                 move grup-alt to lb-1-col-1
                 move grup-turu to lb-1-col-2
                 move grup-adi  to lb-1-col-3
                 modify form1-lb-1,
                        item-to-add(form1-lb-1-value)
              end-if
              move 4 to accept-control
              move 5 to control-id
         when 3
              open i-o grup
              move "R" to grup-tipi
              delete grup invalid continue 
              end-delete
                  perform log-operation-grup
              close grup
              perform listbox-yer-bul
              if i > zeroes
                 modify form1-lb-1,
                        item-to-delete(i)
              end-if
              move 4 to accept-control
              move 5 to control-id
        when 4
              open input grup 
              initialize grup-rec
              move "R" to grup-tipi
              start grup key not < grup-anah invalid
                    initialize mesaj-link
                    move 04 to mesaj-no
                    perform mesaj-ver
                    close grup kodlar02
                    exit paragraph
              end-start

              open i-o genelfis
              move 1 to genelfis-anahtar
              read genelfis no lock invalid
                   accept print-no from time
              not invalid
                   add 1 to print-no
                   rewrite genelfis-rec end-rewrite
              end-read
              close genelfis
