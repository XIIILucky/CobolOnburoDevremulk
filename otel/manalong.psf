                add ekle-tutar-dv to tmp-rg-yil(k) 
             else                                         
                add ekle-tutar-dv to tmp-hg-ay(k)
                add ekle-tutar-dv to tmp-hg-yil(k)    
             end-if  
          end-if     
          add ekle-tutar-dv to tmp-tg-ay(k)
       else
          if onkasar-ref = 1
             add ekle-tutar-dv to tmp-rg-yil(k)
          else
             add ekle-tutar-dv to tmp-hg-yil(k)
          end-if
       end-if
       add ekle-tutar-dv to tmp-tg-yil(k)
    end-if.

