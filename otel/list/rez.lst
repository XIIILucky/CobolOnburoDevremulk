              WHEN PAN-OGLE = "X" AND OGLE-DEP = ROMHRK-CORR-DEPKOD
              COMPUTE FIYATT-FIY ( JJ ) = FIYATT-FIY ( JJ ) -
                ROMHRK-TL-TUTAR
              WHEN PAN-AKSAM = "X" AND AKSAM-DEP = ROMHRK-CORR-DEPKOD
              COMPUTE FIYATT-FIY ( JJ ) = FIYATT-FIY ( JJ ) -
                ROMHRK-TL-TUTAR
              WHEN PAN-ICECEK = "X" AND ICECEK-DEP = ROMHRK-CORR-DEPKOD
