            END-IF
            MOVE FIYATT-FIYTL ( JJ ) TO TAKASFIYAT-FIYAT-TL
            WRITE TAKASFIYAT-REC
            END-WRITE NOT INVALID
            DISPLAY MESSAGE BOX "Hata no: fyhes-8220"
            STOP " "
            END-READ
            END-PERFORM
