       SELECT ISYERI ASSIGN TO RANDOM
              "/asya/ytl/data/otel/standart/isyeri" ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS ISYERI-NO ALTERNATE
              RECORD KEY IS ISYERI-ADRES DUPLICATES ALTERNATE RECORD KEY
              IS ISYERI-SIRA DUPLICATES STATUS FS-ISYERI.
       SELECT ONAY ASSIGN TO RANDOM ONAY-DOSYA ORGANIZATION IS INDEXED
              ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ONAY RECORD KEY
              IS ONAY-ID ALTERNATE RECORD KEY IS ONAYLANACAK =
              ONAY-DURUMU ONAY-IST-TZ WITH DUPLICATES ALTERNATE RECORD
              KEY IS ONAYLANACAKSTAF = ONAY-DURUMU ONAY-IST-STAF
              ONAY-IST-TZ WITH DUPLICATES.
       SELECT GENEL ASSIGN RANDOM GENEL-DOSYA ORGANIZATION INDEXED
              ACCESS RANDOM RECORD KEY IS GENEL-ANAHTAR STATUS FS-GENEL
              .
            SELECT GENEL2 ASSIGN RANDOM GENEL2-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD GENEL2-ANAH STATUS FS-GENEL2
              .
       SELECT GENELFIS ASSIGN TO RANDOM GENELFIS-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS RANDOM FILE STATUS IS FS-GENELFIS
              RECORD KEY IS GENELFIS-ANAHTAR.
            SELECT MUHASEBE-GENELFIS ASSIGN RANDOM MGENELFIS-DOSYA
              ORGANIZATION INDEXED ACCESS RANDOM RECORD KEY IS
              MUHASEBE-GENELFIS-ANAHTAR STATUS FS-MGENELFIS.
       SELECT KLLNC ASSIGN TO RANDOM 
            "/asya/ytl/data/otel/standart/kllnc"
             ORGANIZATION INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS
              K-KODU STATUS FS-KLLNC.
       SELECT BANKA ASSIGN TO RANDOM BANKA-DOSYA ORGANIZATION IS INDEXED
              ACCESS MODE IS DYNAMIC FILE STATUS IS FS-BANKA RECORD KEY
              IS BANKA-KODU ALTERNATE RECORD KEY IS BANKA-ADI WITH
              DUPLICATES.
       SELECT DOVIZ ASSIGN RANDOM DOVIZ-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY DOVIZ-KODU ALTERNATE RECORD KEY
              D-KISA-ADI DUPLICATES ALTERNATE RECORD KEY D-ADI-1
              DUPLICATES STATUS FS-DOVIZ.
            SELECT REZ ASSIGN RANDOM REZ-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS REZ-ANAH ALTERNATE
              RECORD REZ-AD = REZ-DURUMU REZ-ADI DUPLICATES ALTERNATE
              RECORD REZ-SOY = REZ-DURUMU REZ-SOYADI DUPLICATES
              ALTERNATE RECORD REZ-ACEN = REZ-DURUMU REZ-ACEN-ANAH
              DUPLICATES ALTERNATE RECORD REZ-GIR = REZ-DURUMU
              REZ-GIR-TAR DUPLICATES ALTERNATE RECORD REZ-CIK =
              REZ-DURUMU REZ-CIK-TAR DUPLICATES ALTERNATE RECORD
              REZ-VOUC = REZ-DURUMU REZ-VOUCHER DUPLICATES ALTERNATE
              RECORD REZ-ISL = REZ-DURUMU REZ-ISL-TAR DUPLICATES
              ALTERNATE RECORD REZ-SIL-ANAH = REZ-DURUMU REZ-SIL-TAR
              DUPLICATES ALTERNATE RECORD REZ-GRUP = REZ-DURUMU
              REZ-GRUP-ANAH REZ-SHARE DUPLICATES ALTERNATE RECORD
              REZ-ANAH-SHARE = REZ-DURUMU REZ-SHARENUM REZ-BUYUK
              REZ-FIYATI DUPLICATES STATUS FS-REZ.
            SELECT REZEK ASSIGN RANDOM REZEK-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS REZEK-ANAH STATUS
              FS-REZEK.
            SELECT REZDIGTAR ASSIGN RANDOM REZDIGTAR-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS
              REZDIGTAR-ANAH ALTERNATE RECORD REZDIGTAR-MAIL-TAR
              DUPLICATES ALTERNATE RECORD REZDIGTAR-ODEME-TAR DUPLICATES
              ALTERNATE RECORD REZDIGTAR-ODEMEEB-TAR DUPLICATES
              ALTERNATE RECORD REZDIGTAR-4-TAR DUPLICATES ALTERNATE
              RECORD REZDIGTAR-5-TAR DUPLICATES ALTERNATE RECORD
              REZDIGTAR-6-TAR DUPLICATES STATUS FS-REZDIGTAR.
            SELECT REZODABLOK ASSIGN RANDOM REZODABLOK-DOSYA
              ORGANIZATION INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS
              REZODABLOK-ANAH ALTERNATE RECORD REZODABLOK-ANAH2 =
              REZODABLOK-ODANO REZODABLOK-REZNO DUPLICATES STATUS
              FS-REZODABLOK.
            SELECT REZWEB ASSIGN RANDOM REZWEB-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS REZWEB-ANAH
              STATUS FS-REZWEB.
       SELECT ACENTA ASSIGN TO RANDOM ACENTA-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ACENTA
              RECORD KEY IS ACENTA-ANAHTAR ALTERNATE RECORD KEY IS
              ACENTA-ADI WITH DUPLICATES ALTERNATE RECORD KEY IS
              ACENTA-UNVANI WITH DUPLICATES.
       SELECT ACEBAVEL ASSIGN TO RANDOM ACEBAVEL-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ACEBAVEL
              RECORD KEY IS ACEBAVEL-ANAH.
       SELECT OTOMAIL ASSIGN TO RANDOM OTOMAIL-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC FILE STATUS IS FS-OTOMAIL
              RECORD KEY IS OTOMAIL-ANAHTAR ALTERNATE RECORD KEY IS
              OTOMAIL-ALT-ANAH2 WITH DUPLICATES ALTERNATE RECORD KEY IS
              OTOMAIL-ALT-ANAH3 WITH DUPLICATES.
       SELECT FIRMA ASSIGN TO RANDOM FIRMA-DOSYA ORGANIZATION IS INDEXED
              ACCESS MODE IS DYNAMIC FILE STATUS IS FS-FIRMA RECORD KEY
              IS FIRMA-ANAHTAR ALTERNATE RECORD KEY IS FIRMA-ADI WITH
              DUPLICATES ALTERNATE RECORD KEY IS FIRMA-UNVANI WITH
              DUPLICATES.
       SELECT TAKVIM ASSIGN RANDOM TAKVIM-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY TAKVIM-ANAH ALTERNATE RECORD KEY
              TAKVIM-KACINCI DUPLICATES STATUS FS-TAKVIM.
       SELECT BREAK ASSIGN RANDOM BREAK-DOSYA ORGANIZATION INDEXED
              ACCESS MODE DYNAMIC RECORD KEY BREAK-ANAH ALTERNATE RECORD
              KEY BREAK-ANAH1 = BREAK-ADI DUPLICATES ALTERNATE RECORD
              KEY BREAK-ANAH2 = BREAK-KODU DUPLICATES STATUS FS-BREAK.
            SELECT DEVREMULK ASSIGN TO RANDOM DEVREMULK-DOSYA
              ORGANIZATION INDEXED ACCESS MODE IS DYNAMIC RECORD KEY
              DEVREMULK-ANAH ALTERNATE RECORD KEY DEVREMULK-ANAH1 =
              DEVREMULK-ODA-NO DEVREMULK-DEVRE-DONEM WITH DUPLICATES
              ALTERNATE RECORD KEY DEVREMULK-ANAH2 =
              DEVREMULK-DEVRE-DONEM DEVREMULK-ODA-NO WITH DUPLICATES
              ALTERNATE RECORD KEY DEVREMULK-SOZLESME-NO WITH DUPLICATES
              ALTERNATE RECORD KEY DEVREMULK-SATAN-DANISMAN WITH
              DUPLICATES ALTERNATE RECORD KEY DEVREMULK-SATIS-TARIHI
              WITH DUPLICATES ALTERNATE RECORD KEY
              DEVREMULK-KAYIT-TARIHI WITH DUPLICATES ALTERNATE RECORD
              KEY DEVREMULK-HESAP-NO WITH DUPLICATES ALTERNATE RECORD
              KEY DEVREMULK-KESIN-KALAN WITH DUPLICATES ALTERNATE RECORD
              KEY DEVREMULK-KESIN-ODENEN WITH DUPLICATES ALTERNATE
              RECORD KEY DEVREMULK-KALAN-PRIM WITH DUPLICATES STATUS
              FS-DEVREMULK.
            SELECT KODLAR02 ASSIGN RANDOM KODLAR02-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY KODLAR02-ANAH ALTERNATE
              RECORD KEY KODLAR02-ANAH1 = KODLAR02-TIPI KODLAR02-ADI
              DUPLICATES STATUS FS-KODLAR02.
       SELECT ULKE ASSIGN TO RANDOM ULKE-DOSYA ORGANIZATION IS INDEXED
              ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ULKE RECORD KEY
              IS ULKE-ANAH ALTERNATE RECORD KEY IS ULKE-ADI WITH
              DUPLICATES.
       SELECT ULKEEK ASSIGN TO RANDOM ULKEEK-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ULKEEK
              RECORD KEY IS ULKEEK-ANAH WITH DUPLICATES.
            SELECT KONUM ASSIGN TO RANDOM KONUM-DOSYA ORGANIZATION
              INDEXED ACCESS MODE DYNAMIC RECORD KEY KONUM-ANAHTAR
              STATUS FS-KONUM.
            SELECT KONUMEK ASSIGN TO RANDOM KONUMEK-DOSYA ORGANIZATION
              INDEXED ACCESS MODE DYNAMIC RECORD KEY KONUMEK-ANAHTAR
              STATUS FS-KONUMEK.
            SELECT FIYAT ASSIGN TO RANDOM FIYAT-DOSYA ORGANIZATION
              INDEXED ACCESS MODE DYNAMIC RECORD KEY FIYAT-ANAH STATUS
              FS-FIYAT.
            SELECT FIYATANA ASSIGN RANDOM FIYATANA-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD FIYATANA-ANAH ALTERNATE
              RECORD FIYATANA-ILK-TARIH DUPLICATES ALTERNATE RECORD
              FIYATANA-SON-TARIH DUPLICATES STATUS FS-FIYATANA.
            SELECT FIYATIND ASSIGN RANDOM FIYATIND-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD FIYATIND-ANAH ALTERNATE
              RECORD FIYATIND-ILK-TARIH DUPLICATES ALTERNATE RECORD
              FIYATIND-SON-TARIH DUPLICATES STATUS FS-FIYATIND.
       SELECT WEBFIYAT ASSIGN RANDOM WEBFIYAT-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY WEBFIYAT-ANAH STATUS FS-WEBFIYAT
              .
       SELECT CAST ASSIGN RANDOM CAST-DOSYA ORGANIZATION INDEXED ACCESS
              MODE DYNAMIC RECORD KEY CAST-ANAH ALTERNATE RECORD KEY
              CAST-ANAH1 = CAST-REZ-NO CAST-TARIH DUPLICATES ALTERNATE
              RECORD KEY CAST-ANAH2 = CAST-TARIH CAST-ODA-NO
              CAST-SHARENUM CAST-SHARE DUPLICATES ALTERNATE RECORD KEY
              CAST-ANAH3 = CAST-ODA-NO CAST-TARIH DUPLICATES ALTERNATE
              RECORD KEY CAST-ANAH4 = CAST-TARIH CAST-SHARENUM
              DUPLICATES ALTERNATE RECORD KEY CAST-ANAH5 = CAST-SHARENUM
              CAST-ODA-NO CAST-TARIH DUPLICATES ALTERNATE RECORD KEY
              CAST-ANAH6 = CAST-SHARENUM CAST-TARIH CAST-ODA-NO
              CAST-BUYUK CAST-FIYATI DUPLICATES ALTERNATE RECORD KEY
              CAST-ANAH7 = CAST-GRUP CAST-SHARE DUPLICATES STATUS
              FS-CAST.
            SELECT AKSIYHRK ASSIGN TO RANDOM AKSIYHRK-DOSYA ORGANIZATION
              INDEXED ACCESS MODE DYNAMIC RECORD KEY AKSIYHRK-ANAH OF
              AKSIYHRK-REC STATUS FS-AKSIYHRK.
            SELECT ACENHRK ASSIGN TO RANDOM ACENHRK-DOSYA ORGANIZATION
              INDEXED ACCESS MODE DYNAMIC RECORD KEY ACENHRK-ANAH
              ALTERNATE RECORD KEY IS ACENHRK-ANAH1 = ACENHRK-TARIH
              ACENHRK-ACENTA DUPLICATES STATUS FS-ACENHRK.
            SELECT ODALAR ASSIGN RANDOM ODALAR-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD ODALAR-ANAH ALTERNATE RECORD
              ODALAR-ANAH1 = ODALAR-KONUMU ODALAR-NO DUPLICATES STATUS
              FS-ODALAR.
            SELECT ODALAR2 ASSIGN RANDOM ODALAR2-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD ODALAR2-ANAH ALTERNATE
              RECORD ODALAR2-ANAH1 = ODALAR2-NO DUPLICATES STATUS
              FS-ODALAR2.
       SELECT KONUK ASSIGN RANDOM KONUK-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS KONUK-ANAH ALTERNATE
              RECORD KONUK-AD = KONUK-DURUMU KONUK-ADI DUPLICATES
              ALTERNATE RECORD KONUK-SOY = KONUK-DURUMU KONUK-SOYADI
              DUPLICATES ALTERNATE RECORD KONUK-ODA = KONUK-DURUMU
              KONUK-ODANO DUPLICATES ALTERNATE RECORD KONUK-REZ =
              KONUK-DURUMU KONUK-REZ-NO DUPLICATES ALTERNATE RECORD
              KONUK-ACEN = KONUK-DURUMU KONUK-ACENTA DUPLICATES
              ALTERNATE RECORD KONUK-GRUP = KONUK-DURUMU KONUK-GRUP-NO
              KONUK-SHARENUM DUPLICATES ALTERNATE RECORD KONUK-GEL =
              KONUK-DURUMU KONUK-GEL-TAR DUPLICATES ALTERNATE RECORD
              KONUK-GIT = KONUK-DURUMU KONUK-GIT-TAR DUPLICATES
              ALTERNATE RECORD KONUK-KAPAT = KONUK-ACIK-HESAP
              KONUK-KAPAT-TARIH DUPLICATES STATUS FS-KONUK.
            SELECT BILBORD ASSIGN TO RANDOM BILBORD-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS BILBORD-ANAH
              ALTERNATE RECORD KEY BILBORD-ANAH2 = BILBORD-TARIH
              BILBORD-ACENTA BILBORD-ODA-TIP DUPLICATES STATUS
              FS-BILBORD.
       SELECT KARALIST ASSIGN TO RANDOM KARALIST-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS KARALIST-ANAH
              ALTERNATE RECORD KEY IS KARALIST-AD = KARALIST-TIPI
              KARALIST-ADI WITH DUPLICATES ALTERNATE RECORD KEY IS
              KARALIST-SOYAD = KARALIST-TIPI KARALIST-SOYADI WITH
              DUPLICATES FILE STATUS FS-KARALIST.
       SELECT YANREZ ASSIGN RANDOM YANREZ-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS YANREZ-ANAH ALTERNATE
              RECORD KEY IS YANREZ-ADI DUPLICATES ALTERNATE RECORD KEY
              IS YANREZ-SOYADI DUPLICATES ALTERNATE RECORD KEY IS
              YANREZ-GEL-TAR DUPLICATES ALTERNATE RECORD KEY IS
              YANREZ-GIT-TAR DUPLICATES STATUS FS-YANREZ.
            SELECT NT ASSIGN RANDOM NOT-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS NOT-ANAH STATUS
              FS-NOT.
       SELECT PROSIF ASSIGN RANDOM "/asya/ytl/data/otel/standart/prosif"
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY PROSIF-ANAH
              STATUS FS-PROSIF.
       SELECT KUR ASSIGN RANDOM KUR-DOSYA ORGANIZATION INDEXED ACCESS
              MODE IS DYNAMIC RECORD KEY KUR-KODU STATUS FS-KUR.
            SELECT ROMHRK ASSIGN RANDOM ROMHRK-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD ROMHRK-ANAH ALTERNATE RECORD
              ROMHRK-ANAH1 = ROMHRK-FOLIO ROMHRK-TARIH DUPLICATES
              ALTERNATE RECORD ROMHRK-ANAH2 = ROMHRK-TARIH ROMHRK-DEPKOD
              DUPLICATES STATUS FS-ROMHRK.
            SELECT EXTHRK ASSIGN RANDOM EXTHRK-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD EXTHRK-ANAH ALTERNATE RECORD
              EXTHRK-ANAH1 = EXTHRK-FOLIO EXTHRK-TARIH DUPLICATES
              ALTERNATE RECORD EXTHRK-ANAH2 = EXTHRK-TARIH EXTHRK-DEPKOD
              DUPLICATES STATUS FS-EXTHRK.
            SELECT HRK2 ASSIGN RANDOM HRK2-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD HRK2-ANAH ALTERNATE RECORD KEY
              HRK2-KAYNAK-FOLIO DUPLICATES STATUS FS-HRK2.
            SELECT YROMHRK ASSIGN RANDOM YROMHRK-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD YROMHRK-ANAH ALTERNATE
              RECORD YROMHRK-ANAH1 = YROMHRK-FOLIO YROMHRK-TARIH
              DUPLICATES ALTERNATE RECORD YROMHRK-ANAH2 = YROMHRK-TARIH
              YROMHRK-DEPKOD DUPLICATES STATUS FS-YROMHRK.
            SELECT FOLREF ASSIGN RANDOM FOLREF-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD FOLREF-ANAH STATUS FS-FOLREF
              .
            SELECT ROUTE ASSIGN RANDOM ROUTE-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY ROUTE-ANAH ALTERNATE RECORD KEY
              ROUTE-DEP = ROUTE-DEPKOD ROUTE-REZNO DUPLICATES ALTERNATE
              RECORD KEY ROUTE-DATA DUPLICATES STATUS FS-ROUTE.
            SELECT GRUP ASSIGN RANDOM GRUP-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY GRUP-ANAH ALTERNATE
              RECORD KEY GRUP-TIP-ADI = GRUP-TIPI GRUP-ADI WITH
              DUPLICATES ALTERNATE RECORD KEY GRUP-TIP-KOD = GRUP-TIPI
              GRUP-KODU WITH DUPLICATES STATUS FS-GRUP.
            SELECT REZGRUP ASSIGN RANDOM REZ-GRUP-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY REZGRUP-ANAHTAR
              ALTERNATE RECORD KEY REZGRUP-ADI DUPLICATES STATUS
              FS-REZ-GRUP.
       SELECT TAKASFIYAT ASSIGN TO RANDOM TAKASFIYAT-ADRES ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY TAKASFIYAT-ANAH
              ALTERNATE RECORD KEY TAKASFIYAT-TARANAH = TAKASFIYAT-TARIH
              TAKASFIYAT-REZ-NO DUPLICATES STATUS FS-TAKASFIYAT.
       SELECT CAST3 ASSIGN RANDOM CAST3-DOSYA ORGANIZATION INDEXED
              ACCESS MODE DYNAMIC RECORD KEY CAST3-ANAH ALTERNATE RECORD
              KEY CAST3-ANAH1 = CAST3-REZ-NO CAST3-TARIH DUPLICATES
              ALTERNATE RECORD KEY CAST3-ANAH2 = CAST3-TARIH
              CAST3-ODA-NO CAST3-SHARENUM CAST3-SHARE DUPLICATES
              ALTERNATE RECORD KEY CAST3-ANAH3 = CAST3-ODA-NO
              CAST3-TARIH DUPLICATES ALTERNATE RECORD KEY CAST3-ANAH4 =
              CAST3-TARIH CAST3-SHARENUM DUPLICATES ALTERNATE RECORD KEY
              CAST3-ANAH5 = CAST3-SHARENUM CAST3-ODA-NO CAST3-TARIH
              DUPLICATES ALTERNATE RECORD KEY CAST3-ANAH6 =
              CAST3-SHARENUM CAST3-TARIH CAST3-ODA-NO CAST3-BUYUK
              CAST3-FIYATI DUPLICATES ALTERNATE RECORD KEY CAST3-ANAH7 =
              CAST3-GRUP CAST3-SHARE DUPLICATES STATUS FS-CAST.
       SELECT ACEANLAS ASSIGN TO RANDOM ACEANLAS-DOSYA ORGANIZATION
              INDEXED ACCESS MODE DYNAMIC RECORD KEY ACEANLAS-ANAH
              ALTERNATE ACEANLAS-BUL = ACEANLAS-ACENTA
              ACEANLAS-ANLASMA-TAR DUPLICATES STATUS FS-ACEANLAS.
            SELECT GRTUMREZ ASSIGN RANDOM GRTUMREZ-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS GRTUMREZ-ANAH
              ALTERNATE RECORD GRTUMREZ-AD = GRTUMREZ-ADI
              GRTUMREZ-SOYADI DUPLICATES ALTERNATE RECORD GRTUMREZ-AD2 =
              GRTUMREZ-ADI GRTUMREZ-SOYADI GRTUMREZ-GIR-TAR DUPLICATES
              ALTERNATE RECORD GRTUMREZ-SOYAD = GRTUMREZ-SOYADI
              GRTUMREZ-ADI GRTUMREZ-DOG-TAR GRTUMREZ-GIR-TAR DUPLICATES
              STATUS FS-GRTUMREZ.
            SELECT PROHRK ASSIGN RANDOM PROHRK-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS PROHRK-ANAH
              ALTERNATE RECORD PROHRK-REZ-ANAH DUPLICATES ALTERNATE
              RECORD PROHRK-ILK = PROHRK-PRO-ADI PROHRK-PRO-SOYADI
              DUPLICATES ALTERNATE RECORD PROHRK-DUZ = PROHRK-DUZ-TARIH
              PROHRK-REZ-SIRKET DUPLICATES STATUS FS-PROHRK.
       SELECT HESAYR ASSIGN TO RANDOM HESAYR-DOSYA ORGANIZATION INDEXED
              ACCESS MODE DYNAMIC RECORD KEY HESAYR-ANAH STATUS
              FS-HESAYR.
            SELECT PROHRK2 ASSIGN RANDOM PROHRK-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY IS PROHRK2-ANAH
              ALTERNATE RECORD PROHRK2-REZ-ANAH DUPLICATES ALTERNATE
              RECORD PROHRK2-ILK = PROHRK2-PRO-ADI PROHRK2-PRO-SOYADI
              DUPLICATES ALTERNATE RECORD PROHRK2-DUZ =
              PROHRK2-DUZ-TARIH PROHRK2-REZ-SIRKET DUPLICATES STATUS
              FS-PROHRK2.
            SELECT GRUPLAR ASSIGN RANDOM GRUPLAR-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY GRUPLAR-ANAHTAR
              ALTERNATE RECORD KEY GRUPLAR-ADI DUPLICATES STATUS
              FS-GRUPLAR.
       SELECT MUSTERI ASSIGN RANDOM MUSTERI-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS MUSTERI-ANAH
              ALTERNATE RECORD MUSTERI-ANAH1 = MUSTERI-NO DUPLICATES
              ALTERNATE RECORD MUSTERI-ILK = MUSTERI-ADI MUSTERI-SOYADI
              DUPLICATES ALTERNATE RECORD MUSTERI-SOY = MUSTERI-SOYADI
              MUSTERI-ADI DUPLICATES ALTERNATE RECORD MUSTERI-SOY-AD =
              MUSTERI-SOYADI5 MUSTERI-ADI5 DUPLICATES ALTERNATE RECORD
              MUSTERI-AD-SOY = MUSTERI-ADI5 MUSTERI-SOYADI5 DUPLICATES
              ALTERNATE RECORD MUSTERI-ADI2 DUPLICATES ALTERNATE RECORD
              MUSTERI-ADI3 DUPLICATES ALTERNATE RECORD MUSTERI-SOYADI2
              DUPLICATES ALTERNATE RECORD MUSTERI-SOYADI3 DUPLICATES
              ALTERNATE RECORD MUSTERI-ANAH2 = MUSTERI-G-TARIH
              DUPLICATES ALTERNATE RECORD MUSTERI-D-TARIHI DUPLICATES
              ALTERNATE RECORD MUSTERI-E-TARIHI DUPLICATES ALTERNATE
              RECORD MUSTERI-O-TARIHI DUPLICATES ALTERNATE RECORD
              MUSTERI-ULKE DUPLICATES ALTERNATE RECORD MUSTERI-KIM-UYRUK
              DUPLICATES ALTERNATE RECORD MUSTERI-E-MAIL DUPLICATES
              ALTERNATE RECORD MUSTERI-TEL1 DUPLICATES ALTERNATE RECORD
              MUSTERI-GSM DUPLICATES ALTERNATE RECORD MUSTERI-KIS-ADRS
              DUPLICATES ALTERNATE RECORD MUSTERI-KIM-TCNO DUPLICATES
              ALTERNATE RECORD MUSTERI-SERI-NO DUPLICATES ALTERNATE
              RECORD MUSTERI-K-TARIH DUPLICATES ALTERNATE RECORD
              MUSTERI-PUAN-KART-NO DUPLICATES ALTERNATE RECORD
              MUSTERI-KAZANILAN-PUAN DUPLICATES ALTERNATE RECORD
              MUSTERI-HAK-EDILEN-TIP DUPLICATES ALTERNATE RECORD
              MUSTERI-GELIS-SAYISI DUPLICATES STATUS FS-MUSTERI.
       SELECT MISAFIR ASSIGN RANDOM MUSTERI-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS MISAFIR-ANAH STATUS
              FS-MUSTERI.
       SELECT HEDIYE ASSIGN RANDOM HEDIYE-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY HEDIYE-ANAH ALTERNATE RECORD KEY
              HEDIYE-ANAH1 = HEDIYE-ADI DUPLICATES STATUS FS-HEDIYE.
       SELECT HEDIYHRK ASSIGN RANDOM HEDIYHRK-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY HEDIYHRK-ANAH ALTERNATE RECORD
              KEY HEDIYHRK-PROFIL-ANAH DUPLICATES STATUS FS-HEDIYHRK.
       SELECT KARTTIP ASSIGN RANDOM KARTTIP-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY KARTTIP-ANAH ALTERNATE RECORD
              KEY KARTTIP-ANAH1 = KARTTIP-ADI DUPLICATES STATUS
              FS-KARTTIP.
       SELECT PFORMUL ASSIGN RANDOM 
            "/asya/ytl/data/otel/standart/pformul"
             ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              PFORMUL-ANAHTAR ALTERNATE RECORD KEY PFORMUL-ALT-ANAH =
              PFORMUL-TARIH DUPLICATES STATUS FS-PFORMUL.
            SELECT BLOKD ASSIGN RANDOM BLOKD-DOSYA ORGANIZATION INDEXED
