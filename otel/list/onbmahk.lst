       SELECT MGENEL ASSIGN RANDOM MGENEL-ACUSERVE-DOSYA ORGANIZATION
              INDEXED ACCESS RANDOM RECORD KEY IS MGENEL-ANAHTAR STATUS
              FS-GENEL COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT GENELFIS ASSIGN RANDOM GENELFIS-ACUSERVE-DOSYA
              ORGANIZATION INDEXED ACCESS RANDOM RECORD KEY IS
              GENELFIS-ANAHTAR STATUS FS-GENELFIS COLLATING SEQUENCE
              TURKCE-ALFABE.
       SELECT ISYERI ASSIGN TO RANDOM
              "/asya/ytl/data/muha/standart/isyeri" ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS ISYERI-NO ALTERNATE
              RECORD KEY IS ISYERI-ADRES DUPLICATES ALTERNATE RECORD KEY
              IS ISYERI-SIRA DUPLICATES STATUS FS-ISYERI COLLATING
              SEQUENCE TURKCE-ALFABE.
       SELECT HESAP ASSIGN TO RANDOM HESAP-ACUSERVE-DOSYA ORGANIZATION
              IS INDEXED COLLATING SEQUENCE IS TURKCE-ALFABE ACCESS MODE
              IS DYNAMIC FILE STATUS IS FS-HESAP RECORD KEY IS
              HESAP-KODU ALTERNATE RECORD KEY IS HESAP-NOKTASIZ WITH
              DUPLICATES ALTERNATE RECORD KEY IS HESAP-ADI WITH
              DUPLICATES.
       SELECT MAHSUP ASSIGN RANDOM MAHSUP-ACUSERVE-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY MAHSUP-ANAHTAR ALTERNATE
              RECORD KEY MAHSUP-ALT-ANAHTAR ALTERNATE RECORD KEY
              MAHSUP-VADE DUPLICATES ALTERNATE RECORD KEY MAHSUP-REF =
              MAHSUP-REFERANS MAHSUP-TARIH DUPLICATES ALTERNATE RECORD
              KEY MAHSUP-YEV = MAHSUP-TARIH MAHSUP-YEVMIYE-NO
              MAHSUP-FIS-SIRA DUPLICATES STATUS FS-MAHSUP COLLATING
              SEQUENCE TURKCE-ALFABE.
       SELECT CARI ASSIGN RANDOM CARI-ACUSERVE-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY CARI-KODU ALTERNATE
              RECORD KEY CARI-NOKTASIZ DUPLICATES ALTERNATE RECORD KEY
              C-UNVAN-1 DUPLICATES STATUS FS-CARI COLLATING SEQUENCE
              TURKCE-ALFABE.
            SELECT ISLENEN ASSIGN TO RANDOM ISLENEN-ACUSERVE-DOSYA
              ORGANIZATION INDEXED ACCESS MODE IS DYNAMIC RECORD KEY
              ISLENEN-ANAHTAR ALTERNATE RECORD KEY ISLENEN-TARIH
              DUPLICATES STATUS FS-ISLENEN COLLATING SEQUENCE
              TURKCE-ALFABE.
       SELECT REFERANS ASSIGN RANDOM REFERANS-ACUSERVE-DOSYA
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              REFERANS-ANAH ALTERNATE RECORD KEY REFERANS-ALT1 =
              REFERANS-TIPI REFERANS-ADI DUPLICATES STATUS FS-REFERANS
              COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT TAKVIM ASSIGN RANDOM TAKVIM-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY TAKVIM-ANAH ALTERNATE RECORD KEY
              TAKVIM-KACINCI DUPLICATES STATUS FS-TAKVIM.
       SELECT GENEL ASSIGN RANDOM GENEL-DOSYA ORGANIZATION INDEXED
              ACCESS RANDOM RECORD KEY IS GENEL-ANAHTAR STATUS FS-GENEL
              .
            SELECT DEPKOD ASSIGN RANDOM DEPKOD-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY DEPKOD-ANAH
              ALTERNATE RECORD KEY DEPKOD-ADI DUPLICATES STATUS
              FS-DEPKOD.
            SELECT DEPKOD2 ASSIGN RANDOM DEPKOD2-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY DEPKOD2-ANAH
              ALTERNATE RECORD KEY DEPKOD2-ADI DUPLICATES STATUS
              FS-DEPKOD2.
       SELECT ACENFAT ASSIGN TO RANDOM ACENFAT-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ACENFAT
              RECORD KEY IS ACENFAT-ANAH.
            SELECT ONKASA ASSIGN RANDOM ONKASA-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD ONKASA-ANAH STATUS FS-ONKASA
              .
       SELECT KUR ASSIGN RANDOM KUR-DOSYA ORGANIZATION INDEXED ACCESS
              MODE IS DYNAMIC RECORD KEY KUR-KODU STATUS FS-KUR.
       SELECT ACENTA ASSIGN TO RANDOM ACENTA-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ACENTA
              RECORD KEY IS ACENTA-ANAHTAR ALTERNATE RECORD KEY IS
              ACENTA-ADI WITH DUPLICATES ALTERNATE RECORD KEY IS
              ACENTA-UNVANI WITH DUPLICATES.
            SELECT ROMHRK ASSIGN RANDOM ROMHRK-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD ROMHRK-ANAH ALTERNATE RECORD
              ROMHRK-ANAH1 = ROMHRK-FOLIO ROMHRK-TARIH DUPLICATES
              ALTERNATE RECORD ROMHRK-ANAH2 = ROMHRK-TARIH ROMHRK-DEPKOD
              DUPLICATES STATUS FS-ROMHRK.
            SELECT EXTHRK ASSIGN RANDOM EXTHRK-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD EXTHRK-ANAH ALTERNATE RECORD
              EXTHRK-ANAH1 = EXTHRK-FOLIO EXTHRK-TARIH DUPLICATES
              ALTERNATE RECORD EXTHRK-ANAH2 = EXTHRK-TARIH EXTHRK-DEPKOD
              DUPLICATES STATUS FS-EXTHRK.
       SELECT KONUK ASSIGN RANDOM KONUK-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS KONUK-ANAH ALTERNATE
              RECORD KONUK-AD = KONUK-DURUMU KONUK-ADI DUPLICATES
              ALTERNATE RECORD KONUK-SOY = KONUK-DURUMU KONUK-SOYADI
              DUPLICATES ALTERNATE RECORD KONUK-ODA = KONUK-DURUMU
              KONUK-ODANO DUPLICATES ALTERNATE RECORD KONUK-REZ =
              KONUK-DURUMU KONUK-REZ-NO DUPLICATES ALTERNATE RECORD
              KONUK-ACEN = KONUK-DURUMU KONUK-ACENTA DUPLICATES
              ALTERNATE RECORD KONUK-GRUP = KONUK-DURUMU KONUK-GRUP-NO
              DUPLICATES ALTERNATE RECORD KONUK-GEL = KONUK-DURUMU
              KONUK-GEL-TAR DUPLICATES ALTERNATE RECORD KONUK-GIT =
              KONUK-DURUMU KONUK-GIT-TAR DUPLICATES STATUS FS-KONUK.
       SELECT FATDETAY ASSIGN TO RANDOM FATDETAY-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY IS FATDETAY-ANAH
              ALTERNATE RECORD KEY IS FATDETAY-ISL-ANAH =
              FATDETAY-ISL-TAR FATDETAY-ANAH DUPLICATES ALTERNATE RECORD
              KEY IS FATDETAY-KES-ANAH = FATDETAY-KES-TAR FATDETAY-ANAH
              DUPLICATES ALTERNATE RECORD KEY IS FATDETAY-DEP-ANAH =
              FATDETAY-DEPKOD FATDETAY-ANAH DUPLICATES STATUS
              FS-FATDETAY.
       SELECT ERRORLOG ASSIGN TO RANDOM
              "/asya/ytl/data/otel/standart/errorlog.log" ORGANIZATION
              LINE SEQUENTIAL STATUS FS-ERRORLOG.
       SELECT ARA-FARK ASSIGN TO RANDOM ARA-FARK-ADRES ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY ARA-FARK-ANAH STATUS
              FS-ARA-FARK COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT BORC-DEP ASSIGN TO RANDOM BORC-DEP-ADRES ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY BORC-DEP-ANAH ALTERNATE
              RECORD KEY BORC-DEP-ANAH2 = BORC-DEP-FATURA-NO
              BORC-DEP-ANAH WITH DUPLICATES STATUS FS-BORC-DEP COLLATING
              SEQUENCE TURKCE-ALFABE.
       SELECT ALAC-DEP ASSIGN TO RANDOM ALAC-DEP-ADRES ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY ALAC-DEP-ANAH ALTERNATE
              RECORD KEY ALAC-DEP-ANAH2 = ALAC-DEP-FATURA-NO
              ALAC-DEP-ANAH WITH DUPLICATES ALTERNATE RECORD KEY
              ANA-ACIK = ALAC-ANA-HESAP ALAC-DEP-ACIKLAMA WITH
              DUPLICATES STATUS FS-ALAC-DEP COLLATING SEQUENCE
              TURKCE-ALFABE.
       SELECT KASA-BORC-DEP ASSIGN TO RANDOM KASA-BORC-DEP-ADRES
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              KASA-BORC-DEP-ANAH ALTERNATE RECORD KEY
              KASA-BORC-DEP-ANAH2 = KASA-BORC-DEP-FATURA-NO
              KASA-BORC-DEP-ANAH WITH DUPLICATES STATUS FS-KASA-BORC-DEP
              COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT KASA-ALAC-DEP ASSIGN TO RANDOM KASA-ALAC-DEP-ADRES
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              KASA-ALAC-DEP-ANAH ALTERNATE RECORD KEY
              KASA-ALAC-DEP-ANAH2 = KASA-ALAC-DEP-FATURA-NO
              KASA-ALAC-DEP-ANAH WITH DUPLICATES STATUS FS-KASA-ALAC-DEP
              COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT ARA-ALAC-DEP ASSIGN TO RANDOM ARA-ALAC-DEP-ADRES
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              ARA-ALAC-DEP-ANAH STATUS FS-ARA-ALAC-DEP COLLATING
              SEQUENCE TURKCE-ALFABE.
       SELECT ARA-BORC-DEP ASSIGN TO RANDOM ARA-BORC-DEP-ADRES
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              ARA-BORC-DEP-ANAH STATUS FS-ARA-BORC-DEP COLLATING
              SEQUENCE TURKCE-ALFABE.
       SELECT TAKAS-ACENTA ASSIGN TO RANDOM TAKAS-ACENTA-ADRES
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              TAKAS-ACENTA-ANAH STATUS FS-TAKAS-ACENTA COLLATING
              SEQUENCE TURKCE-ALFABE.
            SELECT LOG-INDEX ASSIGN TO RANDOM LOG-INDEX-ADRES
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY IS
              LOG-INDEX-ANAH ALTERNATE RECORD KEY LOG-INDEX-ANAH1 =
              LOG-INDEX-SYS-TARIH LOG-INDEX-SIRKET LOG-INDEX-ISLEM-TURU
