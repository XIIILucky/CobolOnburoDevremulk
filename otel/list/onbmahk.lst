            MOVE 1 TO ACU-TEXT-PTR
            IF ACU-MSG-1 NOT = SPACE
              MOVE 0 TO ACU-SIZE
              INSPECT ACU-MSG-1 TALLYING ACU-SIZE FOR TRAILING SPACE
              STRING ACU-MSG-1 ( 1 : ACU-LENGTH - ACU-SIZE ) DELIMITED
                BY SIZE INTO ACU-MSG-TEXT POINTER ACU-TEXT-PTR
            END-IF
            IF ACU-MSG-2 NOT = SPACE
              MOVE 0 TO ACU-SIZE
              INSPECT ACU-MSG-2 TALLYING ACU-SIZE FOR TRAILING SPACE
              IF ACU-TEXT-PTR > 1
                STRING X"0A" DELIMITED BY SIZE INTO ACU-MSG-TEXT POINTER
                  ACU-TEXT-PTR
              END-IF
              STRING ACU-MSG-2 ( 1 : ACU-LENGTH - ACU-SIZE ) DELIMITED
                BY SIZE INTO ACU-MSG-TEXT POINTER ACU-TEXT-PTR
            END-IF
            IF ACU-MSG-3 NOT = SPACE
              MOVE 0 TO ACU-SIZE
              INSPECT ACU-MSG-3 TALLYING ACU-SIZE FOR TRAILING SPACE
              IF ACU-TEXT-PTR > 1
                STRING X"0A" DELIMITED BY SIZE INTO ACU-MSG-TEXT POINTER
                  ACU-TEXT-PTR
              END-IF
              STRING ACU-MSG-3 ( 1 : ACU-LENGTH - ACU-SIZE ) DELIMITED
                BY SIZE INTO ACU-MSG-TEXT POINTER ACU-TEXT-PTR
            END-IF
            IF ACU-TEXT-PTR = 1
              MOVE 0 TO ACU-SIZE
              INSPECT ACU-MSG-TEXT TALLYING ACU-SIZE FOR TRAILING SPACE
              COMPUTE ACU-TEXT-PTR = ACU-FULL-LEN - ACU-SIZE + 1
