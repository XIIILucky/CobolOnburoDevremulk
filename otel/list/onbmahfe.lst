       SELECT MGENEL ASSIGN RANDOM MGENEL-ACUSERVE-DOSYA ORGANIZATION
              INDEXED ACCESS RANDOM RECORD KEY IS MGENEL-ANAHTAR STATUS
              FS-GENEL COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT GENELFIS ASSIGN RANDOM GENELFIS-ACUSERVE-DOSYA
              ORGANIZATION INDEXED ACCESS RANDOM RECORD KEY IS
              GENELFIS-ANAHTAR STATUS FS-GENELFIS COLLATING SEQUENCE
              TURKCE-ALFABE.
       SELECT ISYERI ASSIGN TO RANDOM
              "/asya/ytl/data/muha/standart/isyeri" ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS ISYERI-NO ALTERNATE
              RECORD KEY IS ISYERI-ADRES DUPLICATES ALTERNATE RECORD KEY
              IS ISYERI-SIRA DUPLICATES STATUS FS-ISYERI COLLATING
              SEQUENCE TURKCE-ALFABE.
            SELECT HESAP ASSIGN TO RANDOM HESAP-ACUSERVE-DOSYA
              ORGANIZATION IS INDEXED ACCESS MODE IS DYNAMIC FILE STATUS
              IS FS-HESAP RECORD KEY IS HESAP-KODU ALTERNATE RECORD KEY
              IS HESAP-NOKTASIZ WITH DUPLICATES ALTERNATE RECORD KEY IS
              HESAP-ADI WITH DUPLICATES.
       SELECT MAHSUP ASSIGN RANDOM MAHSUP-ACUSERVE-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY MAHSUP-ANAHTAR ALTERNATE
              RECORD KEY MAHSUP-ALT-ANAHTAR ALTERNATE RECORD KEY
              MAHSUP-VADE DUPLICATES ALTERNATE RECORD KEY MAHSUP-REF =
              MAHSUP-REFERANS MAHSUP-TARIH DUPLICATES ALTERNATE RECORD
              KEY MAHSUP-YEV = MAHSUP-TARIH MAHSUP-YEVMIYE-NO
              MAHSUP-FIS-SIRA DUPLICATES ALTERNATE RECORD KEY
              MAHSUP-CARI-FATURA-KEY = MAHSUP-HESAP-KODU MAHSUP-BELGE-NO
              MAHSUP-TARIH MAHSUP-FIS-NO MAHSUP-FIS-SIRA DUPLICATES
              ALTERNATE RECORD KEY MAHSUP-FATURA-CARI-KEY =
              MAHSUP-BELGE-NO MAHSUP-HESAP-KODU MAHSUP-TARIH
              MAHSUP-FIS-NO MAHSUP-FIS-SIRA DUPLICATES STATUS FS-MAHSUP
              COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT CARI ASSIGN RANDOM CARI-ACUSERVE-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY CARI-KODU ALTERNATE
              RECORD KEY CARI-NOKTASIZ DUPLICATES ALTERNATE RECORD KEY
              C-UNVAN-1 DUPLICATES ALTERNATE RECORD KEY C-VERGI-NO
              DUPLICATES STATUS FS-CARI COLLATING SEQUENCE TURKCE-ALFABE
              .
            SELECT ISLENEN ASSIGN TO RANDOM ISLENEN-ACUSERVE-DOSYA
              ORGANIZATION INDEXED ACCESS MODE IS DYNAMIC RECORD KEY
              ISLENEN-ANAHTAR ALTERNATE RECORD KEY ISLENEN-TARIH
              DUPLICATES STATUS FS-ISLENEN COLLATING SEQUENCE
              TURKCE-ALFABE.
       SELECT REFERANS ASSIGN RANDOM REFERANS-ACUSERVE-DOSYA
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              REFERANS-ANAH ALTERNATE RECORD KEY REFERANS-ALT1 =
              REFERANS-TIPI REFERANS-ADI DUPLICATES STATUS FS-REFERANS
              COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT EFAT2ONB ASSIGN TO RANDOM EFAT2ONB-ACUSERVE-DOSYA
              ORGANIZATION IS INDEXED ACCESS MODE IS DYNAMIC RECORD KEY
              IS EFAT2ONB-ANAHTAR ALTERNATE RECORD KEY EFAT2ONB-MUH-REF
              = EFAT2ONB-REFERANS EFAT2ONB-MUHASEBE-FATURA-NO DUPLICATES
              ALTERNATE RECORD KEY EFAT2ONB-ONB-KEY = EFAT2ONB-TIPI
              EFAT2ONB-REFERANS EFAT2ONB-FATURA-TARIHI
              EFAT2ONB-ONBURO-FATURA-DURUMU DUPLICATES FILE STATUS
              FS-EFAT2ONB COLLATING SEQUENCE TURKCE-ALFABE.
       SELECT ALSAT ASSIGN RANDOM ALSAT-ACUSERVE-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY ALSAT-ANAHTAR
              ALTERNATE RECORD KEY TARIH-ANAHTAR = ALSAT-TIPI
              ALSAT-TARIH DUPLICATES ALTERNATE RECORD KEY HESAP-ANAHTAR
              = ALSAT-TIPI ALSAT-TARIH ALSAT-TOPLAM-HESAP-BABS
              DUPLICATES ALTERNATE RECORD KEY BELGE-NO-ANAHTAR =
              ALSAT-TIPI ALSAT-BELGE-NO ALSAT-TARIH DUPLICATES ALTERNATE
              RECORD KEY MAMI-ANAHTAR = ALSAT-TIPI ALSAT-KAYNAK
              ALSAT-KAYNAK-KUTUPHANE ALSAT-HAREKETE-YAZILDI ALSAT-TARIH
              DUPLICATES STATUS FS-ALSAT.
       SELECT FATKAPAT ASSIGN RANDOM FATKAPAT-ACUSERVE-DOSYA
              ORGANIZATION INDEXED ACCESS DYNAMIC RECORD KEY
              FATKAPAT-ANAH ALTERNATE RECORD KEY IS
              FATKAPAT-FATURA-ANAHTAR = FATKAPAT-TIPI FATKAPAT-FATURA
              DUPLICATES STATUS FS-FATKAPAT COLLATING SEQUENCE
              TURKCE-ALFABE.
       SELECT TAKVIM ASSIGN RANDOM TAKVIM-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY TAKVIM-ANAH ALTERNATE RECORD KEY
              TAKVIM-KACINCI DUPLICATES STATUS FS-TAKVIM.
            SELECT DEVREMULK ASSIGN TO RANDOM DEVREMULK-DOSYA
              ORGANIZATION INDEXED ACCESS MODE IS DYNAMIC RECORD KEY
              DEVREMULK-ANAH ALTERNATE RECORD KEY DEVREMULK-ANAH1 =
              DEVREMULK-ODA-NO DEVREMULK-DEVRE-DONEM WITH DUPLICATES
              ALTERNATE RECORD KEY DEVREMULK-ANAH2 =
              DEVREMULK-DEVRE-DONEM DEVREMULK-ODA-NO WITH DUPLICATES
              ALTERNATE RECORD KEY DEVREMULK-SOZLESME-NO WITH DUPLICATES
              ALTERNATE RECORD KEY DEVREMULK-SATAN-DANISMAN WITH
              DUPLICATES ALTERNATE RECORD KEY DEVREMULK-SATIS-TARIHI
              WITH DUPLICATES ALTERNATE RECORD KEY
              DEVREMULK-KAYIT-TARIHI WITH DUPLICATES ALTERNATE RECORD
              KEY DEVREMULK-HESAP-NO WITH DUPLICATES ALTERNATE RECORD
              KEY DEVREMULK-KESIN-KALAN WITH DUPLICATES ALTERNATE RECORD
              KEY DEVREMULK-KESIN-ODENEN WITH DUPLICATES ALTERNATE
              RECORD KEY DEVREMULK-KALAN-PRIM WITH DUPLICATES STATUS
              FS-DEVREMULK.
            SELECT ODEMELER ASSIGN TO RANDOM ODEMELER-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY ODEMELER-ANAH
              ALTERNATE RECORD KEY ODEMELER-TARIH WITH DUPLICATES
              ALTERNATE RECORD KEY ODEMELER-ODEME-TURU WITH DUPLICATES
              ALTERNATE RECORD KEY ODEMELER-CEKNO WITH DUPLICATES
              ALTERNATE RECORD KEY ODEMELER-CEK-ISL-NO WITH DUPLICATES
              ALTERNATE RECORD KEY ODEMELER-DAN-ANAH =
              ODEMELER-ODEME-TURU ODEMELER-DANISMAN-KODU WITH DUPLICATES
              ALTERNATE RECORD KEY ODEMELER-DAN-ANAH2 =
              ODEMELER-DANISMAN-KODU ODEMELER-ODEME-TURU WITH DUPLICATES
              ALTERNATE RECORD KEY ODEMELER-DEV-TAR =
              ODEMELER-DEVREMULK-NO ODEMELER-TARIH WITH DUPLICATES
              ALTERNATE RECORD KEY ODEMELER-PRIM-ISL-NO WITH DUPLICATES
              STATUS FS-ODEMELER.
            SELECT DEPKOD ASSIGN RANDOM DEPKOD-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY DEPKOD-ANAH
              ALTERNATE RECORD KEY DEPKOD-ADI DUPLICATES STATUS
              FS-DEPKOD.
            SELECT DEPKOD2 ASSIGN RANDOM DEPKOD2-DOSYA ORGANIZATION
              INDEXED ACCESS MODE IS DYNAMIC RECORD KEY DEPKOD2-ANAH
              ALTERNATE RECORD KEY DEPKOD2-ADI DUPLICATES STATUS
              FS-DEPKOD2.
       SELECT GENEL ASSIGN RANDOM GENEL-DOSYA ORGANIZATION INDEXED
              ACCESS RANDOM RECORD KEY IS GENEL-ANAHTAR STATUS FS-GENEL
              .
            SELECT FATURA ASSIGN RANDOM MATURA-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD KEY FATURA-ANAH ALTERNATE
              RECORD FATURA-ANAH1 = FATURA-TARIHI FATURA-ANAH DUPLICATES
              ALTERNATE RECORD FATURA-ANAH2 = FATURA-DURUMU
              FATURA-TARIHI FATURA-ANAH DUPLICATES STATUS FS-FATURA.
       SELECT ACENFAT ASSIGN TO RANDOM ACENFAT-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ACENFAT
              RECORD KEY IS ACENFAT-ANAH.
       SELECT ACENFAT2 ASSIGN TO RANDOM ACENFAT2-DOSYA ORGANIZATION IS
              INDEXED ACCESS MODE IS DYNAMIC FILE STATUS IS FS-ACENFAT2
              RECORD KEY IS ACENFAT2-ANAH ALTERNATE RECORD KEY
              ACENFAT2-ANAH2 = ACENFAT2-FOLIO ACENFAT2-PENCERE
              DUPLICATES.
            SELECT ONKASA ASSIGN RANDOM ONKASA-DOSYA ORGANIZATION
              INDEXED ACCESS DYNAMIC RECORD ONKASA-ANAH STATUS FS-ONKASA
              .
       SELECT MUSTERI ASSIGN RANDOM MUSTERI-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS MUSTERI-ANAH
              ALTERNATE RECORD MUSTERI-ANAH1 = MUSTERI-NO DUPLICATES
              ALTERNATE RECORD MUSTERI-ILK = MUSTERI-ADI MUSTERI-SOYADI
              DUPLICATES ALTERNATE RECORD MUSTERI-SOY = MUSTERI-SOYADI
              MUSTERI-ADI DUPLICATES ALTERNATE RECORD MUSTERI-SOY-AD =
              MUSTERI-SOYADI5 MUSTERI-ADI5 DUPLICATES ALTERNATE RECORD
              MUSTERI-AD-SOY = MUSTERI-ADI5 MUSTERI-SOYADI5 DUPLICATES
              ALTERNATE RECORD MUSTERI-ADI2 DUPLICATES ALTERNATE RECORD
              MUSTERI-ADI3 DUPLICATES ALTERNATE RECORD MUSTERI-SOYADI2
              DUPLICATES ALTERNATE RECORD MUSTERI-SOYADI3 DUPLICATES
              ALTERNATE RECORD MUSTERI-ANAH2 = MUSTERI-G-TARIH
              DUPLICATES ALTERNATE RECORD MUSTERI-D-TARIHI DUPLICATES
              ALTERNATE RECORD MUSTERI-E-TARIHI DUPLICATES ALTERNATE
              RECORD MUSTERI-O-TARIHI DUPLICATES ALTERNATE RECORD
              MUSTERI-ULKE DUPLICATES ALTERNATE RECORD MUSTERI-KIM-UYRUK
              DUPLICATES ALTERNATE RECORD MUSTERI-E-MAIL DUPLICATES
              ALTERNATE RECORD MUSTERI-TEL1 DUPLICATES ALTERNATE RECORD
              MUSTERI-GSM DUPLICATES ALTERNATE RECORD MUSTERI-KIS-ADRS
              DUPLICATES ALTERNATE RECORD MUSTERI-KIM-TCNO DUPLICATES
              ALTERNATE RECORD MUSTERI-SERI-NO DUPLICATES ALTERNATE
              RECORD MUSTERI-K-TARIH DUPLICATES ALTERNATE RECORD
              MUSTERI-PUAN-KART-NO DUPLICATES ALTERNATE RECORD
              MUSTERI-KAZANILAN-PUAN DUPLICATES ALTERNATE RECORD
              MUSTERI-HAK-EDILEN-TIP DUPLICATES ALTERNATE RECORD
              MUSTERI-GELIS-SAYISI DUPLICATES STATUS FS-MUSTERI.
       SELECT MISAFIR ASSIGN RANDOM MUSTERI-DOSYA ORGANIZATION INDEXED
              ACCESS MODE IS DYNAMIC RECORD KEY IS MISAFIR-ANAH STATUS
              FS-MUSTERI.
       SELECT HEDIYE ASSIGN RANDOM HEDIYE-DOSYA ORGANIZATION INDEXED
              ACCESS DYNAMIC RECORD KEY HEDIYE-ANAH ALTERNATE RECORD KEY
              HEDIYE-ANAH1 = HEDIYE-ADI DUPLICATES STATUS FS-HEDIYE.
       SELECT HEDIYHRK ASSIGN RANDOM HEDIYHRK-DOSYA ORGANIZATION INDEXED
