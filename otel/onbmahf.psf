                             else

                                 compute ramco rounded =  
                                 ramco + acenfat-ger-dv-tutar |( ACENFAT-tl-TUTAR  / konuk-kur-degeri )
                             
                           end-if
                          end-if 
                        end-if
                    end-read

                  end-perform
