*
        SELECT sprohrk ASSIGN RANDOM,sprohrk-DOSYA
           ORGANIZATION INDEXED
           ACCESS MODE IS dynamic
           RECORD KEY IS sprohrk-ANAH
           ALTERNATE RECORD sprohrk-rez-anah  DUPLICATES
           ALTERNATE RECORD sprohrk-ilk = sprohrk-pro-adi, sprohrk-pro-soyadi  DUPLICATES
           ALTERNATE RECORD sprohrk-duz = sprohrk-duz-tarih, sprohrk-rez-sirket  DUPLICATES 
           
           STATUS FS-sprohrk.
