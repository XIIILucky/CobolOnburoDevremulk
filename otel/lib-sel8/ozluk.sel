          SELECT OZLUK ASSIGN RANDOM, OZLUK-DOSYA
                 ORGANIZATION INDEXED
                 ACCESS MODE  IS DYNAMIC
                 RECORD KEY   IS OZLUK-ANAH                          
                 ALTERNATE RECORD OZLUK-YANI1-ADI
                 = OZLUK-DURUMU, OZLUK-YANI-ADI1  DUPLICATES
                 ALTERNATE RECORD OZLUK-YANI1-SOY
                 = OZLUK-DURUMU, OZLUK-YANI-SOY1 DUPLICATES
                 STATUS FS-OZLUK.
