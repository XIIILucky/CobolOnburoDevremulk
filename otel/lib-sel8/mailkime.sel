           SELECT MAILKIME ASSIGN RANDOM, MAILKIME-DOSYA
           ORGANIZATION INDEXED
           ACCESS MODE  IS DYNAMIC
           RECORD KEY  MAILKIME-ANAH
           ALTERNATE RECORD MAILKIME-KEY-1 =
                     MAILKIME-TIP, MAILKIME-TARIH      DUPLICATES,
           ALTERNATE RECORD MAILKIME-KEY-2 =
                     MAILKIME-TARIH, MAILKIME-TIP,MAILKIME-ULKESI    
                                                       DUPLICATES,
           STATUS FS-MAILKIME.
