           SELECT MAILTIPI ASSIGN RANDOM, MAILTIPI-DOSYA
           ORGANIZATION INDEXED
           ACCESS MODE  IS DYNAMIC
           RECORD KEY  MAILTIPI-ANAH
           ALTERNATE RECORD MAILTIPI-KEY-1 =
                     MAILTIPI-KIME         DUPLICATES,
           ALTERNATE RECORD MAILTIPI-KEY-2 =
                     MAILTIPI-GON-TARIH    DUPLICATES,
           STATUS FS-MAILTIPI.
