        SELECT prohrk ASSIGN RANDOM,prohrk-DOSYA
           ORGANIZATION INDEXED
           ACCESS MODE IS dynamic
           RECORD KEY IS prohrk-ANAH
           ALTERNATE RECORD prohrk-rez-anah  DUPLICATES
           ALTERNATE RECORD prohrk-ilk = prohrk-pro-adi, prohrk-pro-soyadi  DUPLICATES 
           ALTERNATE RECORD prohrk-duz = prohrk-duz-tarih, prohrk-rez-sirket  DUPLICATES 
           
           STATUS FS-prohrk.
