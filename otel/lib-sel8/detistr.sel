           SELECT detistr ASSIGN TO RANDOM, detistr-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE IS DYNAMIC
                  RECORD KEY detistr-ANAH
                  ALTERNATE RECORD KEY IS detistr-ANAH1 = 
                            detistr-ACENTA, detistr-TARIH,
                            detistr-ULKE-KODU       WITH DUPLICATES
                  STATUS FS-detistr.
