           SELECT ISTDATAr ASSIGN TO RANDOM, ISTDATAr-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE IS DYNAMIC
                  RECORD KEY ISTDATAr-ANAH
                  ALTERNATE RECORD KEY IS ISTDATAr-ANAH1 = 
                            ISTr-ACENTA, ISTr-TARIH, ISTr-REZ-TIPI
                            DUPLICATES
                  STATUS FS-ISTDATAr.
