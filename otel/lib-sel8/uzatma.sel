          SELECT UZATMA  ASSIGN TO RANDOM, UZATMA-DOSYA
                 ORGANIZATION INDEXED
                 ACCESS MODE IS DYNAMIC
                 RECORD UZATMA-ANAH
                 ALTERNATE RECORD KEY IS UZATMA-ANAH1 = UZATMA-FOLIO,
                                         UZATMA-TARIH DUPLICATES
                 STATUS FS-UZATMA.
