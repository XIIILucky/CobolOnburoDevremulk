SELECT ACENFAT
    ASSIGN       TO RANDOM ACENFAT-DOSYA
    ORGANIZATION IS INDEXED
    ACCESS MODE  IS DYNAMIC
    FILE STATUS  IS FS-ACENFAT
    RECORD KEY   IS ACENFAT-ANAH.


SELECT ACENFAT2
    ASSIGN       TO RANDOM ACENFAT2-DOSYA
    ORGANIZATION IS INDEXED
    ACCESS MODE  IS DYNAMIC
    FILE STATUS  IS FS-ACENFAT2
    RECORD KEY   IS ACENFAT2-ANAH
    alternate record key acenfat2-anah2 = acenfat2-folio acenfat2-pencere 
    duplicates.
