                         move rez-no to cast-rez-no
                         move takvim-anah to cast-tarih
                         if takvim-anah not >= rez-cik-tar then 
                            read cast no lock invalid continue
                              not invalid
                              compute maincurt-oda2  =   maincurt-oda2  + ( cast-fiyati )
                            end-read
                         end-if
                      end-if
                      end-read
                 end-perform
              end-start
             end-if
