                                           add cast-duzeltme to rez-top-tut 
                                       end-if                                   
                               else
                                  add cast-anlasma-fiyati to rez-top-tut
                               end-if 
                             end-if
                          end-if
                      end-read
                 end-perform 
            end-start.
             



