{{@PARAGRAPH:
*
 Form1-Bef-Create.
       perform adresleri-tasi.
     .
*
 before-procedure.
     initialize mesaj.
     evaluate control-id
          when 5
              evaluate true
                  when turkce
                       move "Onburo Calisma Tarihi Giriniz.[GUN].." to mesaj
                  when ingilizce
                       move "Onburo Calisma Tarihi Giriniz.[GUN].." to mesaj
                  when almanca                                   
                       move "Onburo Calisma Tarihi Giriniz.[GUN].." to mesaj
              end-evaluate
          when 6
              evaluate true
                  when turkce
                       move "Onburo Calisma Tarihi Giriniz.[AY ].." to mesaj
                  when ingilizce
                       move "Onburo Calisma Tarihi Giriniz.[AY ].." to mesaj
                  when almanca                                   
                       move "Onburo Calisma Tarihi Giriniz.[AY ].." to mesaj
              end-evaluate
          when 7
              evaluate true
                  when turkce
                       move "Onburo Calisma Tarihi Giriniz.[YIL].." to mesaj
                  when ingilizce
                       move "Onburo Calisma Tarihi Giriniz.[YIL].." to mesaj
                  when almanca                                   
                       move "Onburo Calisma Tarihi Giriniz.[YIL].." to mesaj
              end-evaluate
          when 11
              evaluate true
                  when turkce
                       move "Islemi Baslat...... " to mesaj
                  when ingilizce
                       move "Islemi Baslat...... " to mesaj
                  when almanca
                       move "Islemi Baslat...... " to mesaj
              end-evaluate
     end-evaluate.

     modify form1-st-1-handle,
            panel-index = 1 
            panel-text = mesaj.

     .
 Form1-Aft-Initdata.
    open input genel
    move 1 to genel-anahtar
    read genel no lock invalid
          initialize mesaj-link
          move 03 to mesaj-no
          perform mesaj-ver
    not invalid
        continue
    end-read
    close genel.
    move calisma-tarihi   to girilen-tarih ilk-tarih.
    display form1.
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         
      
*
 secim-kontrolu.
     initialize gec-gecme.
     open input takvim
     initialize takvim-rec.
     move girilen-tarih to takvim-anah.
     read takvim no lock invalid
          move 1 to gec-gecme
          initialize mesaj-link
          move 13 to mesaj-no
          perform mesaj-ver
     not invalid continue
     end-read
     close takvim.
 exception-procedure.
     evaluate key-status
         when 2 
              perform secim-kontrolu
              if gec-gecme = 0
                 perform islem-yap thru islem-yap-exit 
                 perform loga-yaz
                 display message box "Onburo Calisma Tarihi Degistirildi....."
                 move 27 to key-status 
              end-if
     end-evaluate.

 islem-yap.
    open i-o   genel
    move 1 to genel-anahtar
    read genel no lock invalid
          initialize mesaj-link
          move 03 to mesaj-no
          perform mesaj-ver
    not invalid
        continue
    end-read
    move girilen-tarih  to calisma-tarihi
    rewrite genel-rec invalid continue
    end-rewrite.
    perform log-operation-genel.
    close      genel.
    move calis-yil   to yil-tasi
    move calis-ay    to ay-tasi
    move calis-gun   to gun-tasi.
 islem-yap-exit.
       exit.
*
 loga-yaz.
********** PROGRAMA GIRIS LOGA GONDERILIYOR
               initialize call-alan1
                          call-alan2
                          call-alan3
                          call-alan4
                          call-alan5
                          call-alan6
               perform log-no-al
               move genelfis-log-no   to call-anah
               move "Tarih: "         to call-alan1
               move ilk-gun           to call-alan1(10:)
               move "/"               to call-alan1(12:)
               move ilk-ay            to call-alan1(13:)
               move "/"               to call-alan1(15:)
               move ilk-yil           to call-alan1(16:)
               move "Onburo Calisma Tarihi Manuel Degistirildi"    to call-alan2

               perform log-operation-calling               
     .
@}}

