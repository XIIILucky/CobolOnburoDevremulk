       compute oda-ort-satir = (takas-satir - 1 ) * 5 + 2 + 1
       compute pax-satir     = (takas-satir - 1 ) * 5 + 3 + 1
       compute pax-ort-satir = (takas-satir - 1 ) * 5 + 4 + 1
       compute tutar-satir   = (takas-satir - 1 ) * 5 + 5 + 1
