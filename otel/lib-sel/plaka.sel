SELECT plaka
    ASSIGN       TO RANDOM plaka-DOSYA
    ORGANIZATION IS INDEXED
    ACCESS MODE  IS DYNAMIC
    FILE STATUS  IS FS-plaka
    RECORD KEY   IS plaka-id
    ALTERNATE RECORD KEY IS  plaka-text  WITH DUPLICATES 
    ALTERNATE RECORD KEY IS  plaka-folio   WITH DUPLICATES 
    .                                     
