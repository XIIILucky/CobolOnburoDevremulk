           SELECT ISTDATA ASSIGN TO RANDOM, ISTDATA-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE IS DYNAMIC
                  RECORD KEY ISTDATA-ANAH
                  ALTERNATE RECORD KEY IS ISTDATA-ANAH1 = 
                            IST-ACENTA, IST-TARIH, IST-REZ-TIPI
                            DUPLICATES
                  STATUS FS-ISTDATA.
