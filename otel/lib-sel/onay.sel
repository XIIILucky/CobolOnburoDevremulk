SELECT onay
    ASSIGN       TO RANDOM onay-dosya
    ORGANIZATION IS INDEXED
    ACCESS MODE  IS DYNAMIC
    FILE STATUS  IS FS-onay
    RECORD KEY   IS onay-id
    ALTERNATE RECORD KEY IS  
    onaylanacak = ONAY-DURUMU ONAY-IST-tz  
      WITH DUPLICATES 
     ALTERNATE RECORD KEY IS  
    onaylanacakstaf = ONAY-DURUMU ONAY-IST-STAF ONAY-IST-tz  
      WITH DUPLICATES  
    
    .  
     
