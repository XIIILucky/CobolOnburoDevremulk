        SELECT ODENMEZ ASSIGN RANDOM, ODENMEZ-DOSYA
               ORGANIZATION IS INDEXED
               ACCESS MODE  IS DYNAMIC
               RECORD KEY   IS ODENMEZ-ANAH
               ALTERNATE RECORD KEY IS ODENMEZ-ANAH1 =
                         ODENMEZ-ILK, ODENMEZ-ADI
                         DUPLICATES
               FILE STATUS IS FS-ODENMEZ.
