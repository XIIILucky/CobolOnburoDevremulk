           SELECT ACENHRK ASSIGN TO RANDOM, ACENHRK-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE DYNAMIC
                  RECORD KEY ACENHRK-ANAH
                ALTERNATE RECORD KEY IS ACENHRK-ANAH1 = ACENHRK-TARIH ,
                                          ACENHRK-ACENTA DUPLICATES
                  STATUS FS-ACENHRK.
