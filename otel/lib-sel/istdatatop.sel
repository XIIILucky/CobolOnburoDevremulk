           SELECT ISTDATATOP ASSIGN TO RANDOM, ISTDATATOP-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE IS DYNAMIC
                  RECORD KEY ISTDATATOP-ANAH
                  ALTERNATE RECORD KEY IS ISTDATATOP-ANAH1 = 
                            ISTTOP-ACENTA, ISTTOP-TARIH, ISTTOP-REZ-TIPI
                            DUPLICATES
                  STATUS FS-ISTDATATOP.
