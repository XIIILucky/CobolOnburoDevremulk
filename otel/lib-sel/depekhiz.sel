           SELECT DEPEKHIZ ASSIGN TO RANDOM DEPEKHIZ-DOSYA,
                  ORGANIZATION INDEXED,
                  ACCESS DYNAMIC,
                  RECORD KEY IS DEPEKHIZ-ANAHTAR,
                  ALTERNATE RECORD KEY DEPEKHIZ-ALT-1 =
                             DEPEkhiz-TARIH DUPLICATES, 
                  alternate record key depekhiz-ucak-gel-tar duplicates,
                  alternate record key depekhiz-isl-tarih duplicates,
                 STATUS FS-DEPEKHIZ.
