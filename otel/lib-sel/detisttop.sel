           SELECT DETISTTOP ASSIGN TO RANDOM, DETISTTOP-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE IS DYNAMIC
                  RECORD KEY DETISTTOP-ANAH
                  ALTERNATE RECORD KEY IS DETISTTOP-ANAH1 = 
                            DETISTTOP-ACENTA, DETISTTOP-TARIH,
                            DETISTTOP-ULKE-KODU       WITH DUPLICATES
                  STATUS FS-DETISTTOP.
