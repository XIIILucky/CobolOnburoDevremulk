          SELECT telayar ASSIGN RANDOM, telayar-DOSYA
                 ORGANIZATION INDEXED
                 ACCESS MODE  IS DYNAMIC
                 RECORD KEY   IS telayar-anah                        
                 ALTERNATE RECORD telayar-saat           
                           DUPLICATES 
                 ALTERNATE RECORD KONUK-SOY           
                 = KONUK-DURUMU, KONUK-SOYADI          DUPLICATES 
                 ALTERNATE RECORD KONUK-ODA           
                 = KONUK-DURUMU, KONUK-ODANO           DUPLICATES 
                 ALTERNATE RECORD KONUK-REZ           
                 = KONUK-DURUMU, KONUK-REZ-NO          DUPLICATES 
                 ALTERNATE RECORD KONUK-ACEN          
                 = KONUK-DURUMU, KONUK-ACENTA          DUPLICATES 
                 ALTERNATE RECORD KONUK-GRUP          
                 = KONUK-DURUMU, KONUK-GRUP-NO         DUPLICATES 
                 ALTERNATE RECORD KONUK-GEL           
                 = KONUK-DURUMU, KONUK-GEL-TAR         DUPLICATES 
                 ALTERNATE RECORD KONUK-GIT           
                 = KONUK-DURUMU, KONUK-GIT-TAR         DUPLICATES 
                 STATUS FS-KONUK.
