        SELECT onodeme ASSIGN RANDOM, onodeme-dosya
               ORGANIZATION INDEXED
               ACCESS MODE DYNAMIC
               RECORD KEY onodeme-anah  
               alternate record key onodeme-anah1 = 
               onodeme-tarih , onodeme-rez-no duplicates
               alternate record key onodeme-anah2 = 
               onodeme-tarih , onodeme-dep-kod duplicates
               STATUS fs-onodeme.



      
