       SELECT odatemiz ASSIGN RANDOM, odatemiz-dosya
              ORGANIZATION INDEXED
              ACCESS DYNAMIC
              RECORD KEY odatemiz-anah
              ALTERNATE KEY odatemiz-anah1 = 
                        odatemiz-tip,
                        odatemiz-oda,
                        odatemiz-tarih,
                        odatemiz-zaman DUPLICATES
              ALTERNATE KEY odatemiz-anah2 = 
                        odatemiz-tip,
                        odatemiz-tem-tar,
                        odatemiz-tem-zaman,
                        odatemiz-oda DUPLICATES
              ALTERNATE KEY odatemiz-anah3 = 
                        odatemiz-tip,
                        odatemiz-oda,
                        odatemiz-tem-tar,
                        odatemiz-tem-zaman DUPLICATES
              ALTERNATE KEY odatemiz-anah4 = 
                        odatemiz-tip,
                        odatemiz-ger-tem-tar,
                        odatemiz-ger-tem-zaman,
                        odatemiz-oda DUPLICATES
              ALTERNATE KEY odatemiz-anah5 = 
                        odatemiz-tip,
                        odatemiz-oda,
                        odatemiz-ger-tem-tar,
                        odatemiz-ger-tem-zaman DUPLICATES
              STATUS fs-odatemiz.

