           SELECT REZ ASSIGN RANDOM, REZ-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE IS DYNAMIC
                  RECORD KEY  IS REZ-ANAH
                  ALTERNATE RECORD REZ-AD        =
                  REZ-DURUMU, REZ-ADI            DUPLICATES
                  ALTERNATE RECORD REZ-SOY       =
                  REZ-DURUMU, REZ-SOYADI         DUPLICATES
                  ALTERNATE RECORD REZ-ACEN      =
                  REZ-DURUMU, REZ-ACEN-ANAH      DUPLICATES
                  ALTERNATE RECORD REZ-GIR       =
                  REZ-DURUMU, REZ-GIR-TAR        DUPLICATES
                  ALTERNATE RECORD REZ-CIK       =
                  REZ-DURUMU, REZ-CIK-TAR        DUPLICATES
                  ALTERNATE RECORD REZ-VOUC      =
                  REZ-DURUMU, REZ-VOUCHER        DUPLICATES
                  ALTERNATE RECORD REZ-ISL       =
                  REZ-DURUMU, REZ-ISL-TAR        DUPLICATES
                  ALTERNATE RECORD REZ-grup       =
                  REZ-DURUMU, REZ-grup-anah, rez-share     DUPLICATES
                  ALTERNATE RECORD REZ-anah-share       =
                   REZ-DURUMU, rez-sharenum , rez-buyuk, rez-fiyati
                       
                     DUPLICATES 
                  STATUS FS-REZ.
              SELECT REZek ASSIGN RANDOM, REZek-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE IS DYNAMIC
                  RECORD KEY  IS REZek-anah
                 
                  STATUS FS-REZek.
              SELECT REZdigtar ASSIGN RANDOM, REZdigtar-DOSYA
                  ORGANIZATION INDEXED
                  ACCESS MODE IS DYNAMIC
                  RECORD KEY  IS REZdigtar-anah
                  ALTERNATE RECORD  rezdigtar-mail-tar  DUPLICATES
                  ALTERNATE RECORD  rezdigtar-odeme-tar DUPLICATES
                  ALTERNATE RECORD  rezdigtar-odemeeb-tar  DUPLICATES
                  ALTERNATE RECORD  rezdigtar-4-tar  DUPLICATES
                  ALTERNATE RECORD  rezdigtar-5-tar  DUPLICATES
                  ALTERNATE RECORD  rezdigtar-6-tar  DUPLICATES

                  STATUS FS-REZdigtar.
