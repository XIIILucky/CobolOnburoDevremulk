             SELECT DEPOZIT ASSIGN TO RANDOM DEPOZIT-DOSYA,
                  ORGANIZATION INDEXED,
                  ACCESS DYNAMIC,
                  RECORD KEY IS DEPOZIT-ANAHTAR,
                  ALTERNATE RECORD KEY DEPOZIT-ALT-1 =
                            DEPOZIT-DURUMU, DEPOZIT-TARIH DUPLICATES,
                  ALTERNATE RECORD KEY DEPOZIT-ALT-2 =
                         DEPOZIT-DURUMU, DEPOZIT-SIL-TARIH DUPLICATES,
                 STATUS FS-DEPOZIT.
