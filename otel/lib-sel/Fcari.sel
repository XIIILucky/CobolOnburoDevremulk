SELECT FCARI
    ASSIGN       TO RANDOM FCARI-DOSYA
    ORGANIZATION IS INDEXED
    ACCESS MODE  IS DYNAMIC
    FILE STATUS  IS FS-FCARI
    RECORD KEY   IS FCARI-ANAHTAR
    alternate record key fcari-adi1 = fcari-tipi,fcari-adi duplicates
    alternate record key fcari-kodu1 = fcari-tipi,fcari-kodu duplicates.
