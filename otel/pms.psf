 init-emir-grid.
      modify form1-gd-4,
             reset-grid = 1
             mass-update = 1
      initialize form1-gd-4-record
      move "#"                       to gd-4-col-1
      move "Bas.Tarih"               to gd-4-col-2
      move "Bas.Zam."                to gd-4-col-3
      move "Trid"                    to gd-4-col-4
      move "Emir"                    to gd-4-col-5
      move "Oda "                    to gd-4-col-6
      move "-"                       to gd-4-col-7
      move "Bit.Tarih"               to gd-4-col-8
