                                     move "0101" to donem-bit-tar(5:)
                                     move 1000 to ii
                                     exit perform cycle
                                       
                                 end-if
                                 continue
                
                              end-read
                      
                      end-perform
                      
                      if ii >= 1000 then
     
                            exit perform cycle
                      end-if
                      
                      move takvim-anah  to DONEM-BIT-TAR

                      compute donem-yili =  txt-donem-yili + i 
                      move donem-yili to donem-bas-yil donem-bit-yil

                      write donem-rec invalid
                            rewrite donem-rec 
                            end-rewrite 
                      end-write

                     

              end-perform

             
