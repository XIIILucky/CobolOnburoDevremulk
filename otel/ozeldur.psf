 exception-procedure.
     evaluate key-status
         when 1
         initialize infox-cagir
         move "Z"   to infox-tipi-cagir
         call "/asya/ytl/obj/otel/ozelara.asy" using infox-cagir
            on exception perform callerr-proc
               not on exception
         cancel "/asya/ytl/obj/otel/ozelara.asy" 
         end-call
         move infox-kodu-cagir     to kodlar02-kodu
              display accept-0101
              move 4 to accept-control
              move 5 to control-id
         when 2 
             open i-o kodlar02
             move "Z" to kodlar02-tipi

             move onli-bas-yil(3:2) to kodlar02-ozel-durum-onli-bas-yil  
             move onli-bas-ay       to kodlar02-ozel-durum-onli-bas-ay   
             move onli-bas-gun      to kodlar02-ozel-durum-onli-bas-gun  
        
             move onli-bit-yil(3:2) to kodlar02-ozel-durum-onli-bit-yil  
             move onli-bit-ay       to kodlar02-ozel-durum-onli-bit-ay   
             move onli-bit-gun      to kodlar02-ozel-durum-onli-bit-gun  

             write kodlar02-rec invalid 
                    rewrite kodlar02-rec 
                    end-rewrite 
              end-write
              perform log-operation-kodlar02
              close kodlar02
              perform listbox-yer-bul
              if i > zeroes
                   initialize form1-lb-1-value
                   modify form1-lb-1,
                          item-to-delete(i)
                   move kodlar02-kodu to lb-1-col-1
                   move kodlar02-adi  to lb-1-col-2
                   modify form1-lb-1,
                          item-to-add(form1-lb-1-value)
              else
                 initialize form1-lb-1-value
                 move kodlar02-kodu to lb-1-col-1
                 move kodlar02-adi  to lb-1-col-2
                 modify form1-lb-1,
                        item-to-add(form1-lb-1-value)
              end-if
              move 4 to accept-control
              move 5 to control-id
         when 3
              open i-o kodlar02
              move "Z" to kodlar02-tipi
              delete kodlar02 invalid continue 
              end-delete
              perform log-operation-kodlar02
              close kodlar02
              perform listbox-yer-bul
              if i > zeroes
                 modify form1-lb-1,
                        item-to-delete(i)
              end-if
              move 4 to accept-control
              move 5 to control-id
        when 4
             perform raporla
             exit paragraph

              open input kodlar02
              initialize kodlar02-rec
              move "Z" to kodlar02-tipi
              start kodlar02 key not < kodlar02-anah invalid
                    initialize mesaj-link
                    move 04 to mesaj-no
                    perform mesaj-ver
                    close kodlar02
                    exit paragraph
              end-start

              open i-o genelfis
              move 1 to genelfis-anahtar
              read genelfis no lock invalid
                   accept print-no from time
              not invalid
                   add 1 to print-no
                   rewrite genelfis-rec end-rewrite
              end-read
              close genelfis
              initialize dokumer-rec dokumer-anah dokumer-ozellikler-rec
                         dokumer-dosya

              set  dokumer-asya-set to true
              move print-no         to dokumer-dosya-adi

              open output dokumer
              move "Anlasma Tipleri Raporu" to dokumer-rec
              add 1 to dokumer-anah
              write dokumer-rec
              initialize dokumer-rec
              move "Anlasma Tipleri Raporu" to dokumer-rec
              add 1 to dokumer-anah
              write dokumer-rec
              initialize dokumer-rec
              add 1 to dokumer-anah
              write dokumer-rec
              move "E" to dokumer-oto-sayfa-basi
              move 56  to dokumer-oto-sayfa-satir
              move "|" to dokumer-detay-kolon-ayirici
              move dokumer-ozellikler-rec    to dokumer-rec
              add 1 to dokumer-anah
              write dokumer-rec

              initialize detay
              move "Kodu"       to det-1
              move "Adi"        to det-2
              move all "|" to fil-1 fil-2
              move detay to dokumer-rec
              add 1 to dokumer-anah
              write dokumer-rec
              initialize dokumer-rec
              move "LLL" to dokumer-rec
              add 1 to dokumer-anah
              write dokumer-rec

              initialize fs-kodlar02
              perform with test after until fs-kodlar02 = "10"
                    read kodlar02 next no lock end move "10" to fs-kodlar02
                    not at end
                     if kodlar02-tipi = "Z"
                         initialize detay
                         move kodlar02-kodu      to det-1
                         move kodlar02-adi       to det-2
                         add 1 to dokumer-anah
                         move detay to dokumer-rec
                         write dokumer-rec end-write
                     end-if
                    end-read
              end-perform

              close dokumer
              call dokumer-prog on exception
                   perform callerr-proc
              not on exception
                   cancel dokumer-prog
              end-call
              close kodlar02
              delete file dokumer
     end-evaluate.
