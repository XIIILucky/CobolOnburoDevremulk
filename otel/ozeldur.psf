{{@PARAGRAPH:
*
 Form1-Bef-Create.
     perform adresleri-tasi.
     .
*
 accept-0101-Bef-Procedure.
     initialize kodlar02-rec.
     display accept-0102,accept-0102a accept-0102ab c-ozl-tip accept-gec-bit-yil accept-gec-bit-ay accept-gec-bit-gun accept-gec-bas-gun accept-gec-bas-ay accept-gec-bas-yil Form1-Cb-1
     accept-gec-bas-yila accept-gec-bas-aya accept-gec-bas-guna accept-gec-bit-yila accept-gec-bit-aya accept-gec-bit-guna
     modify button-0101,enabled = false
     modify button-0102,enabled = false.
     perform bef-procedure.
     .
*
 accept-0101-Aft-Procedure.
     if kodlar02-kodu = spaces 
        modify button-0101,enabled = false
        modify button-0102,enabled = false
     else 
        modify button-0101,enabled = true
        modify button-0102,enabled = true
     end-if.
     open input kodlar02.
     move "Z" to kodlar02-tipi.
     read kodlar02 no lock invalid
          initialize kodlar02-adi
          move "Z" to kodlar02-tipi
     end-read
     
     initialize onli-bas-tar onli-bit-tar 
     if kodlar02-ozel-durum-onli-bas-yil > 0
             move 20                               to onli-bas-yil(1:2) 
             move kodlar02-ozel-durum-onli-bas-yil to onli-bas-yil(3:2)
