*
 Form1-Bef-Initdata.
      perform adresleri-tasi.
*
 Form1-Pb-2-Link.
     open i-o rez  CAST
     initialize rez-rec
     move "I"        to REZ-DURUMU
     move "20160605" to REZ-GIR-TAR
     start rez key not < REZ-GIR invalid
             continue
     not invalid
     perform until fs-rez = "10"
     read rez next no lock end move "10" to fs-rez
     not end
             if REZ-DURUMU <> "I"
                   exit perform
             end-if
             if REZ-GIR-TAR not >= "20160605"
                   exit perform
             end-if

             delete rez end-delete
             display cast-rec

             initialize cast-rec
             move rez-no to cast-rez-no
             start cast key not < CAST-ANAH1 invalid
                     continue
             not invalid
             perform until fs-cast = "10"
             read cast next no lock end move "10" to fs-cast
             not end
                      if cast-rez-no <> rez-no
                             exit perform
                      end-if

                      delete cast end-delete
                      display cast-rec
             end-read
             end-perform
             end-start

     end-read
     end-perform
     end-start
     close rez CAST
     Display message box "Bittiii"
     .
*
 Form1-Pb-2a-Link.
     open i-o rez  rez2 genelfis
     initialize rez-rec
     move "I"        to REZ-DURUMU
     move "20160605" to REZ-GIR-TAR
     start rez key not < REZ-GIR invalid
             continue
     not invalid
     perform until fs-rez = "10"
     read rez next no lock end move "10" to fs-rez
     not end
             if REZ-DURUMU <> "I"
                   exit perform
             end-if
             if REZ-GIR-TAR not >= "20160605"
                   exit perform
             end-if

             initialize genelfis-rec  
             move 1 to genelfis-anahtar
             read genelfis no lock invalid
                     stop " "
             not invalid
                 add 1 to  REZ-OTO
                 rewrite genelfis-rec end-rewrite
             end-read
 
             initialize rez2-rec
             move rez-rec to rez2-rec
             move REZ-OTO to rez2-no
             initialize rez2-fiyati
             move "0100" to rez2-acenta
             write rez2-rec invalid
                     stop " "
             end-write

             display rez2-rec

     end-read
     end-perform
     end-start
     close rez rez2 genelfis
     Display message box "Rez Aktarim Bittiii".

