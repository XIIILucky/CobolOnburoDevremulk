              not invalid
              perform until fs-odemeler = "10"
              read odemeler next no lock end move "10" to fs-odemeler
              not end
                   if odemeler-prim-isl-no not = prim-tediye-islem-no then
                         exit perform 
                   end-if

                   initialize hatali-islem
                   exit perform
              end-read
              end-perform
              end-start
         close odemeler
         .
*
 yetki-kontrol .
       open input kllnc
       initialize yetki-yok
       move k-kodu-tasi to K-KODU
       read kllnc no lock invalid
               continue
       not invalid
            if k-dev-prim-yetki not = 1 then
               move 1 to yetki-yok
            else
               move 0 to yetki-yok
            end-if
       end-read
       close kllnc      .

@}}

