    if not filtrede
       compute yeni-sutun = yeni-sutun - 0.5 
       modify grid6,
              visible = false
       modify pb-filtre-uygula,
              visible = false
       modify grid0,
              size = yeni-sutun
              lines = yeni-lines
       perform dokumer-grid-proc
       exit paragraph
    end-if

    compute yeni-sutun = yeni-sutun - dummy - 0.5
