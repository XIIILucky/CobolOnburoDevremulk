*
 gd-acenta-Ev-Msg-Bitmap-Dblclick.
     inquire gd-acenta,
       cursor-x in gd-x
       cursor-y in gd-y
     inquire gd-acenta(gd-y,3)
       hidden-data in gd-hidden

     if gd-hidden = 1
        move 7   to gd-bitmap
        move 0   to gd-renk
        move 0   to gd-hidden
        initialize gd-sec
     else
        move 1   to gd-bitmap
        move 352 to gd-renk
        move 1   to gd-hidden
        initialize  gd-sec
     end-if

     modify gd-acenta(gd-y,3)
       hidden-data     = gd-hidden
       bitmap          = artieksi-bmp
       bitmap-number   = gd-bitmap
       bitmap-width    = 16
       bitmap-trailing = 1
       row-color       = gd-renk
     set event-action to event-action-fail
     .
