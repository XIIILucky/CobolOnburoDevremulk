       bitmap          = artieksi-bmp
       bitmap-number   = gd-bitmap
       bitmap-width    = 16
       bitmap-trailing = 1
       row-color       = gd-renk
     .
*
 finish-grid.
     modify gd-acenta
       mass-update = 0
     .
