                                  son-butce-tek-satir
                                 perform butgr-tek-kaydet
                               move 0 to peryod-sayisi
                               initialize butgr-rec
                                else 
                               continue
                            end-if
                            if butce-price-pp1  not =  son-butce-price-pp1  or 
                               peryod-sayisi = 0
                                add 1 to peryod-sayisi 
                            end-if
                            perform  butgr-ekle
                          else
                          continue | tarih disi
                       end-if
                   end-if
                 move butce-rec to son-butce-rec

             end-read     
           end-perform

     end-start
     perform butgr-tek-kaydet
