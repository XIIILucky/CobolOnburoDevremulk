         not invalid
           perform until fs-butce = "10"
             read butce next 
               end move "10" to fs-butce
               not end
                  if butce-adi not =  b-dosya-adi
                     move "10" to fs-butce
                     else
                       if  bit-tar < butce-tarih
                             move "10" to fs-butce
                          end-if
                       if bas-tar <= butce-tarih and 
