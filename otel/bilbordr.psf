{{@PARAGRAPH:
*
 Form1-Bef-Create.
    perform adresleri-tasi.
*
 Form1-Aft-Initdata.
    move tarih-tasi to ilk-tarih son-tarih.
    modify lb-acenadi, title = spaces.
    display acc-01 acc-02 acc-03 acc-04 acc-05 acc-06.
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         
*
 acc-07-Aft-Procedure.
    if acn-kod = spaces
       move "Tum acentalar ..."   to acenta-adi
       modify lb-acenadi, title = acenta-adi of acenta
       exit paragraph
    end-if
    open input acenta.
    initialize acenta-rec.
    move acn-kod to acenta-kodu.
    read acenta no lock invalid 
        display message box "Tanimsiz Acenta Kodu..."
        move 4 to accept-control
        move 12 to control-id
      not invalid 
        modify lb-acenadi, title = acenta-adi of acenta
        modify button-acenara enabled = false
    end-read
    close acenta.
     .
*
 acc-07-Bef-Procedure.
    modify button-acenara enabled = true.
     .
*
 Form1-Ex-Other.
    evaluate key-status
      when 1
        evaluate control-id
        when 12
        when 16
             initialize acenta-cagir
             call "/asya/ytl/obj/otel/acenara.asy" 
                  using acenta-cagir  
                  on exception 
                     perform callerr-proc
                  not on exception
                     if acenta-cagir <> spaces
                        move acenta-cagir to acn-kod
                        display acc-07
                     end-if
             end-call
             move 4 to accept-control
             move 12 to control-id
        end-evaluate
        exit paragraph

      when 2
        perform takas-dosya-yaz thru takas-dosya-yaz-exit
              initialize takas-rec
              start takas key not < takas-tarih invalid
                    initialize mesaj-link
                    move 04 to mesaj-no
                    perform mesaj-ver
                    close takas
                    exit paragraph
              end-start

              open i-o genelfis
              move 1 to genelfis-anahtar
              read genelfis no lock invalid
                   accept print-no from time
              not invalid
                   add 1 to print-no
                   rewrite genelfis-rec end-rewrite
              end-read
              close genelfis

**************************
     initialize dokumer-rec dokumer-anah dokumer-ozellikler-rec
                dokumer-dosya

     set  dokumer-asya-set to true
