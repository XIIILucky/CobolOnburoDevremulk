     move "[1].Tutar"            to gd-1-col-9 
     move "[2]"                  to gd-1-col-10
     move "[2].Tutar"            to gd-1-col-11 
     move "[3]"                  to gd-1-col-12
     move "[3].Tutar"            to gd-1-col-13 
     move "[4]"                  to gd-1-col-14
     move "[4].Tutar"            to gd-1-col-15 
