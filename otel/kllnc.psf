                     move "E" to prosif-giris
                  else 
                     move "H" to prosif-giris
                  end-if
             when 2
                  move "E" to prosif-giris
         end-evaluate
         write prosif-rec invalid
             rewrite prosif-rec end-rewrite
         end-write

     end-read
     end-perform 
     end-start
     close isyeri
