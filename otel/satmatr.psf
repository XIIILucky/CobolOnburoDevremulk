{{@PARAGRAPH:
*
 Form1-Bef-Create.
    perform adresleri-tasi.
    open input genel
    move 1 to genel-anahtar
    read genel no lock invalid continue
         not invalid continue
    end-read
    close genel.
  



    open input genel2
    move 1 to genel2-anah
    read genel2 no lock invalid continue
         not invalid continue
    end-read
    close genel2.

    move ONKPARA-DOVIZ to butce-cev-doviz .
    move 1 to reel-gecmis
    
    
     .
*
 Form1-Aft-Initdata.
    move tarih-tasi to ilk-tarih  sat-tar son-tarih.
    move "1231" to son-tarih(5:)
    move "N" to rap-tip.          
    display acc-01 acc-02 acc-03 acc-04 acc-05 acc-06 acc-07 com-01 acc-01a acc-02a acc-03a.
    perform Form1-Ef-1-Aft-Procedure
    initialize sutun-2
    move 2 to xx
    move 38 to sut-yer sutun(xx)
    perform varying i from 1 by 1 until i > 4
     
       perform varying  i2 from 1 by 1 until i2 > 6
              add 1 to xx
              if data1(1:1) < 6 
                    add 8 to sut-yer 
                    else
                 add 14 to sut-yer
               end-if
               move sut-yer to  sutun(xx)
       end-perform
        
       perform varying  i2 from 1 by 1 until i2 > 6
              add 1 to xx
              if data2(1:1) < 6 
                    add 8 to sut-yer 
                     else
                    add 14 to sut-yer
             end-if
               move sut-yer to  sutun(xx)
       end-perform
    end-perform 

    
     modify com-ana1, item-to-add ("P-Pazar",  "T-Acenta","M-Merkez Acenta", "U-Ulke ", 
    "F-Fiyat Konumu", "O-Oda Konumu", "A-Anlasma " , "1-Kaynak1","2-Kaynak2","3-Kaynak3" )
     modify com-ana2, item-to-add (" -Detaysiz", "P-Pazar",  "T-Acenta", "M-Merkez Acenta", "U-Ulke ", 
    "F-Fiyat Konumu", "O-Oda Konumu", "A-Anlasma " , "1-Kaynak1","2-Kaynak2","3-Kaynak3","E-EB durumu") 

     modify com-data1, item-to-add ("1-Gelen Oda","2-Gelen Pax", "3-Oda Geceleme", "4-Pax Geceleme", "5-Oda Ort", 
     "6-Pax Ort", "7-Tutar  "  )
     modify com-data2, item-to-add ("1-Gelen Oda","2-Gelen Pax", "3-Oda Geceleme", "4-Pax Geceleme", "5-Oda Ort", 
     "6-Pax Ort", "7-Tutar  "  )
    move "P" to rap-anah1
    move "T" to rap-anah2
     move "4" to data1
     move "6" to data2
    display com-ana1 com-ana2 com-ana2a pax-tipi com-data2 com-data1
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         
     .
*
 Form1-Ex-Other.
    evaluate key-status
      when 1
        evaluate control-id
        when 12
        when 2001
             initialize acenta-cagir
             call "/asya/ytl/obj/otel/acenara.asy" 
                  using acenta-cagir  
                  on exception 
                     perform callerr-proc
                  not on exception
                     if acenta-cagir <> spaces
                        move acenta-cagir to acn-kod
                        display acc-07
                     end-if
             end-call
             move 4 to accept-control
             move 12 to control-id
        when 16
        when 17
             initialize acn-grup-kod
             call "/asya/ytl/obj/otel/grupara.asy" 
                  using "A", acn-grup-kod  
                  on exception 
