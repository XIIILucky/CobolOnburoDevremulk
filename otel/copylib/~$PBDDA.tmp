     move "Program ismi"           to gd-2-col-1
     move "Giris"                  to gd-2-col-2
     move "Yazma"                  to gd-2-col-3
     move "Duzel"                  to gd-2-col-4
     move "Silme"                  to gd-2-col-5
     move "Rapor"                  to gd-2-col-6
