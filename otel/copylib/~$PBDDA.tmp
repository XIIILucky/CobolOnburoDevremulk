 grid-2-Ev-Msg-Begin-Entry.
     inquire grid-2,
             y in i
             x in ii
             hidden-data in char
     if ii < 2 
        move event-action-fail to event-action
        exit paragraph.
     if char not = "H"
        move no-bmp  to bmp-handle
        move "H"     to char
     else
        move yes-bmp to bmp-handle
        move "E"     to char
     end-if.
     move i  to sayac
     move ii to bmp-x
     perform bmp-set.
     modify grid-2,
            y = i
            x = ii
            bitmap-trailing = 0
            hidden-data = char
            cell-data = x"ff"
     move event-action-fail   to event-action.
