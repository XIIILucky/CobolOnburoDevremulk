     move "02"                     to gd-2-col-8
     move "03"                     to gd-2-col-9
     move "04"                     to gd-2-col-10
     move "05"                     to gd-2-col-11
     move "06"                     to gd-2-col-12
     move "07"                     to gd-2-col-13
     move "08"                     to gd-2-col-14
     move "09"                     to gd-2-col-15
     move "10"                     to gd-2-col-16
