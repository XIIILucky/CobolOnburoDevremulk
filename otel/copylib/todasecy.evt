*
 Form1-Ex-Other.
     evaluate key-status
          when 2
              open i-o takas 
                  initialize takas-rec  i
                  move 1 to takas-eklenen
                  start takas key not < takas-eklenen invalid
                          continue
                  not invalid
                  perform until fs-takas = "10" 
                  read takas next no lock end move "10" to fs-takas
                  not end
                        if takas-eklenen <> 1 then
                              exit perform 
                        end-if
                        add 1 to i
                        move takas-oda to todasec-link-oda(i)
                  end-read
                  end-perform
                  end-start
               close takas
               set exit-pushed to true
          when 3
             open i-o takas 
                  initialize takas-rec  i
                  start takas key not < takas-oda invalid
                          continue
                  not invalid
                  perform until fs-takas = "10" 
                  read takas next no lock end move "10" to fs-takas
                  not end
                        move 0 to takas-eklenen
                        rewrite takas-rec invalid
                                stop " "
                        end-rewrite
                  end-read
                  end-perform
                  end-start
               close takas
               |perform takas-gride-yaz
               perform eklenen-gride-yaz
          when 21
             open i-o takas 
                  initialize takas-rec  i
                  move 1 to takas-secili
                  start takas key not < takas-secili invalid
                          continue
                  not invalid
                  perform until fs-takas = "10" 
                  read takas next no lock end move "10" to fs-takas
                  not end
                        if takas-secili <> 1 then
                              exit perform
                        end-if
                        move 1 to takas-eklenen
                        rewrite takas-rec invalid
                                stop " "
                        end-rewrite
                  end-read
                  end-perform
                  end-start
               close takas
               |perform takas-gride-yaz
               perform eklenen-gride-yaz
          when 22
             open i-o takas 
                  initialize takas-rec  i
                  move 1 to takas-secili
                  start takas key not < takas-secili invalid
                          continue
                  not invalid
                  perform until fs-takas = "10" 
                  read takas next no lock end move "10" to fs-takas
                  not end
                        if takas-secili <> 1 then
                              exit perform
                        end-if
                        move 0 to takas-eklenen
                        rewrite takas-rec invalid
                                stop " "
                        end-rewrite
                  end-read
                  end-perform
                  end-start
               close takas
               |perform takas-gride-yaz
               perform eklenen-gride-yaz
                 | set exit-pushed to true
     end-evaluate
