          read takvim next no lock
            end move "10" to fs-takvim
            not end
              evaluate true
                when takvim-anah not > son-tarih
                     compute toplam-gun = toplam-gun + 1
                when takvim-anah > son-tarih
                     move "10" to fs-takvim
              end-evaluate
          end-read
        end-perform
    end-start
