             read takas next no lock end move "10" to fs-takas
             not at end
                 if takas-tipi <> "E" 
                    exit perform cycle
                 end-if
                 
                 perform yeni-data-yaz

             end-read
             end-perform 
             end-start
        when 6           ||Kodlar02House-Takas
