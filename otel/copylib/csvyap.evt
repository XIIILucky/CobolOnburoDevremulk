                       when 1
                            perform ole-excel-detay-al
                            perform varying i from 1 by 1 until i > grid0-detay-sayi
                                    if i <= 50
                                       move dokumer-align(i)   to grid0-align(i)
                                    end-if
                            end-perform
                            initialize i ii grid0-i grid0-ii grid0-kisit-detaylar
                                   grid0-kisit-detay-sayi
                            perform varying i from 1 by 1 until i > grid0-detay-sayi
