*
 route-kaydet.
     inquire gd-1,
             last-row in ii

     if ii < 2 
        exit paragraph.


     initialize iii
     perform varying i
             from 1
             by 1
             until i > ii
             inquire gd-1(i,1)
                     hidden-data in hidden-bilgi
             if hidden-secim = "E"
                add 1 to iii
             end-if
     end-perform

     if iii = 0
        display message box
                "Herhangi bir secim yapmadiniz !!!"
                icon mb_warning_icon
                title "Bilgi"
                exit paragraph
     end-if

     if radio-value = 0
        display message box 
                "Routing Tipi Secmediniz !!!"
                icon mb_warning_icon
                title "Bilgi"
                exit paragraph
     end-if
         

     open i-o route
     perform varying i
             from 2
             by 1
             until i > ii
             inquire gd-1(i,1),
                     hidden-data in hidden-bilgi

             if hidden-secim <> "E"
                exit perform cycle
             end-if
             perform varying iii
                     from 1
                     by 1
                     until iii > max-occ
                     if routsec-rez-no(iii) = zeroes             
                        exit perform cycle
                     end-if
                     initialize route-rec

                     if not routsec-tip-folio
                        move routsec-rez-no(iii)                  to route-rezno
                     else
                        move routsec-rez-no(iii)                  to route-folio
                     end-if
                     if  routsec-tip-grup
                        move routsec-rez-no(iii)                  to route-folio
                     end-if
                     move hidden-depkod                           to route-depkod
                     read route no lock invalid
                          initialize route-rec
                          if not routsec-tip-folio
                             move routsec-rez-no(iii)             to route-rezno
                          else
                             move routsec-rez-no(iii)             to route-folio
                          end-if
                           if  routsec-tip-grup
                              move routsec-rez-no(iii)            to route-folio
                          end-if
                          move hidden-depkod                      to route-depkod
                     end-read
                     evaluate radio-value
                     when 1 | Rezerveyþýn ýnstrakþýns
                            move acc-rez-value             to route-no
                            move "R"                       to route-tip
                     when 2
                            move acc-folio-value           to route-no
                            move "F"                       to route-tip
                     when 3
                            move combo-1-value(1:1)        to route-no
                            move "P"                       to route-tip
                     end-evaluate
                 
                     write route-rec invalid
                           rewrite route-rec end-rewrite
                     end-write

             end-perform
                     
     end-perform

     close route.
     display message box
             "Routing Kaydedildi"
             title "Bilgi".


     perform reset-bilgi-goster
     initialize acc-folio-value acc-rez-value radio-value enb-1 enb-2 enb-3
     display acc-rez acc-folio rb-1 rb-2 rb-3 pb-folio pb-rez combo-1
     
     perform load-grid.

