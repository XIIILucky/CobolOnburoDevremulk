*
 cell-renk.
*/ eger yonledirilen yer "R" rezervasyon ise renk Y-Yesil ise kýrmýzý alt beyaz ust renk ver
*/ eger yonledirilen yer "F" rezervasyon ise renk Y-Yesil ise kýrmýzý alt beyaz ust renk ver
*/ eger yonledirilen yer "P" rezervasyon ise renk Y-Yesil ise kýrmýzý alt beyaz ust renk ver
*/ eger yonledirilen yer "*" rezervasyon ise renk Y-Yesil ise kýrmýzý alt beyaz ust renk ver
     initialize renk
     evaluate hidden-tip(1)
     when "R"
          if hidden-tip(2) = spaces and
             hidden-tip(3) = spaces and
             hidden-tip(4) = spaces
                evaluate hidden-renk
                when "Y"
                     move 176         to renk
                when "G"
                     move 304         to renk
                end-evaluate
          else
             move 304 to renk | 
          end-if
     when "F" 
          if hidden-tip(2) = spaces and
             hidden-tip(3) = spaces and
             hidden-tip(4) = spaces
                evaluate hidden-renk
                when "Y"
                     move 80         to renk
                when "G"
                     move 304         to renk
                end-evaluate
          else
             move 304 to renk | 
          end-if
     when "P"
          if hidden-tip(2) = spaces and
             hidden-tip(3) = spaces and
             hidden-tip(4) = spaces
                evaluate hidden-renk
                when "Y"
                     move 112         to renk
                when "G"
                     move 304         to renk
                end-evaluate
          else
             move 304 to renk | 
          end-if 
     when "*"
          move 304         to renk
     end-evaluate
     if renk = 0
        exit paragraph.

     modify gd-1(ii)
            x = 5
            cell-color = renk
            x = 6
            cell-color = renk.
