      when 4
           perform dokumere-yolla

      when 1
        evaluate control-id
        when 4
       

        when 12
        when 16
             initialize acenta-cagir
             call "/asya/ytl/obj/otel/acenara.asy" 
                  using acenta-cagir  
                  on exception 
                     perform callerr-proc
                  not on exception
                     if acenta-cagir <> spaces
                        move acenta-cagir to acn-kod
                        display acc-01
                     end-if
             end-call
             move 4 to accept-control
             move 12 to control-id
        end-evaluate
      end-evaluate.
dokumere-yolla.
      

 
