                            and rez-grup-no > 0 then
                              exit perform cycle
                         end-if
                         initialize konuk-rec 
                         move rez-folio      to konuk-folio
                         read konuk no lock invalid 
                              continue 
                         not invalid 
                                 if cl-cikma = 1 and
                                    konuk-duzeltme = 1 
                                       exit perform cycle                                
                                 end-if                             
                         end-read 
                            if sadece-sejour = 1
                               if rez-sejour-entegre not = 1
                                   exit perform cycle
                               end-if 
                            end-if 
                           
