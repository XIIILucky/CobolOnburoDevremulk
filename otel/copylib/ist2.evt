*
 takvim-start.
     
     start takvim key >= takvim-anah
       invalid
         continue
       not invalid
         initialize fs-takvim
         perform with test after
                 until fs-takvim = "10"
           perform takvim-oku
         end-perform
     end-start              
     .
*
 takvim-oku.
     read takvim next no lock
       end
         move "10" to fs-takvim
       not end
         if takvim-anah > kont-tarih
            move "10" to fs-takvim
         else
            evaluate true
              when onceki-yil
                if takvim-anah = kont-tarih
                   move takvim-hazir-oda to gegun-top-oda
                end-if
                if takvim-yil = kont-yil and
                   takvim-ay = kont-ay
                   add takvim-hazir-oda  to geay-top-oda
                end-if
