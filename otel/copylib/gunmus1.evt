        not invalid,
        move spaces to evet-hayir,
        perform with test after until evet,
            read cast next no lock end move "E" to evet-hayir,
                not end,
                if cast-tarih > rapor-tarih,
                        move "E" to evet-hayir,
                end-if,
                if cast-tarih = rapor-tarih,
                   perform rez-al thru rez-al-exit,
