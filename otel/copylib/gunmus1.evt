       compute takas-tutar-kdvli  rounded = takas-tutar-kdvsiz +
                                    ((takas-tutar-kdvsiz / 100) * cinpara-mus-kdv)
    end-if
     if CINPARA-TASTIKLI-0-ciksin not = 1 and takas-tutar-kdvli = 0 then 
