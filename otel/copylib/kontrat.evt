 Form1-Ex-Other.
     evaluate key-status
         when 17 
             move kontrat-dosya         to link-logview-dosya-adi
             set link-logview-pointer  to address of kontrat-rec
             set link-logview-rec-size to size of kontrat-rec
             perform logview-call
             initialize key-status

          when 1 
              if 
              (control-id = 6 or 79)
              initialize acenta-cagir
              call "/asya/ytl/obj/otel/acenara.asy" using acenta-cagir
              on exception 
                 perform callerr-proc
              not on exception
              cancel "/asya/ytl/obj/otel/acenara.asy"
              end-call
              if acenta-cagir <> spaces
                 move acenta-cagir   to acc-acenta-kodu
                 display acc-acenta
              end-if
              move 4 to accept-control
              move 6 to control-id
              exit paragraph
              end-if
          when 2
               if control-id < 17 or control-id = 5 or control-id = 72
                  exit paragraph
               end-if
               perform kaydet
          when 3
               if control-id < 17 or control-id = 5 or control-id = 72
                  exit paragraph
               end-if
               move control-id to yedek-id
               perform sil
               move 4 to accept-control
               move yedek-id to control-id
          when 4
               perform rapor
          when 5
               if control-id < 17 or control-id = 5 or control-id = 72
                  exit paragraph
               end-if
               initialize mesaj-link
               move 12 to mesaj-no
               perform mesaj-ver
               if mesaj-evet 
                  perform tum-sil
                  perform giris-yap
               end-if
          when 6
               perform giris-yap
     end-evaluate.
