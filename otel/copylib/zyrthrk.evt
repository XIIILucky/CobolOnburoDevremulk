           close zyrt
           exit paragraph
           else 
           continue
        end-if

        if zyrt-giris <> "G"
           display message box "Henuz giris yapmadiniz...!!!"
           close zyrt
           exit paragraph
           else continue
        end-if
        if zyrt-cikis = "C"
           display message box "Ziyaretci cikisi zaten yapilmis...!!!"
           close zyrt
           exit paragraph
           else continue
        end-if
