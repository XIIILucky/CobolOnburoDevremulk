    move "kur.txt"  to fidetext-adi
    move "kur2.txt" to amontext-adi
    open input fidetext
    open output amontext kur
     close kur   

     open i-o kur  fdoviz takvim
     initialize kur-rec  
     read fidetext next no lock
    end continue
    end-read
    perform until fs-fidetext = "10"
     initialize fidetext-rec    
    read fidetext next no lock
    end 
    not end
    move 1 to sayac
    initialize amontext-rec
    perform varying k from 1 by 1 until k > 100
    unstring    fidetext-rec delimited by x"09"
         into amontext-det(k)   pointer sayac
    end-perform
                   
     if function numval(amontext-det(1)) > 0        
            initialize kur-rec 
            move amontext-det(2)   to kur-tarih
            move 1                 to kur-banka
            initialize fdoviz-rec z-depanah
            move amontext-det(1)         to fdoviz-fide-kodu    
            read fdoviz no lock invalid                 
                 exit perform cycle 
            not invalid 
                  move fdoviz-asya-kodu                   to kur-doviz
            end-read 
*            if d = 1 stop " " end-if
           inspect amontext-det(4) replacing all "." by ","
           move function numval(amontext-det(4))  to doviz-alis

        write kur-rec invalid 
          rewrite kur-rec end-rewrite  
        end-write
         
     end-if
    end-read
    end-perform

    initialize takvim-rec
    start takvim key not < takvim-anah invalid 
        continue 
    not invalid 
    perform with test after until fs-takvim = "10"
    read takvim next no lock end move "10" to fs-takvim
    not at end 
            initialize kur-rec
            move takvim-anah   to kur-tarih
            move 1             to kur-banka
            move 1             to kur-doviz
            move 1             to doviz-alis
            write kur-rec end-write
    end-read 
    end-perform
    end-start
    close fidetext close amontext kur fdoviz takvim.

