 listbox-yer-bul.
     initialize durum tekrar
     perform varying i from 1 by 1 until (durum = "E" or "H")
             initialize egrup-alt
             modify form1-lb-1,
                    query-index = i
             inquire form1-lb-1,
                    item-value in egrup-alt
             if egrup-alt <> spaces
                if egrup-alt = tekrar
                   move "E" to durum
                   exit perform
                else
                   move egrup-alt to tekrar
                end-if
             end-if
             if egrup-alt = spaces
                move "H" to durum
                initialize i 
                exit perform
             else
                if egrup-alt = grup-alt
                   move "E" to durum
                   exit perform
                end-if
             end-if
    end-perform
