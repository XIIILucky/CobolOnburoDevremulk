                 (aceanlas-hs not = 1 or ace-hs-var = 1) and 
                 (aceanlas-sart-var not = 1 or 
                 (ace-sart-gece >= aceanlas-sart-min-gece and 
                 ace-SART-gece <= aceanlas-sart-max-gece)) then 
                 move 1 to def-anl-bulundu
                 move "10" to fs-aceanlas 
