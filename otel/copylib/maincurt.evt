                              else
                                 if depkod-tipi = 5
                                    if depkod-adi(1:9) = "KUR FARKI" or 
                                       depkod-adi(1:9) = "Kur Farki" or 
                                       depkod-adi(1:5) = "KUR F" or 
                                       depkod-adi(1:5) = "Kur F"  
                                       add exthrk-tl-tutar to maincurt-kur-arti     |firat ekleme 07/09/2020
                                       initialize exthrk-tl-tutar
                                    else
                                       add exthrk-tl-tutar to  maincurt-borc-diger    
                                       initialize exthrk-tl-tutar                 
                                    end-if

