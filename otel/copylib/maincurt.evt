                         when depkod-turu = 3  
                              subtract exthrk-tl-tutar from maincurt-alac-pesin 
                              initialize exthrk-tl-tutar

                         when depkod-turu = 4
                              subtract exthrk-tl-tutar from maincurt-alac-pesin 
                              initialize exthrk-tl-tutar

                         when other
                              if depkod-tipi = 2 then
