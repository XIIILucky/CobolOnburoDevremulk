        move 4 to accept-control
        move 12 to control-id
        exit paragraph
     end-if

     if yil-tasi <> acc-param-1-value
        display message box 
                "Calisma Yili Disinda Bilgi Girilemez..."
                icon mb-error-icon
                title "Hata"
