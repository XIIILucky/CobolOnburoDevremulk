     evaluate kolbant-durumu
         when 1
              add 1 to sayac-aktif
              add 1 to sayac-bant-top
         when 2
              add 1 to sayac-zaiyat
              add 1 to sayac-bant-top
              add 1 to sayac-pasif
         when 3
              add 1 to sayac-kayip
              add 1 to sayac-bant-top
              add 1 to sayac-pasif
         when 4
              add 1 to sayac-c-out
              add 1 to sayac-bant-top
              add 1 to sayac-pasif
     end-evaluate
     if det-1 <> spaces
        add 1 to sayac-rez-top
     end-if
