{{@PARAGRAPH:
*
 Form1-Bef-Create.
     perform adresleri-tasi.
     .
*
 accept-0101-Bef-Procedure.
     initialize tezkod02-rec asya-karsilik-adi.
     display accept-0102 accept-0102a accept-0102aa.
     modify button-0101,enabled = false
     modify button-0102,enabled = false.
     perform bef-procedure.
     .
*
 accept-0101-Aft-Procedure.
     if tezkod02-kodu = spaces 
        modify button-0101,enabled = false
        modify button-0102,enabled = false
     else 
        modify button-0101,enabled = true
        modify button-0102,enabled = true
     end-if.

     open input tezkod02.
     move "E" to tezkod02-tipi.
     read tezkod02 no lock invalid
          initialize tezkod02-entegre-adi tezkod02-asya-kodu
          move "E" to tezkod02-tipi
     end-read
     display accept-0102 accept-0102a accept-0102aa.
     close tezkod02.
        
     perform listbox-yer-bul.
     if i > zeroes
        modify form1-lb-1,
               selection-index = i
     end-if.
     .
*
 exception-procedure.
     evaluate key-status
         when 2 
              open i-o tezkod02
              move "E" to tezkod02-tipi
              write tezkod02-rec invalid 
                    rewrite tezkod02-rec 
                    end-rewrite 
              end-write
***                  perform log-operation-tezkod02
              close tezkod02
              perform listbox-yer-bul
              if i > zeroes
                   initialize form1-lb-1-value
                   modify form1-lb-1,
                          item-to-delete(i)
                   move tezkod02-kodu to lb-1-col-1
                   move tezkod02-entegre-adi  to lb-1-col-2
                   modify form1-lb-1,
                          item-to-add(form1-lb-1-value)
              else
                 initialize form1-lb-1-value
                 move tezkod02-kodu to lb-1-col-1
                 move tezkod02-entegre-adi  to lb-1-col-2
                 modify form1-lb-1,
                        item-to-add(form1-lb-1-value)
              end-if
              move 4 to accept-control
              move 5 to control-id

         when 3
              open i-o tezkod02
              move "E" to tezkod02-tipi
              delete tezkod02 invalid continue 
              end-delete
*                  perform log-operation-tezkod02
              close tezkod02
              perform listbox-yer-bul
              if i > zeroes
                 modify form1-lb-1,
                        item-to-delete(i)
              end-if
              move 4 to accept-control
              move 5 to control-id
        when 4
              open input tezkod02
              initialize tezkod02-rec
              move "E" to tezkod02-tipi
              start tezkod02 key not < tezkod02-anah invalid
                    initialize mesaj-link
                    move 04 to mesaj-no
                    perform mesaj-ver
                    close tezkod02
                    exit paragraph
              end-start

              open i-o genelfis
              move 1 to genelfis-anahtar
              read genelfis no lock invalid
                   accept print-no from time
              not invalid
                   add 1 to print-no
                   rewrite genelfis-rec end-rewrite
              end-read
              close genelfis
**************************
     initialize dokumer-rec dokumer-anah dokumer-ozellikler-rec
                dokumer-dosya

     set  dokumer-asya-set to true
     move print-no         to dokumer-dosya-adi
     open output dokumer
*/WINDOW TITLE
     initialize dokumer-rec detay
     move "W"                  to det-dokumer-20(1:)
     move "Kullanici Kodu Raporu " to det-filler
     write dokumer-rec from detay
*/ BASLIKLAR
     initialize dokumer-rec detay
     move "E"                  to det-dokumer-20(1:1)
     move "1"          to det-dokumer-20(10:1)
     move "Kullanici Kodu Raporu " to det-filler
     write dokumer-rec from detay

     initialize dokumer-rec detay
     move "E"                  to det-dokumer-20(1:1)
     write dokumer-rec from detay

*/ DOKUMER OZELLIKLER-REC
     initialize dokumer-rec detay
     move "O" to det-dokumer-20(1:)
     move "E" to dokumer-oto-sayfa-basi
     move 56  to dokumer-oto-sayfa-satir
     move "|" to dokumer-detay-kolon-ayirici
     move "LLLLLLLLLLLL" to dokumer-align-occ
     move dokumer-ozellikler-rec     to det-filler
     write dokumer-rec from detay
*/ BASLIKLAR  s
     initialize dokumer-rec detay
     move "D1"         to det-dokumer-20(1:2)
     move "1"          to det-dokumer-20(10:1)
