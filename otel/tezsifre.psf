              initialize fs-tezkod02
              perform with test after until fs-tezkod02 = "10"
                    read tezkod02 next no lock end move "10" to fs-tezkod02
                    not at end
                     if tezkod02-tipi = "E"
                         initialize dokumer-rec detay
                         move tezkod02-kodu      to det-1
                         move tezkod02-entegre-adi       to det-2
                         move tezkod02-asya-kodu          to det-3
                         move "|" to fil-1 fil-2 fil-3
                         write dokumer-rec from detay
                     end-if
                    end-read
              end-perform
                  initialize dokumer-rec detay
                  move "-"            to det-dokumer-20(5:1)
                  move all "-"  to det-1 det-2 det-3
                  write dokumer-rec from detay

              close dokumer
              close tezkod02
              call dokumer-prog on exception
                   perform callerr-proc
              not on exception
                   cancel dokumer-prog
              end-call
              delete file dokumer
     end-evaluate.
 bef-procedure.
     .

 Form1-Aft-Initdata.
     open i-o tezkod02 close tezkod02
     modify form1-lb-1,
            reset-list  = 1
            mass-update = 1
     open input tezkod02
     initialize tezkod02-rec
     move "E" to tezkod02-tipi
     start tezkod02 key not < tezkod02-anah invalid
           continue
     not invalid
     move zeroes to fs-tezkod02
     perform with test after until fs-tezkod02 = "10"
          read tezkod02 next no lock end move "10" to fs-tezkod02
          not at end
              if tezkod02-tipi = "E"
               initialize form1-lb-1-value
               move tezkod02-kodu to lb-1-col-1
               move tezkod02-entegre-adi to lb-1-col-2
               modify form1-lb-1,
                      item-to-add(form1-lb-1-value)
              else 
               move "10" to fs-tezkod02
              end-if
          end-read
     end-perform
     end-start
     close tezkod02.
     initialize tezkod02-rec
     modify form1-lb-1,
            mass-update = 0
            selection-index = 1
            thumb-position = 1.
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         
*
 listbox-eventlar.
     evaluate event-type
          when cmd-dblclick
               inquire form1-lb-1,value in form1-lb-1-value
               move lb-1-col-1 to tezkod02-kodu
               perform accept-0101-Aft-Procedure
               move 4 to accept-control
               move 8 to control-id
               exit paragraph
          when ntf-selchange
          when cmd-goto
               inquire form1-lb-1,value in form1-lb-1-value
               move lb-1-col-1 to tezkod02-kodu
               display accept-0101,
               perform accept-0101-Aft-Procedure
     end-evaluate.
     
     .
 listbox-yer-bul.
     initialize durum tekrar
     perform varying i from 1 by 1 until (durum = "E" or "H")
             initialize etezkod02-kodu
             modify form1-lb-1,
                    query-index = i
             inquire form1-lb-1,
                    item-value in etezkod02-kodu
             if etezkod02-kodu <> spaces
                if etezkod02-kodu = tekrar
                   move "E" to durum
                   exit perform
                else
                   move etezkod02-kodu to tekrar
                end-if
             end-if
             if etezkod02-kodu = spaces
                move "H" to durum
                initialize i 
                exit perform
             else
                if etezkod02-kodu = tezkod02-kodu
                   move "E" to durum
                   exit perform
                end-if
             end-if
    end-perform.
*
 Form1-Pb-2-Link.
            initialize forlink-tum
            move "20200320"  to forlink-bas-tarih
            move "20200420"  to forlink-bit-tarih
                call "/asya/ytl/obj/otel/for04m.asy" using forlink-tum 
             on exception                
                exit paragraph
             not on exception
             cancel "/asya/ytl/obj/otel/for04m.asy"
             end-call
     
     .

