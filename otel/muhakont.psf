                   not end
                      move exthrk-rec to hrkgir-rec
                      if hrkgir-tarih > ilk-tarih
                            move "10" to fs-exthrk
                          else
                           move hrkgir-folio to konuk-folio
                           read konuk no lock 
                                 invalid 
                                    stop " "
                           end-read
                           perform tek-hesap-at
                     end-if                
               end-read
            end-perform 
       end-start  .
    
