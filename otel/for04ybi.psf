 cast-oku.
    initialize cast-rec.
    move takvim-anah to cast-tarih.
    start cast key not < cast-tarih invalid continue,
        not invalid,

        move spaces to var-yok,
        perform with test after until var,
            read cast next no lock end move "V" to var-yok,
                 not end,

                 if cast-tarih > takvim-anah move "V" to var-yok,
                     else,
                     if cast-tarih not > takvim-anah,
                        initialize rez-rec,
                        move cast-rez-no to rez-no,
                        read rez no lock invalid continue,
                                     not invalid 
                         if rez-durumu = "I" and
                            rez-bilbord-eh  = "E" and    
                            rez-cik-tar not = takvim-anah 

                            move cast-tarih       to bil-dus-tarih
                            move rez-acenta       to bil-dus-acenta
                            move rez-fiyat-konumu to bil-dus-fiyat-konumu

                            perform tek-bilbord-dus

                         end-if
                         if filtre-var = 1 then
                             perform filtre
                             if filtre-gecti = 1 then 
                                  perform takas-kaydet thru takas-kaydet-exit
                             end-if
                           else
                            perform takas-kaydet thru takas-kaydet-exit 
                          end-if
                        end-read,
                     end-if,
                 end-if,
            end-read,
        end-perform,
    end-start.
 cast-oku-exit.
    exit.
