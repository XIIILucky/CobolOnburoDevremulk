                           when "NP"               
                           when "PM"
                           when "AG"               
                           when "DP"                
                           when "GS"               
                           when "GN"
                           WHEN "PP"
                           WHEN "SS"
                           when "AV"
                           when "BK"
                           when "BO"
                                exit perform cycle 
                           when "FO"
                               continue                 
                           when "PI"
                               continue 
                           WHEN OTHER 
                               continue 
                           end-evaluate
                 

                     if txt-donem-bit-tar not >= odemeler-tarih then
                           exit perform 
                     end-if

                     initialize takasodeme-rec
                     move  odemeler-devremulk-no to takasodeme-devremulk-no
                     read takasodeme no lock invalid
                             move 1 to donguden-cikti 
                             exit perform cycle
