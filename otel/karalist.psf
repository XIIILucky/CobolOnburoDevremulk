{{@PARAGRAPH:
*
 Form1-Bef-Create.
     perform adresleri-tasi
     .
*
 Form1-Aft-Initdata.
     inquire acc-adi
       id in id-adi
     perform lb-veri-yerlestir.
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         
*
 exception-procedure.
     evaluate key-status
       when 17 
            move karalist-dosya         to link-logview-dosya-adi
            set link-logview-pointer  to address of karalist-rec
            set link-logview-rec-size to size of karalist-rec
            perform logview-call
            initialize key-status
       when 1 perform yardimlar
       when 2 perform kayit-karalist
       when 3 perform sil-karalist
       when 4 perform raporla
     end-evaluate
     .
*
 listbox-eventlar.
     evaluate event-type
          when cmd-dblclick
          when ntf-selchange
          when cmd-goto
               perform lb-veri-al
               perform goster-anah
               perform Aft-Proc-acc-soyadi
     end-evaluate
     .
*
 lb-veri-al.
     inquire lb-sec,
       value in lb-vars
     inquire cb-tipi
       value in karalist-tipi

*     move lb-adi to rap-adi             |firat karalist-adi uzatýldý  29/05/2020
*                    karalist-adi
     move lb-adi(1:20)  to rap-adi              
                           karalist-adi
     move lb-adi(21:15) to rap-adi(21:15)
                           karalist-adi-ek

     move lb-soyadi     to rap-soyadi
                           karalist-soyadi
     .
*
 goster-anah.
     display cb-tipi
             acc-adi
             acc-soyadi
     .
*
 goster-kayit.
     display acc-ack-1
             acc-ack-2
             acc-staff
     .
*
 listbox-yer-bul.
     initialize tekrar
     move all high-value to lb-vars
     perform varying i
                from 1
                  by 1
               until ((lb-adi = rap-adi and
                       lb-soyadi = rap-soyadi) or
                       lb-vars = spaces)
       perform oku-lb-sira
     end-perform
     if lb-vars = tekrar
        subtract 1 from i
     end-if
     if i > zeroes
        modify lb-sec
          selection-index = i
     end-if
     .
*
 oku-lb-sira.
     modify lb-sec
       query-index = i
     inquire lb-sec
       item-value in lb-vars
     if lb-vars = tekrar
        move spaces to lb-vars
     else
        move lb-vars to tekrar
     end-if
     .
*
 lb-veri-yerlestir.
     initialize i
     modify lb-sec
       reset-list  = 1
       mass-update = 1

     open i-o karalist
     initialize karalist-rec
     move rap-tipi to karalist-tipi

     perform start-karalist

     close karalist

     modify lb-sec
       mass-update     = 0
       selection-index = 1
       thumb-position  = 1
     .
*
 start-karalist.
     start karalist key >= karalist-anah
       invalid
         continue
       not invalid
         initialize fs-karalist
         perform loop-karalist
     end-start
     .
*
 loop-karalist.
     perform with test after
             until fs-karalist = "10"
       perform oku-karalist
     end-perform
     .
*
 oku-karalist.
     read karalist next no lock
       end
         move "10" to fs-karalist
       not at end
         perform filtre-bitir-karalist
     end-read
     .
*
 filtre-bitir-karalist.
     if karalist-tipi not = rap-tipi
        move "10" to fs-karalist
     else
        perform yaz-lb-deger
     end-if
     .
*
 yaz-lb-deger.
*     move karalist-adi    to lb-adi                 |firat karalist-adi uzatýldý  29/05/2020
     move karalist-adi    to lb-adi                 
     move karalist-adi-ek to lb-adi(21:15)                 

     move karalist-soyadi to lb-soyadi

     modify lb-sec
       item-to-add(lb-vars)
     .
*
 Bef-Proc-cb-tipi.
     initialize rap-adi
                rap-soyadi
                karalist-rec
     perform Goster-Anah
     perform Goster-Kayit
     .
*
 Ntf-Selchange-cb-tipi.
     evaluate rap-tipi
       when "A" 
         modify la-adi,
           TITLE = "Acenta Kodu : "
         modify la-soyadi, 
           title = "Acenta Adi : "
         modify acc-soyadi,
           ENABLED = false
       when "M"
       when "P"
         modify la-adi,
           TITLE = "Adi : "
         modify la-soyadi, 
           title = "Soyadi : "
         modify acc-soyadi,
           ENABLED = true
     end-evaluate.
     display la-adi
             la-soyadi
     initialize rap-adi
                rap-soyadi
                karalist-rec
     perform goster-anah
     perform goster-kayit
     perform lb-veri-yerlestir 
     .
*
 Aft-Proc-acc-adi.
     if rap-tipi = "A"
        perform oku-acenta
        move acenta-adi to rap-soyadi
        display acc-soyadi
        if acenta-adi = "Tanimsiz"
           move 4      to accept-control
           move id-adi to control-id
        end-if
     end-if
     .
*
 Aft-Proc-acc-soyadi.
     move rap-tipi   to karalist-tipi

*     move rap-adi    to karalist-adi         |firat karalist-adi uzatýldý  29/05/2020
     move rap-adi(1:20)  to karalist-adi
     move rap-adi(21:15) to karalist-adi-ek

     move rap-soyadi to karalist-soyadi
     open i-o karalist
     perform oku-kayit-karalist
     close karalist
     perform goster-kayit
     perform listbox-yer-bul.
     if i > zeroes
        modify lb-sec,
          selection-index = i
     end-if
     .
*
 oku-kayit-karalist.
     read karalist record no lock
       invalid
         initialize karalist-rec
     end-read
     .
*
 yardimlar.
     if rap-tipi = "A" and
        control-id = id-adi

        call "/asya/ytl/obj/otel/acenara.asy"
          using rap-adi
          on exception
            perform callerr-proc
          not on exception
            cancel "/asya/ytl/obj/otel/acenara.asy"
        end-call
        perform oku-acenta
        move acenta-adi to rap-soyadi
        display acc-adi
                acc-soyadi
     end-if
     .
*
 oku-acenta.
     open input acenta
     initialize acenta-rec
     move rap-adi to acenta-kodu
     read acenta no lock
       invalid
         move "Tanimsiz" to acenta-adi
     end-read
     close acenta
     .
*
 kayit-karalist.
     initialize soru-deg
     perform kaydet-soru
     if soru-evet
        open i-o karalist
        move rap-tipi    to karalist-tipi

*        move rap-adi     to karalist-adi          |firat karalist-adi uzatýldý  29/05/2020
        move rap-adi(1:20)  to karalist-adi  
        move rap-adi(21:15) to karalist-adi-ek  

        move rap-soyadi  to karalist-soyadi
        move k-kodu-tasi to karalist-staf
        write karalist-rec
          invalid
            rewrite karalist-rec, end-rewrite
        end-write
        perform log-operation-karalist
        close karalist
        perform listbox-yer-bul
        if i > zeroes
           initialize lb-vars
           modify lb-sec
             item-to-delete(i)
        end-if

*        move rap-adi(1:20)    to lb-adi                |firat listbox içinden 35 yapýldý 29/05/2020
        move rap-adi(1:20)    to lb-adi
        move rap-adi(21:15)   to lb-adi(21:)

        move rap-soyadi       to lb-soyadi
        modify lb-sec,
          item-to-add(lb-vars)
        move 4 to accept-control
        inquire cb-tipi
          id in control-id
     end-if
     .
*
 sil-karalist.
     initialize soru-deg
     perform sil-soru
     if soru-evet
        move rap-tipi   to karalist-tipi

*        move rap-adi    to karalist-adi     |firat karalist-adi uzatýldý  29/05/2020
        move rap-adi(1:20)  to karalist-adi  
        move rap-adi(21:15) to karalist-adi-ek  

        move rap-soyadi to karalist-soyadi
        perform listbox-yer-bul
        if i > zeroes
           modify lb-sec,
             item-to-delete(i)
        end-if
        open i-o karalist
        delete karalist
          invalid
            continue
        end-delete
        perform log-operation-karalist
        close karalist
        move 4 to accept-control
        inquire cb-tipi
          id in control-id
     end-if
     .
*
 raporla.
     perform dokumer-fisno-al
     perform dokumer-init
     perform dokumer-win-title
     perform dokumer-ust-baslik
     perform dokumer-ozellikler
     perform dokumer-basliklar
     perform dokumer-satirlar
     perform dokumer-finish
     .
*
 dokumer-fisno-al.
     open i-o genelfis
     move 1 to genelfis-anahtar
     read genelfis no lock
       invalid
         accept print-no from time
       not invalid
         add 1 to print-no
         rewrite genelfis-rec, end-rewrite
     end-read
     close genelfis
     .
*
 dokumer-init.
     initialize dokumer-rec
                dokumer-anah
                dokumer-ozellikler-rec
                dokumer-dosya

     set  dokumer-asya-set to true
     move print-no         to dokumer-dosya-adi
     open output dokumer
     .
*
 dokumer-win-title.
     initialize dokumer-rec
     set dokumer-20-win-baslik to true
     move "Karaliste Raporu" to dokumer-400
     write dokumer-rec, end-write
     .
*
 dokumer-ust-baslik.
     initialize dokumer-rec
     set dokumer-20-baslik to true
     move "Karaliste Raporu" to dokumer-400
     write dokumer-rec, end-write

     initialize dokumer-rec
     set dokumer-20-baslik to true
     write dokumer-rec, end-write
     .
*
 dokumer-ozellikler.
     initialize dokumer-rec
     set dokumer-20-ozellik to true
     set Dokumer-Oto-Sayfa-Basi-Yap to true
     move 56  to dokumer-oto-sayfa-satir
     move "|" to dokumer-detay-kolon-ayirici
     move "LLLLLLLLLLLLLLLLLLLLLLLL" to dokumer-align-occ
     move dokumer-ozellikler-rec     to dokumer-400
     write dokumer-rec, end-write
     .
*
 dokumer-basliklar.
     initialize dokumer-rec detay
     set dokumer-20-detay to true
     move "1"          to dokumer-20-02
     move "Tipi"       to det-1
     move "Adi"        to det-2
     move "Soyadi"     to det-3
     move "Aciklama 1" to det-4
     move "Aciklama 2" to det-5
     move "Staf"       to det-6
     perform pipe-at
     move det-filler to dokumer-400
     write dokumer-rec, end-write

     initialize dokumer-rec detay
     set dokumer-20-detay to true
     set dokumer-20-ekran-yazma to true
     move "2"          to dokumer-20-02
     perform cizgi-cek
     move det-filler to dokumer-400
     write dokumer-rec, end-write
     .
*
 dokumer-satirlar.
     open input karalist
     initialize karalist-rec
     start karalist key >= karalist-anah
       invalid
         continue
       not invalid
         initialize fs-karalist
         perform loop-dokumer-karalist
     end-start
     .
*
 loop-dokumer-karalist.
     perform with test after
             until fs-doviz = "10"
       perform oku-dokumer-karalist
     end-perform
     .
*
 oku-dokumer-karalist.
     read karalist next no lock
       end
         move "10" to fs-doviz
       not at end
         perform ata-karalist-dokumer
     end-read
     .
*
 ata-karalist-dokumer.
     initialize dokumer-rec detay
     perform pipe-at
     evaluate karalist-tipi
       when "A" move "Acenta"   to det-1
       when "M" move "Musteri"  to det-1
       when "P" move "Personel" to det-1
     end-evaluate

*     move karalist-adi          to det-2               |firat karalist-adi uzatýldý  29/05/2020
     move karalist-adi          to det-2
     move karalist-adi-ek       to det-2(21:15)

     move karalist-soyadi       to det-3
     move karalist-ack-1        to det-4
     move karalist-ack-2        to det-5
     move karalist-staf         to det-6
     write dokumer-rec from detay, end-write
     .
*
 dokumer-finish.
     close dokumer
     call dokumer-prog
       on exception
         perform callerr-proc
       not on exception
         cancel dokumer-prog
     end-call
     close karalist
     delete file dokumer
     .
*
 pipe-at.
     move all "|" to fil-1
                     fil-2
                     fil-3
                     fil-4
                     fil-5
                     fil-6
     .
*
 cizgi-cek.
     move all "-" to det-1
                     det-2
                     det-3
                     det-4
                     det-5
                     det-6
     .
*
@}}

