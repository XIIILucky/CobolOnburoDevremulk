{{@PARAGRAPH:
*
 Form1-Bef-Create.
     perform adresleri-tasi
     open input genel
     move 1 to genel-anahtar
     read genel no lock
       invalid
         move 3 to mesaj-no
         perform mesaj-ver
         set exit-pushed to true
       not invalid
         move cost-sirketi to Kart-Dosya-Adres
     end-read
     close genel
     .
*
 Form1-Aft-Create.
     open input odalar 
     open i-o odalar2

*      close odalar2

      move kartara-oda to o-kisa 
            perform oda-uzat
            move o-uzun to kartara-oda-u
     move kartara-oda  to odalar-anah
    
     read odalar record no lock
       key is odalar-anah
       invalid
         display message box
           "Oda No tanimsiz" new-line
           "Lutfen Oda Seciniz"
           title   = "HATA"
           type    = MB-OK
           icon    = MB-ERROR-ICON
           default = MB-OK
         set exit-pushed to true
       not invalid
         initialize konuk-rec
         open input konuk
         move kartara-folio to konuk-folio
         read konuk record no lock
           key is konuk-anah
           invalid
             display message box
               "Folio tanimsiz" new-line
               "Lutfen folio Seciniz"
               title   = "HATA"
               type    = MB-OK
               icon    = MB-ERROR-ICON
               default = MB-OK
             set exit-pushed to true         not invalid
             perform kart-listele
         end-read
         close odalar
     end-read
     close konuk odalar2 
     .
*
 Form1-Aft-Initdata.
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         
     .
*
 Form1-Ex-Other.
     evaluate key-status
       when 13
         move lb-kart-no to kartara-kart
         set exit-pushed to true
     end-evaluate
     .
*
 kart-listele.
    modify lb-kartlar,
           reset-list  = 1
           mass-update = 1
    open input kart
    move kartara-folio(1:8) to kart-folio
                               konuk-anah
    read konuk record no lock
      key is konuk-anah
      invalid
        move all "*" to konuk-adi
                        konuk-soyadi
    end-read
    start kart key = kart-folio
      invalid
        display message box
          "Listelenecek Kart Bulunamadi" new-line
          title   = "Uyari"
          type    = MB-OK
          icon    = MB-WARNING-ICON
          default = MB-OK
        set exit-pushed to true
      not invalid
        initialize fs-kart
        perform with test after
                until fs-kart = "10"
          read kart next no lock
            end
              move "10" to fs-kart
            not at end
              if kart-folio <> kartara-folio(1:8)
                 move "10" to fs-kart
              else
                 initialize lb-value
                 move kart-no            to lb-kart-no
                 move kart-cin-gun       to egun
                 move kart-cin-ay        to eay
                 move kart-cin-yil       to eyil
                 move etarih             to lb-bas-tarih
                 move kart-cot-gun       to egun
                 move kart-cot-ay        to eay
                 move kart-cot-yil       to eyil
                 move etarih             to lb-bit-tarih
                 move konuk-adi          to lb-adi
                 move konuk-soyadi       to lb-soyadi
                 modify lb-kartlar,
                        item-to-add(lb-value)
              end-if
          end-read
        end-perform
    end-start
    close kart
    modify lb-kartlar,
           selection-index = 1
           mass-update = 0.
    .
*
@}}

