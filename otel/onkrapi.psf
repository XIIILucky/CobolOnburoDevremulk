                             if depkod2-alt-kate(1:2) = "01"
                                compute oda-toplam-bugun = oda-toplam-bugun + takas-tl-bugun
                             end-if 
                        end-read 


                    end-if,
                    if onk-ay = son-ay,
                       compute takas-tl-buay rounded = 
                               takas-tl-buay + (onkasa-tutar-tl - onkasa-corr-tutar-tl) * carpan,
                       compute takas-dv-buay rounded = 
                               takas-dv-buay + (onkasa-tutar-dv - hes-dv-tutar) * carpan,
                        initialize depkod2-rec 
                        move depkod-anah to depkod2-anah
                        read depkod2 no lock invalid 
                           continue 
                        not invalid 
                             if depkod2-alt-kate(1:2) = "01"
                                compute oda-toplam-buay = oda-toplam-buay + takas-tl-buay
                             end-if 
                        end-read 
                    end-if,
                    if onk-yil = son-yil 
                       compute takas-tl-buyil rounded = 
                               takas-tl-buyil + (onkasa-tutar-tl - onkasa-corr-tutar-tl) * carpan
                       compute takas-dv-buyil rounded = 
                               takas-dv-buyil + (onkasa-tutar-dv - hes-dv-tutar) * carpan

                        initialize depkod2-rec 
                        move depkod-anah to depkod2-anah
                        read depkod2 no lock invalid 
                           continue 
                        not invalid 
                             if depkod2-alt-kate(1:2) = "01"
                                compute oda-toplam-buyil = oda-toplam-buyil + takas-tl-buyil
                             end-if 
                        end-read 

                    end-if,
                    rewrite takas-rec invalid write takas-rec,end-rewrite,
                   end-if,
                end-if,
            end-read,
        end-perform,
    end-start.


