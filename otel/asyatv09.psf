{{@PARAGRAPH:
*
 Form1-Bef-Create. 
*      set environment "extra-keys-ok" to 1.
      call "c$narg" using link-var 
   
       perform adresleri-tasi 
       move isyeri-adres-tasi     to
         esgenel-dosya-adres     esgenelfis-dosya-adres estakvim-dosya-adres
         eskonum-dosya-adres     esodalar-dosya-adres   esulke-dosya-adres
         esbanka-dosya-adres     esdoviz-dosya-adres    esdagilim-dosya-adres
         esacenta-dosya-adres    esacenhrk-dosya-adres  eskontrat-dosya-adres
         esfiyatana-dosya-adres  esfiyat-dosya-adres    esaksiyon-dosya-adres
         esaksiyhrk-dosya-adres  esbilbord-dosya-adres  esrez-dosya-adres
         escast-dosya-adres      esturizm-dosya-adres   esistdata-dosya-adres
         eskonuk-dosya-adres     esozluk-dosya-adres    esuzatma-dosya-adres
         esodadegis-dosya-adres  esdepkod-dosya-adres   espostkod-dosya-adres
         esextpost-dosya-adres   esromhrk-dosya-adres   esexthrk-dosya-adres
         essilhrk-dosya-adres    escektr-dosya-adres    esfatura-dosya-adres
         esacenfat-dosya-adres   esonkasa-dosya-adres   eskur-dosya-adres
         esdov-boz-dosya-adres   esonavans-dosya-adres  eskodlar02-dosya-adres
         esteknik-dosya-adres    eshalk-dosya-adres     esdetist-dosya-adres
         escevap1-dosya-adres    escevap2-dosya-adres   essoru-dosya-adres
         estelkod-dosya-adres    esteldata-dosya-adres  estelkim-dosya-adres
         estel-err-dosya-adres   eskategori-dosya-adres esdepozit-dosya-adres
         esajanda-dosya-adres    esfihrist-dosya-adres  essantral-dosya-adres
         eskaralist-dosya-adres  espolisxml-dosya-adres esuyruk-dosya-adres
         esgrup-dosya-adres      
         esreztop-dosya-adres    escasttop-dosya-adres  estakvimtop-dosya-adres
         esistdatatop-dosya-adres esdetisttop-dosya-adres  esturizmtop-dosya-adres
         esyanrez-dosya-adres     esonkasar-dosya-adres   esozluk2-dosya-adres
         estakvim2-dosya-adres    esnot-dosya-adres esaksiyon-dosya-adres
         esaksiyhrk-dosya-adres.
       
                           
*
 before-procedure. 
           continue
             .

 Form1-Aft-Initdata. 
    open input genel
    move 1 to genel-anahtar
    read genel no lock invalid
          initialize mesaj-link
          move 03 to mesaj-no
          perform mesaj-ver
    not invalid
        continue
    end-read
    close genel.
    
*      
 exception-procedure. 
     evaluate key-status
          when 2 
             modify Form1-Ef-1, enabled = false
              if rate-sil =  1 
                  open i-o acenta
                          initialize acenta-rec
                          start acenta key not < acenta-anahtar invalid
                                  continue
                          not invalid
                          perform until fs-acenta = "10"
                          read acenta next no lock end move "10" to fs-acenta
                          not end
                                 if acenta-rate-acenta = "0000"   
                                    initialize acenta-rate-acenta 
                                 end-if 

                                 move acenta-il(17:4) to acenta-rate-acenta
                                 initialize acenta-extra-kdv-yok
                                 rewrite acenta-rec invalid
                                         continue
                                 end-rewrite
                                 display acenta-rec
                          end-read
                          end-perform
                          end-start
                  close acenta
 

                  display message box "Bitti"
                   exit paragraph 
            end-if
             


               if cb-karaliste = 1
                 open i-o acenta
                          initialize acenta-rec
                          start acenta key not < acenta-anahtar invalid
                                  continue
                          not invalid
                          perform until fs-acenta = "10"
                          read acenta next no lock end move "10" to fs-acenta
                          not end
                                 if acenta-kara-liste not = 0
                                    initialize acenta-kara-liste
                                    rewrite acenta-rec invalid
                                            continue
                                    end-rewrite
                                    display acenta-rec
                                 end-if
                          end-read
                          end-perform
                          end-start
                  close acenta
                  display message box "Bitti"
                   exit paragraph                  
               end-if 
                  perform  sirk-copyala   
                  perform islem-yap 
         when 5 
             CONTinue
 
         end-evaluate. 




*
 sirk-copyala. 
     display lb-isl-durumu
     accept system-information   from system-info
     accept terminal-abilities   from terminal-info
        
        move isyeri-adres-tasi        to copya2-adres copya-adres 
                                         paste2-adres paste-adres 
                                         sir1-adres sir2-adres
        if eskiden not = 1 then 
        if operating-system = "Unix"
            call "system" using klasac-unix giving sonuc
            perform varying k from 1 by 1 until k > 20 or dosyalar(k) = spaces
              move dosyalar(k) to kopya-dosya   kopya2-dosya
              call "system" using kopyala-unix giving sonuc
*              if sonuc = 1 then  
*                display message box kopyala-unix
*               end-if
            end-perform
         else
             
           call "system" using klasac-dos giving sonuc
           if sonuc not = 1 then  
                display message box klasac-dos
               end-if
           perform varying k from 1 by 1 until k > 20 or dosyalar(k) = spaces
              move dosyalar(k) to kopya-dosya   kopya2-dosya 
               call "system" using kopyala-dos giving sonuc
               if sonuc not = 1 then  
                display message box kopyala-dos
               end-if
           end-perform
           
        end-if
        end-if
        display message box "Transfer basliyor" .
           

*
 islem-yap. 
          if eskiden not = 1
            if cb-konuk = 1  
              perform Konuk-Tr
            end-if 
            if cb-cast = 1
              perform cast-tr
            end-if 
            if cb-rez = 1
              perform rez-tr
            end-if
            if cb-not = 1
                perform not-tr
            end-if 
            if cb-aksiyon = 1
               perform aksiyon-tr
               perform aksiyhrk-tr
            end-if 
            if cb-takip = 1
             display message box "Transfer Butonuna basarak bekleyiniz.."
                             title "Uyari"
                             icon 1
             call "/asya/ytl/obj/otel/lostgd.asy"  
                  on exception 
                     perform callerr-proc
                  not on exception
                     cancel "/asya/ytl/obj/otel/lostgd.asy"
             end-call
            end-if 
         end-if
        display message box "Islem Bitti".  
 
*
 aksiyhrk-tr. 
       open input esaksiyhrk

       initialize esaksiyhrk-rec
       start  esaksiyhrk key > esaksiyhrk-anah
           invalid 
              set environment "V-VERSION" to 5
              open output aksiyhrk
              close aksiyhrk
           not invalid
              set environment "V-VERSION" to 5
              open output aksiyhrk
              close aksiyhrk
           open i-o aksiyhrk
           perform until fs-esaksiyhrk = "10"
              read esaksiyhrk next 
                  end move "10" to fs-esaksiyhrk
                  not end
                   initialize aksiyhrk-rec
                   perform aksiyhrk-tr-tek
              end-read
           end-perform
           close aksiyhrk
       end-start

       close esaksiyhrk
       set environment "V-VERSION" to 3.
*
 aksiyhrk-tr-tek.
       move esaksiyhrk-rec   to aksiyhrk-rec
       write aksiyhrk-rec invalid 
            display message box "aksiyhrk tr hata"
       end-write 
       .
      
*
 aksiyon-tr. 
       open input esaksiyon

       initialize esaksiyon-rec
       start  esaksiyon key > esaksiyon-anah
           invalid 
              open output aksiyon
              close aksiyon
           not invalid
              open output aksiyon
              close aksiyon
           open i-o aksiyon
           perform until fs-esaksiyon = "10"
              read esaksiyon next 
                  end move "10" to fs-esaksiyon
                  not end
                   initialize aksiyon-rec
                   perform aksiyon-tr-tek
              end-read
           end-perform
           close aksiyon
       end-start

       close esaksiyon.
*
 aksiyon-tr-tek.
       move esAKSIYON-TIPI         to AKSIYON-TIPI         
