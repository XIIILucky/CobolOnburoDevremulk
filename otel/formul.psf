                 when 5
                    move "AKSIYON 1"           to lb-2-col-2
                 when 6
                    move "AKSIYON 2"           to lb-2-col-2
                 when 7
                    move "SPECIAL OFFER 1"     to lb-2-col-2
                 when 8
                    move "SPECIAL OFFER 2"     to lb-2-col-2
                 when 9
                    move "KATALOG KATKI PAYI"  to lb-2-col-2
                 when 10 
