*
 operator-grid-yukle.
     open i-o esletab 
     modify form1-gd-1, reset-grid  = 1 
                        mass-update = 1
     initialize form1-gd-1-record
     move "Kodu"           to gd-1-col-1
     move "Operator Adi"   to gd-1-col-2
     modify form1-gd-1, record-to-add(form1-gd-1-record)
     initialize esletab-rec 
     move 2 to i
     move "OP" to esletab-tipi 
     start esletab key not < esletab-anah invalid
           continue
     not invalid
     perform with test after until fs-esletab = "10"
     read esletab next no lock end move "10" to fs-esletab
     not at end 
         if esletab-tipi <> "OP"
            exit perform 
         end-if
         initialize form1-gd-1-record
         move esletab-kodu             to gd-1-col-1
         move esletab-adi              to gd-1-col-2
         modify form1-gd-1, record-to-add (form1-gd-1-record)
         modify form1-gd-1(i,1), hidden-data = esletab-anah
         add 1 to i
     end-read 
     end-perform 
     end-start
     modify form1-gd-1, mass-update = 0
     close esletab
     .
