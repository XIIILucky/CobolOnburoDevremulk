{{@PARAGRAPH:
*
 Form1-Bef-Create.
     perform adresleri-tasi.
*/
     open input genel.
     move 1 to genel-anahtar
     read genel no lock invalid
          initialize mesaj-link
          move 17 to mesaj-no
          perform mesaj-ver
          goback
     end-read
     close genel.
     .
*
 exception-procedure.
     evaluate key-status
         when 1 
            continue   
         when 2 
         perform kaydet-kontrol
         if kayit-hata-var = 1 
            exit paragraph
         end-if
         open i-o merkod esletab
         initialize merkod-rec esletab-rec
         move esleme-tipi(1:2) to merkod-tipi
         if esleme-tipi(1:2) = "KN" or "OP" 
            move esleme-tipi(1:2) to esletab-tipi
         end-if
         evaluate merkod-tipi
             when "AC"                       
             move esleme-kodu-4 to merkod-ace-anah-kodu
             read merkod no lock invalid continue end-read 
             move esleme-adi    to merkod-ace-dig-adi
             write merkod-rec invalid
                   rewrite merkod-rec end-rewrite 
             end-write 

             when "DP"
             move esleme-kodu-3 to merkod-dep-anah-kodu
             read merkod no lock invalid continue end-read 
             move esleme-adi    to merkod-dep-dig-adi
             write merkod-rec invalid
                   rewrite merkod-rec end-rewrite 
             end-write 

             when "SK"
             move esleme-sira   to merkod-sir-anah-sira
             move esleme-kodu-8 to merkod-sir-anah-kodu
             read merkod no lock invalid continue end-read 
             move esleme-adi    to merkod-sir-dig-adi
             move ip-adr        to merkod-sir-dig-ip
             move local-sirket  to merkod-sir-dig-local
             write merkod-rec invalid
                   rewrite merkod-rec end-rewrite 
             end-write 
         end-evaluate

         evaluate esletab-tipi

             when "KN"                       
             move esleme-kodu-80 to esletab-kodu
             read esletab no lock invalid continue end-read 
             move asya-kodu-2   to esletab-kon-kodu
             move esleme-adi    to esletab-adi
             write esletab-rec invalid
                   rewrite esletab-rec end-rewrite 
             end-write 

             when "OP"                       
             move esleme-kodu-80 to esletab-kodu
             read esletab no lock invalid continue end-read 
             move asya-kodu-4   to esletab-ope-kodu
             move esleme-adi    to esletab-adi
             write esletab-rec invalid
                   rewrite esletab-rec end-rewrite 
             end-write 
         end-evaluate 

         close merkod esletab
         display message box "Kayit islemi tamamlandi.."
         perform cm-1301-Ex-Ntf-Selchange

         when 3 
         perform sil-kontrol
         if sil-hata-var = 1 
            exit paragraph
         end-if
         display message box "Kayit Silinecektir.." new-line
                             "Emin Misiniz?"
                         title "Uyari"
                         icon 1
                         type 2 
                         default 2
                         returning return-code 
         if return-code = 2
             exit paragraph 
         end-if 
         open i-o merkod esletab
         initialize merkod-rec esletab-rec
         move esleme-tipi(1:2) to merkod-tipi
         if esleme-tipi(1:2) = "KN" or "OP" 
            move esleme-tipi(1:2) to esletab-tipi
         end-if
         evaluate esleme-tipi(1:2)
             when "AC"
                  move esleme-kodu-4 to merkod-ace-anah-kodu
                  start merkod key not < merkod-anah invalid
                        continue
                  not invalid
                  perform with test after until fs-merkod = "10"
                  read merkod next no lock end move "10" to fs-merkod
                  not at end 
                      if merkod-tipi <> "AC" or merkod-ace-anah-kodu <> esleme-kodu-4 
                         exit perform 
                      end-if
                      delete merkod end-delete 
                  end-read 
                  end-perform 
                  end-start
             when "DP"
                  move esleme-kodu-3 to merkod-dep-anah-kodu
                  start merkod key not < merkod-anah invalid
                        continue
                  not invalid
                  perform with test after until fs-merkod = "10"
                  read merkod next no lock end move "10" to fs-merkod
                  not at end 
                      if merkod-tipi <> "DP" or merkod-dep-anah-kodu <> esleme-kodu-3 
                         exit perform 
                      end-if
                      delete merkod end-delete 
                  end-read 
                  end-perform 
                  end-start
             when "KN" 
                  move esleme-kodu-80 to esletab-kodu
                  read esletab no lock invalid
                       continue
                  not invalid
                      delete esletab end-delete 
                  end-read 
             when "OP" 
                  move esleme-kodu-80 to esletab-kodu
                  read esletab no lock invalid
                       continue
                  not invalid
                      delete esletab end-delete 
                  end-read 
             when "SK" 
                  move esleme-sira   to merkod-sir-anah-sira
                  move esleme-kodu-8 to merkod-sir-anah-kodu
                  read merkod no lock invalid
                       continue
                  not invalid
                      delete merkod end-delete 
                  end-read 
         end-evaluate 
         close merkod esletab
         display message box "Silme islemi tamamlandi.."
         perform cm-1301-Ex-Ntf-Selchange
         when 4
         initialize detay-yok
         if esleme-tipi(1:2) <> "AC" 
            display message box "Rapor yalnizca acenta kodlamalarini verecektir."
                          title "Bilgi.."
         end-if
         display message box "Raporda acenta detayi gosterilsin mi?" new-line
                       title "Rapor Detay"
                        icon 1
                        type 2 
                     default 2
                   returning return-code 
         if return-code = 2
            move 1 to detay-yok 
         end-if 
         perform raporla
     end-evaluate.
*
 Form1-Aft-Initdata.
     perform cm-1301-Ex-Ntf-Selchange
     perform data-bakim
     .
*
 data-bakim.
     open i-o esletab
     initialize esletab-rec 
     move "OP" to esletab-tipi 
     start esletab key not < esletab-anah invalid
           continue
     not invalid
     perform with test after until fs-esletab = "10" 
     read esletab next no lock end move "10" to fs-esletab 
     not at end 
         
         if esletab-tipi <> "OP" 
            exit perform 
         end-if

         if esletab-kodu = spaces 
            delete esletab end-delete 
         end-if

     end-read 
     end-perform
     end-start 
     initialize esletab-rec 
     move "KN" to esletab-tipi 
     start esletab key not < esletab-anah invalid
           continue
     not invalid
     perform with test after until fs-esletab = "10" 
     read esletab next no lock end move "10" to fs-esletab 
     not at end 
         
         if esletab-tipi <> "KN" 
            exit perform 
         end-if

         if esletab-kodu = spaces 
            delete esletab end-delete 
         end-if

     end-read 
     end-perform
     end-start 
     close esletab
     .
*
 Raporla.
     open i-o genelfis
     move 1 to genelfis-anahtar
     read genelfis no lock invalid
          accept print-no from time
     not invalid
          add 1 to print-no
          rewrite genelfis-rec end-rewrite
     end-read
     close genelfis
     initialize dokumer-rec dokumer-anah dokumer-ozellikler-rec
                dokumer-dosya

     set  dokumer-asya-set to true
     move print-no         to dokumer-dosya-adi
     open output dokumer

*/WINDOW TITLE
     initialize dokumer-rec detay
     move "W"                    to det-dokumer-20(1:)
     move "Tanimli Acenta Eslesme Raporu" to det-filler
     write dokumer-rec from detay
*/ BASLIKLAR
     initialize dokumer-rec detay
     move "B"                    to det-dokumer-20(1:)
     move "Tanimli Acenta Eslesme Raporu" to det-filler
     write dokumer-rec from detay
     initialize dokumer-rec detay
     move "B"                    to det-dokumer-20(1:)
     write dokumer-rec from detay

*/ DOKUMER OZELLIKLER-REC
     initialize dokumer-rec detay
     move "O"                    to det-dokumer-20(1:)
     move "E" to dokumer-oto-sayfa-basi
     move 56  to dokumer-oto-sayfa-satir
     move "|" to dokumer-detay-kolon-ayirici
     move "LLLLLLLLLLLL" to dokumer-align-occ
     move dokumer-ozellikler-rec     to det-filler
     write dokumer-rec from detay
*/ BASLIKLAR 
     initialize dokumer-rec detay
     move "D1"                to det-dokumer-20(1:) 
     move "AnaK"              to det-1
     move "Ana Acenta Adi"    to det-2
     move "Sirket K"          to det-3
     move "Sirket Adi"        to det-4
     move "AceK"              to det-5
     move "Sirket Acenta Adi" to det-6

     move all "|" to fil-1 fil-2 fil-3 fil-4 fil-5 fil-6
     write dokumer-rec from detay

     initialize dokumer-rec detay
     move "D2"         to det-dokumer-20(1:)     
     move "-"          to det-dokumer-20(5:1)
     move all "-" to det-1 det-2 det-3 det-4 det-5 det-6
     write dokumer-rec from detay
     open i-o merkod mer2 

     initialize merkod-rec 
     move "AC" to merkod-tipi
     start merkod key not < merkod-anah invalid
           continue
     not invalid
     perform with test after until fs-merkod = "10"
     read merkod next no lock end move "10" to fs-merkod
     not at end
 
         if "AC" <> merkod-tipi
            exit perform 
         end-if
         if detay-yok = 1 and 
            merkod-ace-anah-sirket <> spaces
            exit perform cycle
         end-if
         initialize dokumer-rec detay 
         if merkod-ace-anah-sirket = spaces 
            move merkod-ace-anah-kodu to det-1
            move merkod-ace-dig-adi   to det-2
         else
            move merkod-anah to xmerkod-anah
            initialize mer2-rec 
            move "SK"              to mer2-tipi
            start mer2 key not < mer2-anah invalid
                  continue
            not invalid
            perform with test after until fs-mer2 = "10"
            read mer2 next no lock end move "10" to fs-mer2
            not at end 
                if mer2-tipi <> "SK"
                   exit perform 
                end-if
                if mer2-sir-anah-kodu <> xmerkod-anah(7:8)
                   move all "*" to mer2-sir-dig-adi
                   exit perform cycle
                else
                   exit perform 
                end-if
            end-read 
            end-perform 
            end-start

            move merkod-ace-anah-sira to z-3
            move z-3                  to det-1
            move mer2-sir-anah-kodu   to det-3
            move mer2-sir-dig-adi     to det-4

            initialize mer2-rec 
            move xmerkod-anah to mer2-anah
            read mer2 no lock invalid
                 continue
            end-read 

            move mer2-ace-dig-kodu to det-5
            move mer2-ace-dig-adi  to det-6

         end-if
         write dokumer-rec from detay


     end-read 
     end-perform 
     end-start

     initialize dokumer-rec detay
     move "-"          to det-dokumer-20(5:1)
     move all "-"      to det-1 det-2 det-3 det-4 det-5 det-6
     write dokumer-rec from detay

     close dokumer merkod mer2
     call dokumer-prog on exception
          perform callerr-proc
     not on exception
          cancel dokumer-prog
     end-call
     delete file dokumer.

