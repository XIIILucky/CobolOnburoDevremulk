                            move takas6-beb(7)     to zpax2,
                            move zpax2             to det-1(sutun(yy + 6) :8)

                          
                            compute t-kisi = takas6-pax2(7) + takas6-chi(7) + takas6-fre(7) + takas6-beb(7)
                            move t-kisi         to zpax2
                            move zpax2          to det-1(sutun(yy + 7) :14)

                            move " "            to det-1(sutun(yy + 8) :6)       | firat doluluk ekleme

                         else

                            compute yy =  7 * (xx - 1) | compute yy =  6 * (xx - 1)       firat doluluk duzeltme

                            move takas6-oda(xx)    to z4,
                            move z4          to det-1(sutun(yy + 2):6)
                            compute docc rounded =
