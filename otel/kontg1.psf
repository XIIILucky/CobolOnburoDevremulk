 bef-procedure.
     initialize mesaj.
     evaluate control-id
          when 4
              evaluate true
                  when turkce
                       move "Acenta Kodunu Seciniz ..." to  mesaj
                  when ingilizce
                       move "Acenta Kodunu Seciniz ..." to  mesaj
                  when almanca
                       move "Acenta Kodunu Seciniz ..." to  mesaj
              end-evaluate
          when 9
          when 10
          when 11
              evaluate true
                  when turkce
                       move "Rapor Baslangic Tarihini Giriniz ... " to mesaj
                  when ingilizce
                       move "Rapor Baslangic Tarihini Giriniz ... " to mesaj
                  when almanca
                       move "Rapor Baslangic Tarihini Giriniz ... " to mesaj
              end-evaluate
          when 12
          when 13
          when 14
              evaluate true
                  when turkce
                       move "Rapor Bitis Tarihini Giriniz ... " to mesaj
                  when ingilizce
                       move "Rapor Bitis Tarihini Giriniz ... " to mesaj
                  when almanca
                       move "Rapor Bitis Tarihini Giriniz ... " to mesaj
              end-evaluate

          when 8
              evaluate true
                  when turkce
                       move "Rapor Detay Tipini Seciniz" to mesaj
                  when ingilizce
                       move "Rapor Detay Tipini Seciniz" to mesaj
                  when almanca
                       move "Rapor Detay Tipini Seciniz" to mesaj
              end-evaluate

     end-evaluate.

     modify form1-st-1-handle,
            panel-index = 1
            panel-text = mesaj.

 bekleyiniz-goster.
     add 1 to bekleyiniz-ii.
     if bekleyiniz-ii > 50 
        continue
     else
        exit paragraph
     end-if.
     initialize bekleyiniz-ii.

     add 1 to bekleyiniz-i.
     if bekleyiniz-i > 22
        display status-bar,
                panel-index = 1
                panel-text  = " "
        move zeroes to bekleyiniz-i
     else
        display status-bar
                panel-index = 1
                panel-text  = bekleyiniz(1:bekleyiniz-i)
                style = 3
     end-if.
