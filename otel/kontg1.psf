{{@PARAGRAPH:
*
 Form1-Bef-Create.
     perform adresleri-tasi.
     .
*
 Form1-Aft-Initdata.
     modify combo-02,
            reset-list  = 1
            mass-update = 1


     modify combo-02,
            item-to-add("0-Detaysiz","1-Oda Tipi Detayi")

     modify combo-02,
            mass-update = 0
            value "0-Detaysiz".

     move gun-tasi        to bas-gun   
     move  ay-tasi        to bas-ay    
     move yil-tasi        to bas-yil   
     move gun-tasi        to bit-gun   
     move  ay-tasi        to bit-ay    
     move yil-tasi        to bit-yil   
     display accept-01,accept-02,accept-03,accept-04
     accept-05,accept-06.
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         
*
 Form1-Ex-Other.
     if key-status = 1 and
     (control-id = 4 or 20)
        initialize acenta-cagir
        call "/asya/ytl/obj/otel/acenara.asy" using acenta-cagir
        on exception 
            perform callerr-proc
        not on exception
        cancel "/asya/ytl/obj/otel/acenara.asy"
        end-call
        if acenta-cagir <> spaces
           move acenta-cagir   to bas-acenta
           display acc-1
        end-if
        move 4 to accept-control
        move 4 to control-id
        exit paragraph
     end-if.

     if key-status = 1001 
        continue
     else 
        exit paragraph
     end-if.

     open input takvim
     move baslangic-tarihi    to takvim-anah
     read takvim no lock invalid
          display omitted beep
          move 4 to accept-control
          move 9 to control-id
          close takvim
          initialize mesaj-link
          move 13 to mesaj-no
          perform mesaj-ver
          exit paragraph
     move bitis-tarihi        to takvim-anah
     read takvim no lock invalid
          display omitted beep
          move 4 to accept-control
          move 12 to control-id
          close takvim
          initialize mesaj-link
          move 13 to mesaj-no
          perform mesaj-ver
          exit paragraph
     end-read.
     close takvim.
     if baslangic-tarihi > bitis-tarihi
        display omitted beep
        move 4 to accept-control
        move 9 to control-id
        initialize mesaj-link
        move 14 to mesaj-no
        perform mesaj-ver
        exit paragraph.

     open input acenta
     move bas-acenta to acenta-kodu
     read acenta no lock invalid
          display omitted beep
          move 4 to accept-control
          move 4 to control-id
          exit paragraph
          close acenta
     end-read
