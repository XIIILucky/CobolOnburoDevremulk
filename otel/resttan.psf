{{@PARAGRAPH:
*
 Form1-Bef-Create.
     perform adresleri-tasi.
     .
*
 accept-0101-Bef-Procedure.
     initialize kodlar02-rec.
     display accept-0102,accept-0102,accept-0103 accept-0103a.
     modify button-0101,enabled = false
     modify button-0102,enabled = false.
*     modify acc-cb1, enabled = false
     perform bef-procedure.
     .
*
 accept-0101-Aft-Procedure.
     if kodlar02-kodu = spaces 
        modify button-0101,enabled = false
        modify button-0102,enabled = false
     else 
        modify button-0101,enabled = true
        modify button-0102,enabled = true
     end-if.
     open input kodlar02.
     move "Q" to kodlar02-tipi.
     read kodlar02 no lock invalid
          initialize kodlar02-adi ode-indirim ode-posting
          move "A" to ode-tipi 
          move "Q" to kodlar02-tipi
     end-read
     if kodlar02-alkol = "1" 
        move 1 to cb-1
     else 
        move 0 to cb-1
     end-if
     move ode-bos        to sabah
     move hou-bos        to ogle
     move kodlar02-bos   to aksam
     move kahvalti-dep   to ucret
     display accept-0102,accept-0103,combo-0101, accept-0103a acc-cb1
             acc1 acc2 acc3 acc4 acc5 acc6 acc7.
*     if ode-tipi = "D" or "E" or "F" or "O"
*        modify acc-cb1,enabled = true
*     else
*        modify acc-cb1,enabled = false
*     end-if
     close kodlar02.
     perform listbox-yer-bul.
     if i > zeroes
        modify form1-lb-1,
               selection-index = i
     end-if.
     .
*
 exception-procedure.
     evaluate key-status
         when 1
         initialize odeme-cagir
         move "D" to odeme-tipi-cagir
         call "/asya/ytl/obj/otel/restara.asy" using odeme-cagir
            on exception perform callerr-proc
               not on exception
               cancel "/asya/ytl/obj/otel/restara.asy" 
         end-call
         move odeme-kodu-cagir     to kodlar02-kodu
              display accept-0101
              move 4 to accept-control
              move 5 to control-id
         when 2 
*              if ode-indirim > 100
*                 move 4 to accept-control
*                 move 12 to control-id
*                 exit paragraph
*              end-if
              open i-o kodlar02
              move "Q" to kodlar02-tipi
              move cb-1         to kodlar02-alkol
              move sabah            to ode-bos
              move ogle             to hou-bos
              move aksam            to kodlar02-bos
              move ucret            to kahvalti-dep
              write kodlar02-rec invalid 
                    rewrite kodlar02-rec 
                    end-rewrite 
              end-write
              close kodlar02
              initialize cb-1 s o a1 s2 o2 a2 ucret
              display acc1 acc2 acc3 acc4 acc5 acc6 acc7 
              perform listbox-yer-bul
              if i > zeroes
                   initialize form1-lb-1-value
                   modify form1-lb-1,
                          item-to-delete(i)
                   move kodlar02-kodu to lb-1-col-1
                   move kodlar02-adi  to lb-1-col-2
                   modify form1-lb-1,
                          item-to-add(form1-lb-1-value)
              else
                 initialize form1-lb-1-value
                 move kodlar02-kodu to lb-1-col-1
                 move kodlar02-adi  to lb-1-col-2
                 modify form1-lb-1,
                        item-to-add(form1-lb-1-value)
              end-if
              move 4 to accept-control
              move 5 to control-id
         when 3
              open i-o kodlar02
              move "Q" to kodlar02-tipi
              delete kodlar02 invalid continue 
              end-delete
              close kodlar02
              perform listbox-yer-bul
              if i > zeroes
                 modify form1-lb-1,
                        item-to-delete(i)
              end-if
              move 4 to accept-control
              move 5 to control-id
              initialize s o a1 s2 o2 a2 ucret 
              display acc1 acc2 acc3 acc4 acc5 acc6 acc7 
        when 4
              open input kodlar02
              initialize kodlar02-rec
              move "Q" to kodlar02-tipi
              start kodlar02 key not < kodlar02-anah invalid
                    initialize mesaj-link
                    move 04 to mesaj-no
                    perform mesaj-ver
                    close kodlar02
                    exit paragraph
              end-start

              open i-o genelfis
              move 1 to genelfis-anahtar
              read genelfis no lock invalid
                   accept print-no from time
              not invalid
                   add 1 to print-no
                   rewrite genelfis-rec end-rewrite
              end-read
              close genelfis
