     if buffer-xxxx = spaces   and kontrat-eksi not = "-"
         move 9999 to buffer-xxx
     end-if
     move buffer-xxx to kontrat-oda(41)
     
     
     move mmmmmmmm          to kontrat-bekle-gunu
     move k-kodu-tasi       to kontrat-staf
