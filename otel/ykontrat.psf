        inquire form1-gd-2( i + 2 , 2 ) cell-data
                               in buffer-xxxx
        if buffer-xxxx = spaces     and kontrat-eksi not = "-"
           move 9999 to buffer-xxx
        end-if
        move buffer-xxx to     kontrat-oda(i)
