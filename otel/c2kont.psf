{{@PARAGRAPH:

*
 Form1-Bef-Create.
      call "c$narg" using link-var.
*      move "ASYA"   to k-kodu-tasi.
*      move "voyage07"  to isyeri-adres-tasi.
*      move 20070827    to tarih-tasi.
      perform adresleri-tasi.
     .
*
 before-procedure.
     initialize mesaj.
     evaluate control-id
          when 11
              evaluate true
                  when turkce
                       move "Islemi Baslat  "   to mesaj
                  when ingilizce 
                       move "Islemi Baslat  "   to mesaj
                  when almanca
                       move "Islemi Baslat  "   to mesaj
              end-evaluate
     end-evaluate.

     modify form1-st-1-handle,
            panel-index = 1 
            panel-text = mesaj.

 Form1-Aft-Initdata.
    
     .
    open input genel
    move 1 to genel-anahtar
    read genel no lock invalid
          initialize mesaj-link
          move 03 to mesaj-no
          perform mesaj-ver
    not invalid
        continue
    end-read
    close genel
    move 1 to rezpara-trace
    open input konum
     initialize konum-rec
     start konum key not < konum-anahtar invalid
           continue
     not invalid
     perform  with test after until fs-konum = "10"
           read konum next no lock end move "10" to fs-konum
           not at end
                if konum-anahtar > 0 and konum-anahtar not > 19
                   move konum-anahtar to son-konum
                end-if
           end-read
     end-perform
     close konum.
    
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.

     .
 exception-procedure.
     evaluate key-status
           when 8
               
           when 2
           when 6
               move 0 to hata-var
                 move k-kodu-tasi to takasu-k
                 open output takasu
                 close takasu
                 open i-o takasu
                 open input     takvim
                 open i-o   fiyat  fiyatana 
                 perform fiyat-isle thru fiyat-isle-exit
                 close fiyat fiyatana takvim
                
                 if hata-var not = 0 then
                    perform hata-goster
                    else
                    display message box "Tum Fiyat tablolari kontrol edildi"

                end-if
                 close takasu
                  delete file  takasu
         end-evaluate.
*
 hata-goster.
            
              initialize takasu-rec
             
              start takasu key not < takasu-anah invalid
                    continue
              end-start

              open i-o genelfis
              move 1 to genelfis-anahtar
              read genelfis no lock invalid
                   accept print-no from time
              not invalid
                   add 1 to print-no
                   rewrite genelfis-rec end-rewrite
              end-read
              close genelfis
