 takas-dosya-yaz.  
    initialize tum-oda.  
    perform takas-dosya-ac.  
    open input takvim.  
    initialize takvim-rec.  
    move ilk-tarih to takvim-anah.  
    start takvim key not < takvim-anah invalid close takvim go takas-dosya-yaz-exit,  
          not invalid,  
          open input rez cast konuk kodlar02 romhrk,  
  
          move spaces to evet-hayir,  
          perform with test after until evet,  
              read takvim next no lock end move "E" to evet-hayir,  
                   not end,  
                   if takvim-anah > son-tarih,  
                         move "E" to evet-hayir,  
                       else,  
                         if takvim-anah not > son-tarih,  
                            perform cast-oku thru cast-oku-exit,  
                         end-if,  
                   end-if,  
              end-read,  
          end-perform,  
    end-start.  
    close takvim rez cast konuk kodlar02 romhrk. 
 takas-dosya-yaz-exit.  
    exit.  
 cast-oku.  
    initialize cast-rec.  
    move takvim-anah to cast-tarih.  
    start cast key not < cast-tarih invalid continue,  
        not invalid,  
  
        move spaces to var-yok,  
        perform with test after until var,  
            read cast next no lock end move "V" to var-yok,  
                 not end,  
  
                 if cast-tarih > takvim-anah move "V" to var-yok,  
                     else,  
                     if cast-tarih not > takvim-anah,  
                        initialize rez-rec,  
                        move cast-rez-no to rez-no,  
                        read rez no lock invalid continue,  
                                     not invalid perform takas-kaydet thru takas-kaydet-exit,  
                        end-read,  
                     end-if,  
                 end-if,  
            end-read,  
        end-perform,  
    end-start.  
 cast-oku-exit.  
    exit.  
  
 takas-kaydet.  
*********if takvim-anah > rez-cik-tar go takas-kaydet-exit.  
         if rez-durumu  = "S"         go takas-kaydet-exit.  
         if rez-cik-tar = takvim-anah go takas-kaydet-exit.  
         if rap-tip = "G"
            move rez-folio      to konuk-folio
            read konuk no lock invalid go takas-kaydet-exit
            end-read,
         end-if 
*   TRACE Uygulamasi Acik ise kisi sayilari cast'tan alinacak
*    if rezpara-trace = 1
*       if rez-kisi not = cast-kisi
*          move cast-kisi to rez-kisi konuk-kisi
*       end-if


    move 1 to eklenecek-oda
    if rezpara-trace = 1
          if rez-kisi not = cast-kisi
            move cast-kisi to rez-kisi konuk-kisi
          end-if
       if rez-pan-tipi not = cast-pan-tipi
          move cast-pan-tipi to rez-pan-tipi konuk-pan-tipi
       end-if
          move cast-share to rez-share 
          if rez-share = 1 then 
             move 0 to eklenecek-oda 
            else
              move 1 to eklenecek-oda
          end-if
      else 
          move 1 to eklenecek-oda
    end-if    


 
   
*
         if rap-tip = "B"
            initialize konuk-rec
            move rez-acenta      to konuk-acenta
            move rez-pan-tipi    to konuk-pan-tipi
            move rez-odeme-tipi  to konuk-odeme-tipi
            move rez-kisi        to konuk-kisi 
         end-if. 
    if acn-kod not = spaces and konuk-acenta not = acn-kod,  
                                    go takas-kaydet-exit.  
    if pan-kod not = spaces and konuk-pan-tipi not = pan-kod,  
                                    go takas-kaydet-exit.  

    move "B" to kodlar02-tipi.  
