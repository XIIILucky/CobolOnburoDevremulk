        initialize arabul-degiskenler
        set arabul-yeni-arama         to true
        exit paragraph
     end-if.

     set arabul-tekrar-arama to true
     inquire form2-gd-1,
             x in arabul-x,
             y in arabul-y,
     
     modify form2-gd-1,
            cursor-x = arabul-x
            cursor-y = arabul-y
            start-x = arabul-x
            start-y = arabul-y
            region-color = 480.

     .
