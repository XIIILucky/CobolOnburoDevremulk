*
 flt-grid-reset.
    initialize flt-deg.
    modify flt-grid,
           reset-grid = 1
           mass-update = 1
    initialize flt-grid-data
    move " "                           to gd-2-col-1
    move "Degisken"                    to gd-2-col-2
    move "Baslangic Deger   "          to gd-2-col-3
    move "Bitis Deger   "              to gd-2-col-4
    move "X"                           to gd-2-col-5
    
    modify flt-grid,
           record-to-add(flt-grid-data)

    perform ara-xfd-yukle.
    perform varying xfd-key-i
            from 1
            by 1
            until xfd-key-i > parsexfd-number-fields + 1
                  compute dummy-999 = xfd-key-i - 1
                  call "c$parsexfd" using parsexfd-get-field-info
                                          parsexfd-handle,
                                          dummy-999,
                                          parsexfd-field-description
                  end-call

                  if parsexfd-field-name = spaces
                     exit perform cycle
                  end-if
                    
                  initialize flt-grid-data
                  move parsexfd-field-name   to gd-2-col-2
                  inspect form2-gd-1-record replacing all low-values by spaces
                  if parsexfd-field-user-type = UserDate
                     move "*"                to gd-2-col-1
                  end-if
                  move xfd-key-i             to gd-2-col-6
                  modify flt-grid,
                         record-to-add(flt-grid-data)
    end-perform.

    call "c$parsexfd" using parsexfd-release, parsexfd-handle.


    modify flt-grid,
           mass-update = 0.
*
 combo-dosya-Ex-Ntf-Selchange.
*/    exit paragraph.

    perform flt-grid-reset.
     .
*
 flt2dizi.
    initialize flt-i flt-ii flt-son-i flt-durum.
    inquire flt-grid, 
            last-row in flt-ii
    perform varying flt-i 
            from 2
            by 1
            until flt-i > flt-ii
            inquire flt-grid(flt-i)
                    record-data flt-grid-data
            inquire flt-grid(flt-i,5),
                    hidden-data in eh

            if eh <> "E"
               exit perform cycle
            end-if

            if gd-2-col-3 > gd-2-col-4 
                   display message box
                           gd-2-col-2," alani bitis degeri baslangic degerinden kucuk !!!"
                           icon mb_error_icon
                           title "Hata"
                   move "X"                  to flt-durum
                  exit paragraph
            end-if


            add 1                 to flt-son-i
            if gd-2-col-1 = "*" | Tarih bu alan
*/ Baslangic deger               
               initialize flt-iii
               inspect gd-2-col-3 tallying flt-iii for all "."
               if flt-iii = zeroes
                  inspect gd-2-col-3 tallying flt-iii for all "/"
               end-if

               if flt-iii = zeroes | 01052017 eklinde girilmi
                  move gd-2-col-3(5:4)    to flt-bas(flt-son-i)
                  move gd-2-col-3(3:2)    to flt-bas(flt-son-i)(5:)
                  move gd-2-col-3(1:2)    to flt-bas(flt-son-i)(7:)
               else | 01.01.2017 eklinde girilmi
                  move gd-2-col-3(7:4)    to flt-bas(flt-son-i)
                  move gd-2-col-3(4:2)    to flt-bas(flt-son-i)(5:)
                  move gd-2-col-3(1:2)    to flt-bas(flt-son-i)(7:)
               end-if
*/ Bitis deger               
               initialize flt-iii
               inspect gd-2-col-4 tallying flt-iii for all "."
               if flt-iii = zeroes
                  inspect gd-2-col-4 tallying flt-iii for all "/"
               end-if

               if flt-iii = zeroes | 01052017 eklinde girilmi
                  move gd-2-col-4(5:4)    to flt-bit(flt-son-i)
                  move gd-2-col-4(3:2)    to flt-bit(flt-son-i)(5:)
                  move gd-2-col-4(1:2)    to flt-bit(flt-son-i)(7:)
               else | 01.01.2017 eklinde girilmi
                  move gd-2-col-4(7:4)    to flt-bit(flt-son-i)
                  move gd-2-col-4(4:2)    to flt-bit(flt-son-i)(5:)
                  move gd-2-col-4(1:2)    to flt-bit(flt-son-i)(7:)
               end-if

            else
               move gd-2-col-3       to flt-bas(flt-son-i)
               move gd-2-col-4       to flt-bit(flt-son-i)
            end-if

            move gd-2-col-6          to flt-no(flt-son-i)

    end-perform.


*
 flt-incele.
    initialize flt-durum
    if flt-son-i = zeroes
       exit paragraph.


    compute flt-iii = log-index-record-size + 10

    call "m$alloc" using flt-iii
                         flt-pointer

    call "m$put" using flt-pointer 
                       log-kayit-alani,
                       flt-iii,
                       1



    perform varying flt-i
            from 1
            by 1
            until flt-i > flt-son-i

                  move flt-no(flt-i)            to xfd-key-i
                  perform xfd-get-mem


                  call "C$LCONVERT" using flt-data, 
                                    flt-pointer,
                                    parsexfd-field-offset,
                                    parsexfd-field-length,
                                    parsexfd-field-type, 
                                    parsexfd-field-digits, 
                                    parsexfd-field-scale
                  end-call


                  if flt-bas(flt-i) > flt-data or 
                     flt-bit(flt-i) < flt-data
                         move "X"    to flt-durum
                         exit perform
                  end-if

    end-perform.

    call "m$free" using flt-pointer.
*
 flt-grid-Ev-Other.
     evaluate event-type
     when msg-finish-entry
     when msg-cancel-entry
          inquire flt-grid(event-data-2)
                  record-data in flt-grid-data
          inquire flt-grid(event-data-2,5)
                  hidden-data in eh 

          if gd-2-col-3 = spaces and gd-2-col-4 = spaces
               if eh = "E"
                  modify flt-grid(event-data-2,5)
                         bitmap = null
                         hidden-data = " "
               else
                  modify flt-grid(event-data-2,5)
                         bitmap = yes-bmp
                         bitmap-width = 16
                         bitmap-number = 1
                         bitmap-trailing = 1
                         hidden-data = "E"
               end-if
          else
               if eh = space
                  modify flt-grid(event-data-2,5)
                         bitmap = yes-bmp
                         bitmap-width = 16
                         bitmap-number = 1
                         bitmap-trailing = 1
                         hidden-data = "E"
               end-if
          end-if
     when msg-begin-entry
          evaluate event-data-1
          when 1
          when 2
          when 6
               move event-action-fail         to event-action
          when 3
          when 4
               continue
          when 5
               move event-action-fail         to event-action
               inquire flt-grid(event-data-2,5)
                       hidden-data in eh 
               if eh = "E"
                  modify flt-grid(event-data-2,5)
                         bitmap = null
                         hidden-data = " "
               else
                  modify flt-grid(event-data-2,5)
                         bitmap = yes-bmp
                         bitmap-width = 16
                         bitmap-number = 1
                         bitmap-trailing = 1
                         hidden-data = "E"
               end-if
          end-evaluate
     end-evaluate.
     .

@}}

