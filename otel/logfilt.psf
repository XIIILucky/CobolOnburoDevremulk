                          ara-data-data
                          function length(ara-data-data),
                          1
       call "m$alloc" using function length(kiyas-alan),
                            kiyas-pointer
       call "m$put" using kiyas-pointer 
                          kiyas-alan
                          function length(kiyas-alan),
                          1
       perform varying xfd-key-i
               from 1
               by 1
               until xfd-key-i > (parsexfd-number-fields - 1)
               perform xfd-get-mem
               call "C$LCONVERT" using kiyas-x-80, 
                                       kiyas-pointer,
                                       parsexfd-field-offset,
                                       parsexfd-field-length,
                                       parsexfd-field-type, 
                                       parsexfd-field-digits, 
                                       parsexfd-field-scale
               end-call
               call "C$LCONVERT" using x-80, 
                                       temp-pointer,
                                       parsexfd-field-offset,
                                       parsexfd-field-length,
                                       parsexfd-field-type, 
                                       parsexfd-field-digits, 
                                       parsexfd-field-scale
               end-call
               if x-80 <> kiyas-x-80
                  move 1               to kiyas-degisen-alan(xfd-key-i)
               end-if
       end-perform
       call "m$free" using temp-pointer
       call "m$free" using kiyas-pointer
       move ara-data-data             to kiyas-alan
    else
       if ara-data-islem-turu = "W" 
          move ara-data-data          to kiyas-alan
       end-if
    end-if.
*
 xfd-degisenleri-goster.
    initialize slh
    perform varying kiyas-i
            from 1
            by 1
            until kiyas-i > max-field
            if kiyas-degisen-alan(kiyas-i) = 1
               modify form2-gd-1,
                      y = grid-kayit-no,
                      x = kiyas-i
                      cell-color = 456
               if kiyas-i = 4 or kiyas-i = 5 or kiyas-i = 6| hangi kolon degisti
                  move "E" to slh
               end-if

            end-if
    end-perform.

    if SLH(1:1) = "E"
                  add 1 to kayit-sira

                  modify form2-gd-1,
                        y = grid-kayit-no,
                        x = 9
                        cell-data = kayit-x

                     move 1 to aa
       perform until 1 = 0
               inquire form2-gd-1(grid-kayit-no - aa,9)
                       cell-data in slh
               if slh = spaces 
                  add 1 to aa
                  exit perform cycle
               else
                  if slh(1:5) = "WRITE" 
                          subtract 1 from kayit-sira
                          modify form2-gd-1(grid-kayit-no - aa ,9)
                              cell-data = kayit-x
                          add 1 to kayit-sira
                          exit perform
                  else
                     exit perform
                  end-if
              end-if
       end-perform

    end-if


    initialize kiyas-degisen-alanlar.
*
 convert-to-ddmmyyyyy.

    inspect x-80(1:8) replacing all spaces by zeroes.
    move x-80(1:4)             to eyil
    move x-80(5:2)             to eay
    move x-80(7:2)             to egun
    move etarih                to x-80.
