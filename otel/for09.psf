        when 1
             add rez-buyuk to pax-sayisi
        when 2
             compute pax-sayisi = rez-buyuk + rez-kucuk  / 2
        when 3
             add rez-buyuk rez-kucuk rez-free  to pax-sayisi 
        when 4
