     when 2
        perform yeni-depozit
     when 4
        perform raporla
     when 5
         perform baslik-yukle
         perform grid-yukle
     when 6
          inquire form1-gd-1,cursor-y in sayac
          inquire form1-gd-1(sayac,2),
               cell-data in ydk-konuk-folio
           perform folio-cagir 
           perform baslik-yukle
           perform grid-yukle       
     when 7
           perform bakim-yap
**            perform hata-duzelt-palazzo
     end-evaluate
     
     .
*
 Form1-Gd-1-Ev-Other.
     evaluate event-type
     when msg-begin-entry
         move event-action-fail                 to event-action
     when msg-bitmap-clicked
         move event-action-fail                 to event-action          
         evaluate event-data-1
         when 1
              inquire form1-gd-1(event-data-2,1),
                      hidden-data form1-gd-1-record
             
              if gd-1-col-0 = spaces
                 modify form1-gd-1(event-data-2,1),
                        bitmap = yes-bmp
                        bitmap-width = 16
                        bitmap-number = 1
                        bitmap-trailing = 1

                 modify form1-gd-1(event-data-2,1),
                        hidden-data = 1                       
               else
                 modify form1-gd-1(event-data-2,1),
                        bitmap = no-bmp
                        bitmap-width = 16
                        bitmap-number = 1
                        bitmap-trailing = 1

                 modify form1-gd-1(event-data-2,1),
                        hidden-data = spaces
               end-if                             
         end-evaluate          
     end-evaluate
     
