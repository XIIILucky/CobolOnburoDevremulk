              move "DUN "                 to det-2
              move "% DUN "                to det-3
              move "BUGUN "               to det-4
              move "% BUGUN "              to det-5
              
              move "TARIH"                to det-6
              move "ODA"                  to det-7
              move "% DOL"                to det-8
