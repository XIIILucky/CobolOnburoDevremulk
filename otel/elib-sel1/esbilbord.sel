             SELECT esbilbord ASSIGN TO RANDOM, esbilbord-DOSYA
                    ORGANIZATION INDEXED
                    ACCESS MODE IS DYNAMIC
                    RECORD KEY IS esbilbord-ANAH
                    ALTERNATE RECORD KEY esbilbord-ANAH2 = 
                               esbilbord-TARIH,
                               esbilbord-ODA-TIP,
                                         DUPLICATES
                    STATUS FS-esbilbord.
