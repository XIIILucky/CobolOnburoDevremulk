         perform until fs-takvim = "10"
           read takvim next no lock end 
                   move "10" to fs-takvim
                not end
               
                 if takvim-anah >= rez-cik-tar 
                    move "10" to fs-takvim
                 else
                   initialize bilbord-rec
                   move rez-acenta      to bilbord-acenta 
