{{@PARAGRAPH:
* okundu ve uyari icin eh olayi dusunulecek 
* vazgec-yeni de problem var ustteki grid sifirlanmiyor ...
*
 Form1-Bef-Create.
     perform adresleri-tasi.
     open input kllnc.
     move k-kodu-tasi to k-kodu
     read kllnc no lock invalid
          move "Tanimsiz ..." to k-adi
     end-read
     close kllnc.
     move tarih-tasi     to tarih.
     move 1 to disable-enable-2
     move 0 to disable-enable-1.
*
 Form1-Aft-Initdata.

     modify combo-saat,reset-list = 1
                       mass-update = 1
     modify combo-dakika,reset-list = 1
                       mass-update = 1

     perform varying saat from 0 by 1 until saat > 23
             move saat to x-2
             modify combo-saat
                    item-to-add(x-2)
     end-perform
     
     perform varying dakika from 0 by 1 until dakika > 59
             move dakika to x-2
             modify combo-dakika,
                    item-to-add(x-2)
     end-perform
     move 07 to x-2
     modify combo-saat,mass-update = 0
                       value = x-2
     modify combo-dakika,mass-update = 0
                       value = 00.

     perform grid-yukle.
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         

 combo-yukle.
     open i-o ajanda
     initialize ajanda-rec
     inquire combo-saat,value in saat
     inquire combo-dakika,value in dakika
     move tarih       to ajanda-tarih
     move k-kodu-tasi to ajanda-kullanici
     move saat        to ajanda-saat
     move dakika      to ajanda-dakika
     modify form1-gd-2,mass-update = 1
                       reset-grid = 1
     modify form1-gd-2,mass-update = 0
     start ajanda key not < ajanda-anahtar
           invalid 
                  continue
     not invalid
     initialize fs-ajanda
     perform with test after until fs-ajanda = "10"
     read ajanda next no lock end move "10" to fs-ajanda
     not at end
          if ajanda-tarih = tarih and 
             ajanda-kullanici = k-kodu-tasi and
             ajanda-saat = saat and
             ajanda-dakika = dakika
               modify form1-gd-2(ajanda-sira),
                      record-data = ajanda-aciklama
          else
               move "10" to fs-ajanda
               exit perform
          end-if
     end-read
     end-perform
     end-start
     close ajanda.

 grid-yukle.
     modify form1-gd-1,
            reset-grid = 1
            mass-update = 1.
     initialize form1-gd-1-record grid-kayit-sayi
     move "Zaman"      to gd-1-col-1
     move "Aciklama"   to gd-1-col-2
     modify form1-gd-1,
            record-to-add(form1-gd-1-record)
     open i-o ajanda
     initialize ajanda-rec
     move tarih       to ajanda-tarih
     move k-kodu-tasi to ajanda-kullanici
     start ajanda key not < ajanda-anahtar invalid
           continue
     not invalid
     initialize fs-ajanda zaman durum
     move 1 to sayac
     perform with test after until fs-ajanda = "10"
     read ajanda next no lock end move "10" to fs-ajanda
     not at end
          if ajanda-tarih = tarih and 
             ajanda-kullanici = k-kodu-tasi
* mesaj kaydidir
             if ajanda-saat = 99 and 
                ajanda-dakika = 99
                   exit perform cycle
             end-if
             add 1 to grid-kayit-sayi
             if zaman = zeroes
                move ajanda-zaman to zaman
                move saat         to esaat
                move dakika       to edakika
                move ezaman       to gd-1-col-1
                move ajanda-aciklama to gd-1-col-2
                add 1 to sayac
                modify form1-gd-1,
                       record-to-add(form1-gd-1-record)
                modify form1-gd-1
                       y = sayac
                       x = 1
                       hidden-data = zaman
                       y = sayac
                       x = 3
                       hidden-data = ajanda-okundu-e-h
                       y = sayac
                       x = 4
                       hidden-data = ajanda-uyar-e-h
                if ajanda-okundu-e-h = "E"
                   modify form1-gd-1,
                       y = sayac
                       x = 3
                       bitmap = check-bmp
                       bitmap-number = 2
                       bitmap-width = 16
                else
                   modify form1-gd-1,
                       y = sayac
                       x = 3
                       bitmap = check-bmp
                       bitmap-number = 1
                       bitmap-width = 16
                end-if
                if ajanda-uyar-e-h = "E"
                   modify form1-gd-1,
                       y = sayac
                       x = 4
                       bitmap = yes-bmp
                       bitmap-number = 1
                       bitmap-width = 16
                else
                   modify form1-gd-1,
                       y = sayac
                       x = 4
                       bitmap = null
                end-if

                move zeroes to durum
             else
                if ajanda-zaman <> zaman
                   move ajanda-zaman to zaman
                   move saat         to esaat
                   move dakika       to edakika
                   move ezaman       to gd-1-col-1
                   move ajanda-aciklama to gd-1-col-2
                   add 1 to sayac
                   modify form1-gd-1,
                          record-to-add(form1-gd-1-record)
                   modify form1-gd-1
                          y = sayac
                          x = 1
                          hidden-data = zaman
                          y = sayac
                          x = 3
                          hidden-data = ajanda-okundu-e-h
                          y = sayac
                          x = 4
                          hidden-data = ajanda-uyar-e-h
                   if ajanda-okundu-e-h = "E"
                      modify form1-gd-1,
                             y = sayac
                             x = 3
                             bitmap = check-bmp
                             bitmap-number = 2
                             bitmap-width = 16
                   else
                        modify form1-gd-1,
                             y = sayac
                             x = 3
                             bitmap = check-bmp
                             bitmap-number = 1
                             bitmap-width = 16
                   end-if
                   if ajanda-uyar-e-h = "E"
                      modify form1-gd-1,
                             y = sayac
                             x = 4
                             bitmap = yes-bmp
                             bitmap-number = 1
                             bitmap-width = 16
                   else
                      modify form1-gd-1,
                             y = sayac
                             x = 4
                             bitmap = null
                   end-if
                   move zeroes to durum
                end-if
             end-if
             add 1 to durum
          else
             move "10" to fs-ajanda
             exit perform
          end-if
     end-read
     end-perform
     end-start
     close ajanda
     modify form1-gd-1,
            mass-update = 0.
*
 combo-saat-Ex-Ntf-Selchange.
     perform combo-yukle.
*
 combo-dakika-Ex-Ntf-Selchange.
     perform combo-yukle.
*
 acc-yil-Aft-Procedure.
     open input takvim.
     move tarih     to takvim-anah
     read takvim no lock invalid
          initialize mesaj-link
          move 13 to mesaj-no
          perform mesaj-ver
          close takvim
          move 4 to accept-control
          move 4 to control-id
          exit paragraph
     end-read
     close takvim.
     move 1 to disable-enable-1
     move 0 to disable-enable-2
     display form1.
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc.
*/-----------------------------\*         

     perform grid-yukle.
     modify form1-gd-1,
            y = 2 
            cursor-y = 2
     modify form1-gd-2,
            y = 2 
            cursor-y = 2.
     move msg-goto-cell to event-type.
     perform Form1-Gd-1-Ev-Other.

     move 4 to accept-control
*/kayit yoksa direkt saat combosuna gitsin diye
     if grid-kayit-sayi = 0
        move 15 to control-id
     else
        move 12 to control-id.
*
 Form1-Ex-Other.
     evaluate key-status
          when 002
               initialize frame-degiskenler
               move 999        to frame-kayit-sayi
               move 6                    to frame-win-line
               move 10                   to frame-win-pos
               move "Duzenleniyor "      to frame-title
               perform frame-ac
               inquire combo-saat,
                       value in saat
               inquire combo-dakika,
                       value in dakika
               open i-o  ajanda
               initialize i
               perform varying i from 1 by 1 until i > 999
                       inquire form1-gd-2(i),
                               record-data in form1-gd-2-record
                       initialize ajanda-rec
                       move tarih       to ajanda-tarih
                       move k-kodu-tasi to ajanda-kullanici
                       move saat        to ajanda-saat
                       move dakika      to ajanda-dakika
                       move i           to ajanda-sira
*                       move "E"         to ajanda-uyar-e-h
                       move form1-gd-2-record to ajanda-aciklama
                       if form1-gd-2-record(1:1) = x"ff" or
                          form1-gd-2-record = spaces 
                              continue
                       else
                          write ajanda-rec invalid
                                 rewrite ajanda-rec end-rewrite
                          end-write
                       end-if
                       perform frame-goster
               end-perform
               close ajanda
               perform frame-kapat
               perform grid-yukle
               move 4 to accept-control
               move 18 to control-id
          when 003
               perform sil-soru
               if soru-hayir
                  exit paragraph
               end-if
               initialize frame-degiskenler
               move 999        to frame-kayit-sayi
               move 6                    to frame-win-line
               move 10                   to frame-win-pos
               move "Duzenleniyor "      to frame-title
               perform frame-ac

               inquire combo-saat,
                       value in saat
               inquire combo-dakika,
                       value in dakika
               open i-o  ajanda
               initialize i
               perform varying i from 1 by 1 until i > 999
                       initialize ajanda-rec
                       move tarih       to ajanda-tarih
                       move k-kodu-tasi to ajanda-kullanici
                       move saat        to ajanda-saat
                       move dakika      to ajanda-dakika
                       move i           to ajanda-sira
                       delete ajanda invalid
                              continue
                       end-delete
                       perform frame-goster
               end-perform
               close ajanda
               perform frame-kapat
               modify form1-gd-2,mass-update = 1
                                 reset-grid = 1
*               perform varying i from 1 by 1 until i > 999
*                       initialize form1-gd-2-record
*                       move x"ff" to form1-gd-2-record(1:1)
*                       modify form1-gd-2(i),record-data = form1-gd-2-record
*               end-perform
               modify form1-gd-2,mass-update = 0

               perform grid-yukle
          when 007 
               perform acu-form2-routine
          when 008
               display message box
                       gun,"/",ay,"/",yil
                       " tarihli butun ajanda bilgileri silinecektir !!!",new-line,
                       "Bilgiler silinsin mi ? ",
                       title "Bilginize",
                       type mb_yes_no,
                       default 2
                       returning donus
               if donus = 2 
                  exit paragraph
               end-if
               open i-o ajanda
               initialize ajanda-rec
               move tarih       to ajanda-tarih
               move k-kodu-tasi to ajanda-kullanici
               start ajanda key not < ajanda-anahtar invalid
                     continue
               not invalid
               initialize fs-ajanda
               perform with test after until fs-ajanda = "10"
               read ajanda next no lock end move "10" to fs-ajanda
               not at end
                    if ajanda-tarih = tarih and 
                       ajanda-kullanici = k-kodu-tasi
                           if ajanda-saat = 99 and 
                              ajanda-dakika = 99 
                                 exit perform cycle
                           end-if
                           delete ajanda end-delete
                    else
                       move "10" to fs-ajanda
                       exit perform
                    end-if
               end-read
               end-perform
               end-start
               close ajanda
               perform grid-yukle
               perform combo-yukle
          when 006
               modify form1-gd-2,
                      reset-grid  = 1
                      mass-update = 1
               modify form1-gd-2,
                      mass-update = 0
               move 1 to disable-enable-2
               move 0 to disable-enable-1
               initialize form1-gd-1-record
               display form1
*/-----------------------------\*  
*/---------DIL AYARLA----------\*    
*/-----------------------------\*    
     perform dil-ayarla-proc
*/-----------------------------\*         
               move 4 to accept-control
               move 4 to control-id
          when 005
               display message box
                       gun,"/",ay,"/",yil
                       " tarihli butun ajanda bilgileri okundu olarak imlenecektir !!!",new-line,
                       "Devam etmek icin Evet butonunu seciniz ",
                       title "Bilginize",
                       type mb_yes_no,
                       default 2
                       returning donus
               if donus = 2 
                  exit paragraph
               end-if
               open i-o ajanda
               initialize ajanda-rec
               move tarih       to ajanda-tarih
               move k-kodu-tasi to ajanda-kullanici
               start ajanda key not < ajanda-anahtar invalid
                     continue
               not invalid
               initialize fs-ajanda
               perform with test after until fs-ajanda = "10"
               read ajanda next no lock end move "10" to fs-ajanda
               not at end
                    if ajanda-tarih = tarih and 
                       ajanda-kullanici = k-kodu-tasi
                       if ajanda-saat = 99 and 
                          ajanda-dakika = 99 
                             exit perform cycle
                       end-if
                       move "E" to ajanda-okundu-e-h
                       rewrite ajanda-rec end-rewrite
                    else
                       move "10" to fs-ajanda
                       exit perform
                    end-if
               end-read
               end-perform
               end-start
               close ajanda
               perform grid-yukle
               perform combo-yukle
     end-evaluate
     .
*
 Form1-Gd-1-Ev-Other.
    evaluate event-type
        when msg-begin-entry
             move event-action-fail to event-action
             inquire form1-gd-1,
                     y in a 
                     x in b
             evaluate b
                 when 1
                 when 2
                     inquire form1-gd-1(a,1),
                             hidden-data in zaman
                     move saat to x-2
                     modify combo-saat,value = x-2
                     move dakika to x-2
                     modify combo-dakika,value = x-2
                     perform combo-yukle
                 when 3
                     inquire form1-gd-1(a,3),
                             hidden-data in dummy
                     if dummy = "E"
                        modify form1-gd-1,
                               y = a
                               x = 3
                               bitmap = check-bmp
                               bitmap-number = 1
                               bitmap-width = 16
                               hidden-data = "H"
                        move "H" to dummy
                        set okundu-islem to true
                        perform okundu-yap
                     else
                        modify form1-gd-1,
                               y = a
                               x = 3
                               bitmap = check-bmp
                               bitmap-number = 2
                               bitmap-width = 16
                               hidden-data = "E"
                        move "E" to dummy
                        set okundu-islem to true
                        perform okundu-yap
                     end-if

                 when 4
                     inquire form1-gd-1(a,4),
                             hidden-data in dummy
                     if dummy = "E"
                        modify form1-gd-1,
                               y = a
                               x = 4
                               bitmap = null
                               hidden-data = "H"
                        move "H" to dummy
                        set uyari-islem to true
                        perform okundu-yap
                     else
                        modify form1-gd-1,
                               y = a
                               x = 4
                               bitmap = yes-bmp
                               bitmap-number = 1
                               bitmap-width = 16
                               hidden-data = "E"
                        move "E" to dummy
                        set uyari-islem to true
                        perform okundu-yap
                     end-if
             end-evaluate
             perform parlat-parlatma
             move event-action-fail to event-action
             exit paragraph
        when msg-goto-cell
        when msg-goto-cell-mouse
        when msg-begin-drag
        when msg-end-drag
        when msg-goto-cell-drag
             perform parlat-parlatma

    end-evaluate.
 parlat-parlatma.
    inquire form1-gd-1,y in yeni-y
                       x in a
    if yeni-y < 2
       move 2 to yeni-y eski-y
    end-if
    if eski-y > zeroes 
       modify form1-gd-1(eski-y),x = 1
              row-color = 0
              cell-color = 0
    end-if
    modify form1-gd-1,
           y = yeni-y,
           x = 1,
           row-color = 481
    move yeni-y to eski-y.
 okundu-yap.
     display floating window 
             lines 6 size 25 
             handle bekle-handle
             title-bar
             title "Bilgiler yaziliyor ..."
             background-low
     display label "Lutfen Bekleyiniz ..." line 3 col 7 color 2
             font small-font transparent

    inquire form1-gd-1(a,1),
            hidden-data in zaman
    open i-o ajanda
    initialize ajanda-rec
    move tarih        to ajanda-tarih
    move k-kodu-tasi  to ajanda-kullanici
    move saat         to ajanda-saat
    move dakika       to ajanda-dakika
    start ajanda key not < ajanda-anahtar
          invalid 
                 continue
    not invalid
    initialize fs-ajanda
    perform with test after until fs-ajanda = "10"
    read ajanda next no lock end move "10" to fs-ajanda
    not at end
         if ajanda-tarih = tarih and
            ajanda-kullanici = k-kodu-tasi and
            ajanda-saat = saat and
            ajanda-dakika = dakika
               if okundu-islem
                  move dummy     to ajanda-okundu-e-h
               else
                  move dummy     to ajanda-uyar-e-h
               end-if
               rewrite ajanda-rec end-rewrite
         else
            move "10" to fs-ajanda
            exit perform
         end-if
    end-read
    end-perform
    end-start
    close ajanda.
    close window bekle-handle.
*
 kllnc2takas.
     open i-o genelfis 
     initialize genelfis-rec 
     move 1   to genelfis-anahtar
     read genelfis no lock invalid 
         continue 
     not invalid 
           add  1 to ekran-fis-1
           move ekran-fis-1  to lucky-no
           move k-kodu-tasi  to lucky-kllnc-kodu
     end-read 
     close genelfis
     open output lucky close lucky open i-o lucky 
    open i-o kllnc 
    initialize kllnc-rec lucky-rec
    start kllnc key not < k-kodu invalid
          continue
    not invalid
    initialize fs-kllnc
    perform with test after until fs-kllnc = "10"
    read kllnc next no lock end move "10" to fs-kllnc
    not at end
        if k-kullanimda = "H" 
           exit perform cycle
        end-if
        if k-kodu-tasi = k-kodu
           exit perform cycle
        end-if
        string k-adi    delimited by space
               " "      delimited by size,
               k-soyadi delimited by size into lucky-k-adi
        move k-kodu to lucky-k-kodu
        write lucky-rec invalid
              rewrite lucky-rec end-rewrite
        end-write
    end-read
    end-perform
    end-start
    close kllnc lucky
    .
*
 form2-Aft-Initdata.
    modify form2-gd-1,
           reset-grid = 1
           mass-update = 1.
    initialize form2-gd-1-record
    move "X"          to gd-3-col-1
    move "Kullanici"  to gd-3-col-2
    modify form2-gd-1,
           record-to-add(form2-gd-1-record)
    perform kllnc2takas
    open input lucky
    initialize lucky-rec
    start lucky key not < lucky-anah invalid
          continue
    not invalid
*    initialize fs-kllnc
    move 1 to chat-i
    perform with test after until fs-lucky = "10"
    read lucky next no lock end move "10" to fs-lucky
    not at end
*        if k-kullanimda = "H" 
*           exit perform cycle
*        end-if
*        if k-kodu-tasi = k-kodu
*           exit perform cycle
*        end-if
        add 1 to chat-i
        initialize form2-gd-1-record
*        string k-adi delimited by space
*               " " delimited by size,
*               k-soyadi delimited by size into gd-3-col-2
        move lucky-k-adi to gd-3-col-2
        modify form2-gd-1,
               record-to-add(Form2-gd-1-record)
        modify form2-gd-1(chat-i),
               x = 1
               hidden-data = "H"
               bitmap = check-bmp
               bitmap-number = 1
               bitmap-width = 16
               x = 2
               hidden-data = lucky-k-kodu
        move chat-i     to chat-son-i
    end-read
    end-perform
    end-start
    close lucky
    modify form2-gd-1,
           mass-update = 0.
    move 1 to form2-Pg-1-Visible 
    move 0 to form2-Pg-2-Visible 
    display form2.
