              open input depkod  konuk  cast
              initialize fs-maincurt toplam
              perform with test after until fs-maincurt = "10"
                    read maincurt next no lock end move "10" to fs-maincurt
                    not at end
                        initialize dokumer-rec detay
*                        if konuk-durumu not = "I" exit perform
*                        end-if
                        if maincurt-folio = 1005030 and isyeri-adres-tasi = "smelia17"
                           exit perform cycle 
                        end-if 
                        if rapor-folio = "H" or
                           rapor-folio = maincurt-fol-kodu 
                           if rapor-acenta = spaces or
                              rapor-acenta = maincurt-acenta

                              move maincurt-folio to konuk-folio
                              read konuk no lock invalid 
                                 display message box konuk-folio " nolu folio bulunamadi" 
                              end-read

                              if konuk-fol-kodu not = "R" or konuk-git-tar <= filtre-tarih or  konuk-gel-tar >  filtre-tarih
                                 initialize maincurt-buyuk  maincurt-kucuk maincurt-free   maincurt-bebek
                                 else
                                 move konuk-rez-no to cast-rez-no
                                 move filtre-tarih  to cast-tarih
                                 read cast no lock invalid continue
                                    not invalid 
                                    move cast-kisi to maincurt-kisi
                                    move cast-oda-no to maincurt-ODANO
                                end-read
                              end-if
                             if  rapor-firma = spaces or
                              rapor-firma = konuk-firma
                               if tumu > 1 then
          
                                  move "B" to kodlar02-tipi
                                  move maincurt-odeme-tipi to kodlar02-kodu
                                  read kodlar02 no lock invalid 
                                      move spaces to kodlar02-adi
                                  end-read
                                  if ( ode-tipi = "D" or  ode-tipi = "F" or  ode-tipi = "O")   and tumu = 2 then
                                     exit perform cycle
                                  end-if
                                  if ( ode-tipi not = "D" and ode-tipi not = "F" and ode-tipi not = "O") and tumu = 3 then
                                    exit perform cycle
                                  end-if
                              end-if
                          
                              if konuk-grup-no not =  rapor-grup and
                                 rapor-grup not = zeroes 
                                    exit perform cycle 
                              end-if 
                                perform detay-ata
                                if tut-knt = 0 or kalan > tut-knt
                                  write dokumer-rec from detay
