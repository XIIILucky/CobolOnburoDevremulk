       move sonraki-tar to hrkgir-tarih
       if i > 4 then 
          compute hrkgir-ref = i - 4
          else 
           if debug = 1 then stop " " end-if
       end-if
        move i to zz 
         string   "Yilbasi Yansitmasi " delimited by size
                  zz delimited by size
                 "nolu pencereden" delimited by size  
         into yazilacak-not
         perform yazortak
     end-if.
