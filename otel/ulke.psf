 Form2-Gd-1-Ev-Other.
    evaluate event-type
    when msg-begin-entry
       move event-action-fail                 to event-action
       
          inquire form2-gd-1(event-data-2,1),hidden-data secim
          if secim = "H"
           modify form2-gd-1(event-data-2,1),
                  bitmap = check-bmp
                  bitmap-width = 16
                  bitmap-number = 2
                  bitmap-trailing = 1 
            modify form2-gd-1(event-data-2,1),hidden-data "E" 
          else 
           modify form2-gd-1(event-data-2,1),
                  bitmap = check-bmp
                  bitmap-width = 16
                  bitmap-number = 1
                  bitmap-trailing = 1             
            modify form2-gd-1(event-data-2,1),hidden-data "H" 
          end-if
       
    when msg-heading-dblclick
       evaluate event-data-1
       when 1
        inquire form2-gd-1,last-row in son-satir
        initialize i
        perform varying i
                from 2
                by 1
                until i > son-satir
                  inquire form2-gd-1(i,1),hidden-data secim
                  if secim = "H"
                   modify form2-gd-1(i,1),
                          bitmap = check-bmp
                          bitmap-width = 16
                          bitmap-number = 2
                          bitmap-trailing = 1 
                    modify form2-gd-1(i,1),hidden-data "E" 
                  else 
                   modify form2-gd-1(i,1),
                          bitmap = check-bmp
                          bitmap-width = 16
                          bitmap-number = 1
                          bitmap-trailing = 1             
                    modify form2-gd-1(i,1),hidden-data "H" 
                  end-if
        end-perform          
       end-evaluate 
    end-evaluate.     
